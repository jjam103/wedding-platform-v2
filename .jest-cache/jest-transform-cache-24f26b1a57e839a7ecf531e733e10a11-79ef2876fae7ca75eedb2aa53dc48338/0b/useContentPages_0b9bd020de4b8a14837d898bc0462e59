      /* istanbul ignore next */
      cov_21fwfn6bmg().s[43]++;
      console.error('Failed to update content page:', err);
      /* istanbul ignore next */
      cov_21fwfn6bmg().s[44]++;
      return {
        success: false,
        error: errorMsg
      };
    }
  }, []));
  const remove =
  /* istanbul ignore next */
  (cov_21fwfn6bmg().s[45]++, (0, _react.useCallback)(async id => {
    /* istanbul ignore next */
    cov_21fwfn6bmg().f[8]++;
    cov_21fwfn6bmg().s[46]++;
    try {
      const response =
      /* istanbul ignore next */
      (cov_21fwfn6bmg().s[47]++, await fetch(`/api/admin/content-pages/${id}`, {
        method: 'DELETE'
      }));
      const result =
      /* istanbul ignore next */
      (cov_21fwfn6bmg().s[48]++, await response.json());
      /* istanbul ignore next */
      cov_21fwfn6bmg().s[49]++;
      if (
      /* istanbul ignore next */
      (cov_21fwfn6bmg().b[15][0]++, !response.ok) ||
      /* istanbul ignore next */
      (cov_21fwfn6bmg().b[15][1]++, !result.success)) {
        /* istanbul ignore next */
        cov_21fwfn6bmg().b[14][0]++;
        cov_21fwfn6bmg().s[50]++;
        return {
          success: false,
          error:
          /* istanbul ignore next */
          (cov_21fwfn6bmg().b[16][0]++, result.error?.message) ||
          /* istanbul ignore next */
          (cov_21fwfn6bmg().b[16][1]++, 'Failed to delete content page')
        };
      } else
      /* istanbul ignore next */
      {
        cov_21fwfn6bmg().b[14][1]++;
      }
      // Optimistic update
      cov_21fwfn6bmg().s[51]++;
      setData(prev => {
        /* istanbul ignore next */
        cov_21fwfn6bmg().f[9]++;
        cov_21fwfn6bmg().s[52]++;
        return prev.filter(page => {
          /* istanbul ignore next */
          cov_21fwfn6bmg().f[10]++;
          cov_21fwfn6bmg().s[53]++;
          return page.id !== id;
        });
      });
      /* istanbul ignore next */
      cov_21fwfn6bmg().s[54]++;
      return {
        success: true
      };
    } catch (err) {
      const errorMsg =
      /* istanbul ignore next */
      (cov_21fwfn6bmg().s[55]++, err instanceof Error ?
      /* istanbul ignore next */
      (cov_21fwfn6bmg().b[17][0]++, err.message) :
      /* istanbul ignore next */
      (cov_21fwfn6bmg().b[17][1]++, 'Unknown error'));
      /* istanbul ignore next */
      cov_21fwfn6bmg().s[56]++;
      console.error('Failed to delete content page:', err);
      /* istanbul ignore next */
      cov_21fwfn6bmg().s[57]++;
      return {
        success: false,
        error: errorMsg
      };
    }
  }, []));
  /* istanbul ignore next */
  cov_21fwfn6bmg().s[58]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_21fwfn6bmg().f[11]++;
    cov_21fwfn6bmg().s[59]++;
    refetch();
  }, [refetch]);
  /* istanbul ignore next */
  cov_21fwfn6bmg().s[60]++;
  return {
    data,
    loading,
    error,
    refetch,
    create,
    update,
    remove
  };
}
