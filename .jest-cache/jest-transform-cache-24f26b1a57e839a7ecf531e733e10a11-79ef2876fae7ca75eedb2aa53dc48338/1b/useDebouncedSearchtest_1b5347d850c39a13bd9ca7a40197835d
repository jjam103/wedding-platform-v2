/**
 * Tests for debounced search hooks
 * 
 * Validates:
 * - 300ms debounce delay
 * - Loading indicator during search
 * - Cleanup on unmount
 */ "use strict";
Object.defineProperty(exports, "__esModule", {
    value: true
});
const _react = require("@testing-library/react");
const _useDebouncedSearch = require("./useDebouncedSearch");
// Mock timers
jest.useFakeTimers();
describe('useDebouncedSearch', ()=>{
    afterEach(()=>{
        jest.clearAllTimers();
    });
    it('should initialize with initial value', ()=>{
        const { result } = (0, _react.renderHook)(()=>(0, _useDebouncedSearch.useDebouncedSearch)('initial'));
        expect(result.current.value).toBe('initial');
        expect(result.current.debouncedValue).toBe('initial');
        expect(result.current.isSearching).toBe(false);
    });
    it('should debounce value changes by 300ms', ()=>{
        const { result } = (0, _react.renderHook)(()=>(0, _useDebouncedSearch.useDebouncedSearch)(''));
        // Change value
        (0, _react.act)(()=>{
            result.current.setValue('test');
        });
        // Value should update immediately
        expect(result.current.value).toBe('test');
        expect(result.current.debouncedValue).toBe('');
        expect(result.current.isSearching).toBe(true);
        // Fast-forward 299ms (not enough)
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(299);
        });
        expect(result.current.debouncedValue).toBe('');
        expect(result.current.isSearching).toBe(true);
        // Fast-forward 1ms more (total 300ms)
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(1);
        });
        expect(result.current.debouncedValue).toBe('test');
        expect(result.current.isSearching).toBe(false);
    });
    it('should show loading indicator during debounce', ()=>{
        const { result } = (0, _react.renderHook)(()=>(0, _useDebouncedSearch.useDebouncedSearch)(''));
        (0, _react.act)(()=>{
            result.current.setValue('searching');
        });
        expect(result.current.isSearching).toBe(true);
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(300);
        });
        expect(result.current.isSearching).toBe(false);
    });
    it('should cancel previous timeout on rapid changes', ()=>{
        const { result } = (0, _react.renderHook)(()=>(0, _useDebouncedSearch.useDebouncedSearch)(''));
        // First change
        (0, _react.act)(()=>{
            result.current.setValue('a');
        });
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(100);
        });
        // Second change before first completes
        (0, _react.act)(()=>{
            result.current.setValue('ab');
        });
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(100);
        });
        // Third change
        (0, _react.act)(()=>{
            result.current.setValue('abc');
        });
        // Only the last value should be debounced
        expect(result.current.debouncedValue).toBe('');
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(300);
        });
        expect(result.current.debouncedValue).toBe('abc');
    });
    it('should reset value and debounced value', ()=>{
        const { result } = (0, _react.renderHook)(()=>(0, _useDebouncedSearch.useDebouncedSearch)('initial'));
        (0, _react.act)(()=>{
            result.current.setValue('changed');
        });
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(300);
        });
        expect(result.current.value).toBe('changed');
        expect(result.current.debouncedValue).toBe('changed');
        (0, _react.act)(()=>{
            result.current.reset();
        });
        expect(result.current.value).toBe('');
        expect(result.current.debouncedValue).toBe('');
        expect(result.current.isSearching).toBe(false);
    });
    it('should cleanup timeout on unmount', ()=>{
        const { result, unmount } = (0, _react.renderHook)(()=>(0, _useDebouncedSearch.useDebouncedSearch)(''));
        (0, _react.act)(()=>{
            result.current.setValue('test');
        });
        expect(result.current.isSearching).toBe(true);
        unmount();
        // Should not throw error
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(300);
        });
    });
    it('should support custom delay', ()=>{
        const { result } = (0, _react.renderHook)(()=>(0, _useDebouncedSearch.useDebouncedSearch)('', 500));
        (0, _react.act)(()=>{
            result.current.setValue('test');
        });
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(300);
        });
        expect(result.current.debouncedValue).toBe('');
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(200);
        });
        expect(result.current.debouncedValue).toBe('test');
    });
});
describe('useDebounce', ()=>{
    afterEach(()=>{
        jest.clearAllTimers();
    });
    it('should debounce value changes', ()=>{
        const { result, rerender } = (0, _react.renderHook)(({ value })=>(0, _useDebouncedSearch.useDebounce)(value, 300), {
            initialProps: {
                value: 'initial'
            }
        });
        expect(result.current).toBe('initial');
        rerender({
            value: 'changed'
        });
        expect(result.current).toBe('initial');
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(300);
        });
        expect(result.current).toBe('changed');
    });
    it('should work with non-string values', ()=>{
        const { result, rerender } = (0, _react.renderHook)(({ value })=>(0, _useDebouncedSearch.useDebounce)(value, 300), {
            initialProps: {
                value: 0
            }
        });
        expect(result.current).toBe(0);
        rerender({
            value: 42
        });
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(300);
        });
        expect(result.current).toBe(42);
    });
    it('should work with objects', ()=>{
        const obj1 = {
            id: 1,
            name: 'Test'
        };
        const obj2 = {
            id: 2,
            name: 'Updated'
        };
        const { result, rerender } = (0, _react.renderHook)(({ value })=>(0, _useDebouncedSearch.useDebounce)(value, 300), {
            initialProps: {
                value: obj1
            }
        });
        expect(result.current).toBe(obj1);
        rerender({
            value: obj2
        });
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(300);
        });
        expect(result.current).toBe(obj2);
    });
});
describe('useDebouncedCallback', ()=>{
    afterEach(()=>{
        jest.clearAllTimers();
    });
    it('should debounce callback execution', ()=>{
        const callback = jest.fn();
        const { result } = (0, _react.renderHook)(()=>(0, _useDebouncedSearch.useDebouncedCallback)(callback, 300));
        (0, _react.act)(()=>{
            result.current('test');
        });
        expect(callback).not.toHaveBeenCalled();
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(300);
        });
        expect(callback).toHaveBeenCalledWith('test');
        expect(callback).toHaveBeenCalledTimes(1);
    });
    it('should cancel previous callback on rapid calls', ()=>{
        const callback = jest.fn();
        const { result } = (0, _react.renderHook)(()=>(0, _useDebouncedSearch.useDebouncedCallback)(callback, 300));
        (0, _react.act)(()=>{
            result.current('first');
        });
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(100);
        });
        (0, _react.act)(()=>{
            result.current('second');
        });
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(100);
        });
        (0, _react.act)(()=>{
            result.current('third');
        });
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(300);
        });
        // Only the last call should execute
        expect(callback).toHaveBeenCalledWith('third');
        expect(callback).toHaveBeenCalledTimes(1);
    });
    it('should handle multiple arguments', ()=>{
        const callback = jest.fn();
        const { result } = (0, _react.renderHook)(()=>(0, _useDebouncedSearch.useDebouncedCallback)(callback, 300));
        (0, _react.act)(()=>{
            result.current('arg1', 'arg2', 'arg3');
        });
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(300);
        });
        expect(callback).toHaveBeenCalledWith('arg1', 'arg2', 'arg3');
    });
    it('should update callback reference', ()=>{
        const callback1 = jest.fn();
        const callback2 = jest.fn();
        const { result, rerender } = (0, _react.renderHook)(({ cb })=>(0, _useDebouncedSearch.useDebouncedCallback)(cb, 300), {
            initialProps: {
                cb: callback1
            }
        });
        (0, _react.act)(()=>{
            result.current('test');
        });
        // Update callback before timeout
        rerender({
            cb: callback2
        });
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(300);
        });
        // Should call the updated callback
        expect(callback1).not.toHaveBeenCalled();
        expect(callback2).toHaveBeenCalledWith('test');
    });
    it('should cleanup timeout on unmount', ()=>{
        const callback = jest.fn();
        const { result, unmount } = (0, _react.renderHook)(()=>(0, _useDebouncedSearch.useDebouncedCallback)(callback, 300));
        (0, _react.act)(()=>{
            result.current('test');
        });
        unmount();
        (0, _react.act)(()=>{
            jest.advanceTimersByTime(300);
        });
        // Callback should not be called after unmount
        expect(callback).not.toHaveBeenCalled();
    });
});

