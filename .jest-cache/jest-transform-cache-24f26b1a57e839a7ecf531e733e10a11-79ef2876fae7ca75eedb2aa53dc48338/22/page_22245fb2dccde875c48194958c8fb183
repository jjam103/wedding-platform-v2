    /* istanbul ignore next */
    (cov_ak9r7dk7q().b[35][0]++, isFormOpen) &&
    /* istanbul ignore next */
    (cov_ak9r7dk7q().b[35][1]++, selectedAccommodation) &&
    /* istanbul ignore next */
    (cov_ak9r7dk7q().b[35][2]++, /*#__PURE__*/(0, _jsxruntime.jsx)(_InlineSectionEditor.InlineSectionEditor, {
