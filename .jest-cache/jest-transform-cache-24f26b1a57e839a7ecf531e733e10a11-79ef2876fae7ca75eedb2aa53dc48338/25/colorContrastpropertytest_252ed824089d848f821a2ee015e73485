    it('should meet WCAG 2.1 AA contrast for all common color combinations', ()=>{
        // Test all predefined common color combinations
        _colorContrast.commonColorCombinations.forEach((combo)=>{
            const ratio = (0, _colorContrast.getContrastRatio)(combo.foreground, combo.background);
            // Determine if this is large text (buttons, headings) or normal text
            const isLargeText = combo.usage.includes('button') || combo.usage.includes('badge');
            const level = isLargeText ? 'large' : 'normal';
            expect((0, _colorContrast.meetsWCAG_AA)(ratio, level)).toBe(true);
            // Log for debugging
            if (!(0, _colorContrast.meetsWCAG_AA)(ratio, level)) {
                console.error(`Failed contrast check for ${combo.usage}: ${ratio.toFixed(2)}:1 (${level} text)`);
            }
        });
    });
