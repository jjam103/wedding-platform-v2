        // Convert datetime-local format (YYYY-MM-DDTHH:mm) to ISO 8601 (YYYY-MM-DDTHH:mm:ss.sssZ)
        const dateValue =
        /* istanbul ignore next */
        (cov_1s1ybn75au().s[47]++, processedData[field.name]);
        /* istanbul ignore next */
        cov_1s1ybn75au().s[48]++;
        if (
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[14][0]++, typeof dateValue === 'string') &&
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[14][1]++, dateValue.length > 0)) {
          /* istanbul ignore next */
          cov_1s1ybn75au().b[13][0]++;
          cov_1s1ybn75au().s[49]++;
          try {
            /* istanbul ignore next */
            cov_1s1ybn75au().s[50]++;
            processedData[field.name] = new Date(dateValue).toISOString();
          } catch (error) {
            /* istanbul ignore next */
            cov_1s1ybn75au().s[51]++;
            console.error(`Failed to convert datetime field ${field.name}:`, error);
          }
        } else
        /* istanbul ignore next */
        {
          cov_1s1ybn75au().b[13][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_1s1ybn75au().b[11][1]++;
      }
    });
    // Validate with Zod
    const validation =
    /* istanbul ignore next */
    (cov_1s1ybn75au().s[52]++, schema.safeParse(processedData));
    /* istanbul ignore next */
    cov_1s1ybn75au().s[53]++;
    if (!validation.success) {
      /* istanbul ignore next */
      cov_1s1ybn75au().b[15][0]++;
      // Extract field-level errors
      const fieldErrors =
      /* istanbul ignore next */
      (cov_1s1ybn75au().s[54]++, {});
      /* istanbul ignore next */
      cov_1s1ybn75au().s[55]++;
      validation.error.issues.forEach(issue => {
        /* istanbul ignore next */
        cov_1s1ybn75au().f[15]++;
        const field =
        /* istanbul ignore next */
        (cov_1s1ybn75au().s[56]++, issue.path[0]);
        /* istanbul ignore next */
        cov_1s1ybn75au().s[57]++;
        if (field) {
          /* istanbul ignore next */
          cov_1s1ybn75au().b[16][0]++;
          cov_1s1ybn75au().s[58]++;
          fieldErrors[field] = issue.message;
        } else
        /* istanbul ignore next */
        {
          cov_1s1ybn75au().b[16][1]++;
        }
      });
      /* istanbul ignore next */
      cov_1s1ybn75au().s[59]++;
      setErrors(fieldErrors);
      /* istanbul ignore next */
      cov_1s1ybn75au().s[60]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_1s1ybn75au().b[15][1]++;
    }
    cov_1s1ybn75au().s[61]++;
    setIsSubmitting(true);
    /* istanbul ignore next */
    cov_1s1ybn75au().s[62]++;
    try {
      /* istanbul ignore next */
      cov_1s1ybn75au().s[63]++;
      await onSubmit(validation.data);
      // Clear form on success
      /* istanbul ignore next */
      cov_1s1ybn75au().s[64]++;
      setFormData({});
      /* istanbul ignore next */
      cov_1s1ybn75au().s[65]++;
      setErrors({});
      /* istanbul ignore next */
      cov_1s1ybn75au().s[66]++;
      setIsDirty(false);
      /* istanbul ignore next */
      cov_1s1ybn75au().s[67]++;
      onToggle(); // Collapse form
    } catch (error) {
      /* istanbul ignore next */
      cov_1s1ybn75au().s[68]++;
      console.error('Form submission error:', error);
      /* istanbul ignore next */
      cov_1s1ybn75au().s[69]++;
      setErrors({
        _form: error instanceof Error ?
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[17][0]++, error.message) :
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[17][1]++, 'Submission failed')
      });
    } finally {
      /* istanbul ignore next */
      cov_1s1ybn75au().s[70]++;
      setIsSubmitting(false);
    }
  };
  /* istanbul ignore next */
  cov_1s1ybn75au().s[71]++;
  const handleCancel = () => {
    /* istanbul ignore next */
    cov_1s1ybn75au().f[16]++;
    cov_1s1ybn75au().s[72]++;
    if (isDirty) {
      /* istanbul ignore next */
      cov_1s1ybn75au().b[18][0]++;
      const confirmed =
      /* istanbul ignore next */
      (cov_1s1ybn75au().s[73]++, window.confirm('You have unsaved changes. Are you sure you want to cancel?'));
      /* istanbul ignore next */
      cov_1s1ybn75au().s[74]++;
      if (!confirmed) {
        /* istanbul ignore next */
        cov_1s1ybn75au().b[19][0]++;
        cov_1s1ybn75au().s[75]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_1s1ybn75au().b[19][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_1s1ybn75au().b[18][1]++;
    }
    cov_1s1ybn75au().s[76]++;
    setFormData(
    /* istanbul ignore next */
    (cov_1s1ybn75au().b[20][0]++, initialData) ||
    /* istanbul ignore next */
    (cov_1s1ybn75au().b[20][1]++, {}));
    /* istanbul ignore next */
    cov_1s1ybn75au().s[77]++;
    setErrors({});
    /* istanbul ignore next */
    cov_1s1ybn75au().s[78]++;
    setIsDirty(false);
    /* istanbul ignore next */
    cov_1s1ybn75au().s[79]++;
    onToggle();
    /* istanbul ignore next */
    cov_1s1ybn75au().s[80]++;
    if (onCancel) {
      /* istanbul ignore next */
      cov_1s1ybn75au().b[21][0]++;
      cov_1s1ybn75au().s[81]++;
      onCancel();
    } else
    /* istanbul ignore next */
    {
      cov_1s1ybn75au().b[21][1]++;
    }
  };
  /* istanbul ignore next */
  cov_1s1ybn75au().s[82]++;
  const renderField = field => {
    /* istanbul ignore next */
    cov_1s1ybn75au().f[17]++;
    const value =
    /* istanbul ignore next */
    (cov_1s1ybn75au().s[83]++,
    /* istanbul ignore next */
    (cov_1s1ybn75au().b[22][0]++, formData[field.name]) ||
    /* istanbul ignore next */
    (cov_1s1ybn75au().b[22][1]++, ''));
    const error =
    /* istanbul ignore next */
    (cov_1s1ybn75au().s[84]++, errors[field.name]);
    const fieldId =
    /* istanbul ignore next */
    (cov_1s1ybn75au().s[85]++, `field-${field.name}`);
    const baseInputClasses =
    /* istanbul ignore next */
    (cov_1s1ybn75au().s[86]++, `w-full px-3 py-2 border rounded-lg transition-colors focus:outline-none focus:ring-2 focus:ring-green-500 ${error ?
    /* istanbul ignore next */
    (cov_1s1ybn75au().b[23][0]++, 'border-red-500') :
    /* istanbul ignore next */
    (cov_1s1ybn75au().b[23][1]++, 'border-gray-300')}`);
    /* istanbul ignore next */
    cov_1s1ybn75au().s[87]++;
    return /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
      className: "space-y-1",
      children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("label", {
        htmlFor: fieldId,
        className: "block text-sm font-medium text-gray-700",
        children: [field.label,
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[24][0]++, field.required) &&
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[24][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
          className: "text-red-500 ml-1",
          children: "*"
        }))]
      }), field.type === 'textarea' ?
      /* istanbul ignore next */
      (/*#__PURE__*/cov_1s1ybn75au().b[25][0]++, (0, _jsxruntime.jsx)("textarea", {
        id: fieldId,
        name: field.name,
        value: value,
        onChange: e => {
          /* istanbul ignore next */
          cov_1s1ybn75au().f[18]++;
          cov_1s1ybn75au().s[88]++;
          return handleFieldChange(field.name, e.target.value);
        },
        placeholder: field.placeholder,
        required: field.required,
        rows:
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[26][0]++, field.rows) ||
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[26][1]++, 4),
        className: baseInputClasses,
        "aria-invalid": !!error,
        "aria-describedby": error ?
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[27][0]++, `${fieldId}-error`) :
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[27][1]++, undefined)
      })) :
      /* istanbul ignore next */
      (cov_1s1ybn75au().b[25][1]++, field.type === 'select' ?
      /* istanbul ignore next */
      (/*#__PURE__*/cov_1s1ybn75au().b[28][0]++, (0, _jsxruntime.jsxs)("select", {
        id: fieldId,
        name: field.name,
        value: value,
        onChange: e => {
          /* istanbul ignore next */
          cov_1s1ybn75au().f[19]++;
          cov_1s1ybn75au().s[89]++;
          return handleFieldChange(field.name, e.target.value);
        },
        required: field.required,
        className: baseInputClasses,
        "aria-invalid": !!error,
        "aria-describedby": error ?
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[29][0]++, `${fieldId}-error`) :
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[29][1]++, undefined),
        children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("option", {
          value: "",
          children: ["Select ", field.label]
        }), field.options?.map(option => {
          /* istanbul ignore next */
          cov_1s1ybn75au().f[20]++;
          cov_1s1ybn75au().s[90]++;
          return /*#__PURE__*/(0, _jsxruntime.jsx)("option", {
            value: option.value,
            children: option.label
          }, option.value);
        })]
      })) :
      /* istanbul ignore next */
      (/*#__PURE__*/cov_1s1ybn75au().b[28][1]++, (0, _jsxruntime.jsx)("input", {
        id: fieldId,
        type: field.type,
        name: field.name,
        value: value,
        onChange: e => {
          /* istanbul ignore next */
          cov_1s1ybn75au().f[21]++;
          // Convert number inputs to actual numbers
          const newValue =
          /* istanbul ignore next */
          (cov_1s1ybn75au().s[91]++, field.type === 'number' ?
          /* istanbul ignore next */
          (cov_1s1ybn75au().b[30][0]++, e.target.value === '' ?
          /* istanbul ignore next */
          (cov_1s1ybn75au().b[31][0]++, '') :
          /* istanbul ignore next */
          (cov_1s1ybn75au().b[31][1]++, Number(e.target.value))) :
          /* istanbul ignore next */
          (cov_1s1ybn75au().b[30][1]++, e.target.value));
          /* istanbul ignore next */
          cov_1s1ybn75au().s[92]++;
          handleFieldChange(field.name, newValue);
        },
        placeholder: field.placeholder,
        required: field.required,
        className: baseInputClasses,
        "aria-invalid": !!error,
        "aria-describedby": error ?
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[32][0]++, `${fieldId}-error`) :
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[32][1]++, undefined)
      }))),
      /* istanbul ignore next */
      (cov_1s1ybn75au().b[33][0]++, error) &&
      /* istanbul ignore next */
      (cov_1s1ybn75au().b[33][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
        id: `${fieldId}-error`,
        className: "text-sm text-red-600",
        role: "alert",
        children: error
      }))]
    }, field.name);
  };
  /* istanbul ignore next */
  cov_1s1ybn75au().s[93]++;
  return /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
    ref: formRef,
    className: "border border-sage-200 rounded-lg overflow-hidden bg-white shadow-sm",
    children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("button", {
      onClick: onToggle,
      className: "w-full flex items-center justify-between px-4 py-3 bg-sage-50 hover:bg-sage-100 transition-colors",
      "aria-expanded": isOpen,
      "aria-controls": "collapsible-form-content",
      children: [/*#__PURE__*/(0, _jsxruntime.jsx)("span", {
        className: "font-medium text-sage-900",
        children: title
      }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
        className: `text-sage-600 transition-transform duration-300 ${isOpen ?
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[34][0]++, 'rotate-180') :
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[34][1]++, '')}`,
        "aria-hidden": "true",
        children: ""
      })]
    }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      ref: contentRef,
      id: "collapsible-form-content",
      className: `transition-all duration-300 ease-in-out ${isOpen ?
      /* istanbul ignore next */
      (cov_1s1ybn75au().b[35][0]++, '') :
      /* istanbul ignore next */
      (cov_1s1ybn75au().b[35][1]++, 'overflow-hidden')}`,
      style: {
        maxHeight: isOpen ?
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[36][0]++, 'none') :
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[36][1]++, '0px'),
        opacity: isOpen ?
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[37][0]++, 1) :
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[37][1]++, 0)
      },
      "aria-hidden": !isOpen,
      children: /*#__PURE__*/(0, _jsxruntime.jsxs)("form", {
        onSubmit: handleSubmit,
        className: "p-4 space-y-4",
        children: [
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[38][0]++, errors._form) &&
        /* istanbul ignore next */
        (cov_1s1ybn75au().b[38][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "p-3 bg-red-50 border border-red-200 rounded-lg",
          role: "alert",
          children: /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
            className: "text-sm text-red-700",
            children: errors._form
          })
        })), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-4",
          children: fields.map(field => {
            /* istanbul ignore next */
            cov_1s1ybn75au().f[22]++;
            cov_1s1ybn75au().s[94]++;
            // Textarea fields should span full width
            if (field.type === 'textarea') {
              /* istanbul ignore next */
              cov_1s1ybn75au().b[39][0]++;
              cov_1s1ybn75au().s[95]++;
              return /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
                className: "md:col-span-2 lg:col-span-3",
                children: renderField(field)
              }, field.name);
            } else
            /* istanbul ignore next */
            {
              cov_1s1ybn75au().b[39][1]++;
            }
            // Regular fields use default grid sizing
            cov_1s1ybn75au().s[96]++;
            return renderField(field);
          })
        }), children, /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "flex gap-3 pt-2",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)("button", {
            type: "submit",
            disabled: isSubmitting,
            style: {
              backgroundColor: '#22c55e',
              color: '#ffffff'
            },
            className: "px-4 py-2 rounded-lg hover:opacity-90 disabled:opacity-50 disabled:cursor-not-allowed transition-colors font-medium",
            children: isSubmitting ?
            /* istanbul ignore next */
            (cov_1s1ybn75au().b[40][0]++, 'Submitting...') :
            /* istanbul ignore next */
            (cov_1s1ybn75au().b[40][1]++, submitLabel)
          }), /*#__PURE__*/(0, _jsxruntime.jsx)("button", {
            type: "button",
            onClick: handleCancel,
            disabled: isSubmitting,
            style: {
              backgroundColor: '#e5e7eb',
              color: '#111827'
            },
            className: "px-4 py-2 rounded-lg hover:opacity-90 disabled:opacity-50 disabled:cursor-not-allowed transition-colors font-medium",
            children: cancelLabel
          })]
        })]
      })
    })]
  });
}
