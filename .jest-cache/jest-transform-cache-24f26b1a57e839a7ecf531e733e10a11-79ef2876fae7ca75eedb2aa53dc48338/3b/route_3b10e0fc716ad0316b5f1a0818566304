        (cov_lzayorkxl().s[43]++, _emailSchemas.scheduleEmailSchema.safeParse(emailData));
        /* istanbul ignore next */
        cov_lzayorkxl().s[44]++;
        if (!validation.success) {
          /* istanbul ignore next */
          cov_lzayorkxl().b[19][0]++;
          cov_lzayorkxl().s[45]++;
          return _server.NextResponse.json({
            success: false,
            error: {
              code: 'VALIDATION_ERROR',
              message: 'Invalid request',
              details: validation.error.issues
            }
          }, {
            status: 400
          });
        } else
        /* istanbul ignore next */
        {
          cov_lzayorkxl().b[19][1]++;
        }
        const result =
        /* istanbul ignore next */
        (cov_lzayorkxl().s[46]++, await _emailService.scheduleEmail(validation.data));
        /* istanbul ignore next */
        cov_lzayorkxl().s[47]++;
        results.push(result);
      }
      const successCount =
      /* istanbul ignore next */
      (cov_lzayorkxl().s[48]++, results.filter(r => {
        /* istanbul ignore next */
        cov_lzayorkxl().f[5]++;
        cov_lzayorkxl().s[49]++;
        return r.success;
      }).length);
      const failCount =
      /* istanbul ignore next */
      (cov_lzayorkxl().s[50]++, results.length - successCount);
      /* istanbul ignore next */
      cov_lzayorkxl().s[51]++;
      return _server.NextResponse.json({
        success: true,
        data: {
          scheduled: successCount,
          failed: failCount,
          total: results.length
        }
      }, {
        status: 200
      });
    } else {
      /* istanbul ignore next */
      cov_lzayorkxl().b[17][1]++;
      // Single recipient
      const validation =
      /* istanbul ignore next */
      (cov_lzayorkxl().s[52]++, _emailSchemas.scheduleEmailSchema.safeParse(body));
      /* istanbul ignore next */
      cov_lzayorkxl().s[53]++;
      if (!validation.success) {
        /* istanbul ignore next */
        cov_lzayorkxl().b[20][0]++;
        cov_lzayorkxl().s[54]++;
        return _server.NextResponse.json({
          success: false,
          error: {
            code: 'VALIDATION_ERROR',
            message: 'Invalid request',
            details: validation.error.issues
          }
        }, {
          status: 400
        });
      } else
      /* istanbul ignore next */
      {
        cov_lzayorkxl().b[20][1]++;
      }
      // 3. Call service
      const result =
      /* istanbul ignore next */
      (cov_lzayorkxl().s[55]++, await _emailService.scheduleEmail(validation.data));
      // 4. Return response
      /* istanbul ignore next */
      cov_lzayorkxl().s[56]++;
      if (result.success) {
        /* istanbul ignore next */
        cov_lzayorkxl().b[21][0]++;
        cov_lzayorkxl().s[57]++;
        return _server.NextResponse.json(result, {
          status: 200
        });
      } else {
        /* istanbul ignore next */
        cov_lzayorkxl().b[21][1]++;
        const statusCode =
        /* istanbul ignore next */
        (cov_lzayorkxl().s[58]++, result.error.code === 'VALIDATION_ERROR' ?
        /* istanbul ignore next */
        (cov_lzayorkxl().b[22][0]++, 400) :
        /* istanbul ignore next */
        (cov_lzayorkxl().b[22][1]++, result.error.code === 'NOT_FOUND' ?
        /* istanbul ignore next */
        (cov_lzayorkxl().b[23][0]++, 404) :
        /* istanbul ignore next */
        (cov_lzayorkxl().b[23][1]++, 500)));
        /* istanbul ignore next */
        cov_lzayorkxl().s[59]++;
        return _server.NextResponse.json(result, {
          status: statusCode
        });
      }
    }
  } catch (error) {
    /* istanbul ignore next */
    cov_lzayorkxl().s[60]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: 'INTERNAL_ERROR',
        message: 'Internal server error'
      }
    }, {
      status: 500
    });
  }
}
