  var newObj =
  /* istanbul ignore next */
  (cov_23h2tlezv3().s[27]++, {
    __proto__: null
  });
  var hasPropertyDescriptor =
  /* istanbul ignore next */
  (cov_23h2tlezv3().s[28]++,
  /* istanbul ignore next */
  (cov_23h2tlezv3().b[8][0]++, Object.defineProperty) &&
  /* istanbul ignore next */
  (cov_23h2tlezv3().b[8][1]++, Object.getOwnPropertyDescriptor));
  /* istanbul ignore next */
  cov_23h2tlezv3().s[29]++;
  for (var key in obj) {
    /* istanbul ignore next */
    cov_23h2tlezv3().s[30]++;
    if (
    /* istanbul ignore next */
    (cov_23h2tlezv3().b[10][0]++, key !== "default") &&
    /* istanbul ignore next */
    (cov_23h2tlezv3().b[10][1]++, Object.prototype.hasOwnProperty.call(obj, key))) {
      /* istanbul ignore next */
      cov_23h2tlezv3().b[9][0]++;
      var desc =
      /* istanbul ignore next */
      (cov_23h2tlezv3().s[31]++, hasPropertyDescriptor ?
      /* istanbul ignore next */
      (cov_23h2tlezv3().b[11][0]++, Object.getOwnPropertyDescriptor(obj, key)) :
      /* istanbul ignore next */
      (cov_23h2tlezv3().b[11][1]++, null));
      /* istanbul ignore next */
      cov_23h2tlezv3().s[32]++;
      if (
      /* istanbul ignore next */
      (cov_23h2tlezv3().b[13][0]++, desc) && (
      /* istanbul ignore next */
      (cov_23h2tlezv3().b[13][1]++, desc.get) ||
      /* istanbul ignore next */
      (cov_23h2tlezv3().b[13][2]++, desc.set))) {
        /* istanbul ignore next */
        cov_23h2tlezv3().b[12][0]++;
        cov_23h2tlezv3().s[33]++;
        Object.defineProperty(newObj, key, desc);
      } else {
        /* istanbul ignore next */
        cov_23h2tlezv3().b[12][1]++;
        cov_23h2tlezv3().s[34]++;
        newObj[key] = obj[key];
      }
    } else
    /* istanbul ignore next */
    {
      cov_23h2tlezv3().b[9][1]++;
    }
  }
  /* istanbul ignore next */
  cov_23h2tlezv3().s[35]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_23h2tlezv3().s[36]++;
  if (cache) {
    /* istanbul ignore next */
    cov_23h2tlezv3().b[14][0]++;
    cov_23h2tlezv3().s[37]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_23h2tlezv3().b[14][1]++;
  }
  cov_23h2tlezv3().s[38]++;
  return newObj;
}
async function GET(request) {
  /* istanbul ignore next */
  cov_23h2tlezv3().f[6]++;
  cov_23h2tlezv3().s[39]++;
  try {
    // 1. Auth check
    const supabase =
    /* istanbul ignore next */
    (cov_23h2tlezv3().s[40]++, (0, _authhelpersnextjs.createRouteHandlerClient)({
      cookies: _headers.cookies
    }));
    const {
      data: {
        session
      },
      error: authError
    } =
    /* istanbul ignore next */
    (cov_23h2tlezv3().s[41]++, await supabase.auth.getSession());
    /* istanbul ignore next */
    cov_23h2tlezv3().s[42]++;
    if (
    /* istanbul ignore next */
    (cov_23h2tlezv3().b[16][0]++, authError) ||
    /* istanbul ignore next */
    (cov_23h2tlezv3().b[16][1]++, !session)) {
      /* istanbul ignore next */
      cov_23h2tlezv3().b[15][0]++;
      cov_23h2tlezv3().s[43]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: _types.ERROR_CODES.UNAUTHORIZED,
          message: 'Authentication required'
        }
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_23h2tlezv3().b[15][1]++;
    }
    // Get guest ID from session
    const {
      data: guest,
      error: guestError
    } =
    /* istanbul ignore next */
    (cov_23h2tlezv3().s[44]++, await supabase.from('guests').select('id').eq('email', session.user.email).single());
    /* istanbul ignore next */
    cov_23h2tlezv3().s[45]++;
    if (
    /* istanbul ignore next */
    (cov_23h2tlezv3().b[18][0]++, guestError) ||
    /* istanbul ignore next */
    (cov_23h2tlezv3().b[18][1]++, !guest)) {
      /* istanbul ignore next */
      cov_23h2tlezv3().b[17][0]++;
      cov_23h2tlezv3().s[46]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: _types.ERROR_CODES.NOT_FOUND,
          message: 'Guest not found'
        }
      }, {
        status: 404
      });
    } else
    /* istanbul ignore next */
    {
      cov_23h2tlezv3().b[17][1]++;
    }
    // 2. Get all RSVPs for this guest
    const rsvpsResult =
    /* istanbul ignore next */
    (cov_23h2tlezv3().s[47]++, await _rsvpService.getByGuest(guest.id));
    /* istanbul ignore next */
    cov_23h2tlezv3().s[48]++;
    if (!rsvpsResult.success) {
      /* istanbul ignore next */
      cov_23h2tlezv3().b[19][0]++;
      cov_23h2tlezv3().s[49]++;
      return _server.NextResponse.json(rsvpsResult, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_23h2tlezv3().b[19][1]++;
    }
    // 3. Enrich RSVPs with activity/event details and capacity info
    const enrichedRsvps =
    /* istanbul ignore next */
    (cov_23h2tlezv3().s[50]++, await Promise.all(rsvpsResult.data.map(async rsvp => {
      /* istanbul ignore next */
      cov_23h2tlezv3().f[7]++;
      let entityDetails =
      /* istanbul ignore next */
      (cov_23h2tlezv3().s[51]++, null);
      let capacityInfo =
      /* istanbul ignore next */
      (cov_23h2tlezv3().s[52]++, null);
      let deadline =
      /* istanbul ignore next */
      (cov_23h2tlezv3().s[53]++, null);
      /* istanbul ignore next */
      cov_23h2tlezv3().s[54]++;
      if (rsvp.activity_id) {
        /* istanbul ignore next */
        cov_23h2tlezv3().b[20][0]++;
        const activityResult =
        /* istanbul ignore next */
        (cov_23h2tlezv3().s[55]++, await _activityService.get(rsvp.activity_id));
        /* istanbul ignore next */
        cov_23h2tlezv3().s[56]++;
        if (activityResult.success) {
          /* istanbul ignore next */
          cov_23h2tlezv3().b[21][0]++;
          cov_23h2tlezv3().s[57]++;
          entityDetails = {
            type: 'activity',
            ...activityResult.data
          };
          // Activities don't have RSVP deadlines
          // Get capacity info
          const capacityResult =
          /* istanbul ignore next */
          (cov_23h2tlezv3().s[58]++, await _rsvpService.calculateActivityCapacity(rsvp.activity_id));
          /* istanbul ignore next */
          cov_23h2tlezv3().s[59]++;
          if (capacityResult.success) {
            /* istanbul ignore next */
            cov_23h2tlezv3().b[22][0]++;
            cov_23h2tlezv3().s[60]++;
            capacityInfo = capacityResult.data;
          } else
          /* istanbul ignore next */
          {
            cov_23h2tlezv3().b[22][1]++;
          }
        } else
        /* istanbul ignore next */
        {
          cov_23h2tlezv3().b[21][1]++;
        }
      } else {
        /* istanbul ignore next */
        cov_23h2tlezv3().b[20][1]++;
        cov_23h2tlezv3().s[61]++;
        if (rsvp.event_id) {
          /* istanbul ignore next */
          cov_23h2tlezv3().b[23][0]++;
          const eventResult =
          /* istanbul ignore next */
          (cov_23h2tlezv3().s[62]++, await _eventService.get(rsvp.event_id));
          /* istanbul ignore next */
          cov_23h2tlezv3().s[63]++;
          if (eventResult.success) {
            /* istanbul ignore next */
            cov_23h2tlezv3().b[24][0]++;
            cov_23h2tlezv3().s[64]++;
            entityDetails = {
              type: 'event',
              ...eventResult.data
            };
            /* istanbul ignore next */
            cov_23h2tlezv3().s[65]++;
            deadline = eventResult.data.rsvpDeadline;
          } else
          /* istanbul ignore next */
          {
            cov_23h2tlezv3().b[24][1]++;
          }
        } else
        /* istanbul ignore next */
        {
          cov_23h2tlezv3().b[23][1]++;
        }
      }
      /* istanbul ignore next */
      cov_23h2tlezv3().s[66]++;
      return {
        ...rsvp,
        entity: entityDetails,
        capacity: capacityInfo,
        deadline,
        can_modify: deadline ?
        /* istanbul ignore next */
        (cov_23h2tlezv3().b[25][0]++, new Date(deadline) > new Date()) :
        /* istanbul ignore next */
        (cov_23h2tlezv3().b[25][1]++, true)
      };
    })));
    /* istanbul ignore next */
    cov_23h2tlezv3().s[67]++;
    return _server.NextResponse.json({
      success: true,
      data: enrichedRsvps
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_23h2tlezv3().s[68]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: _types.ERROR_CODES.UNKNOWN_ERROR,
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_23h2tlezv3().b[26][0]++, error.message) :
        /* istanbul ignore next */
        (cov_23h2tlezv3().b[26][1]++, 'Unknown error')
      }
    }, {
      status: 500
    });
  }
}
async function POST(request) {
  /* istanbul ignore next */
  cov_23h2tlezv3().f[8]++;
  cov_23h2tlezv3().s[69]++;
  try {
    // 1. Auth check
    const supabase =
    /* istanbul ignore next */
    (cov_23h2tlezv3().s[70]++, (0, _authhelpersnextjs.createRouteHandlerClient)({
      cookies: _headers.cookies
    }));
    const {
      data: {
        session
      },
      error: authError
    } =
    /* istanbul ignore next */
    (cov_23h2tlezv3().s[71]++, await supabase.auth.getSession());
    /* istanbul ignore next */
    cov_23h2tlezv3().s[72]++;
    if (
    /* istanbul ignore next */
    (cov_23h2tlezv3().b[28][0]++, authError) ||
    /* istanbul ignore next */
    (cov_23h2tlezv3().b[28][1]++, !session)) {
      /* istanbul ignore next */
      cov_23h2tlezv3().b[27][0]++;
      cov_23h2tlezv3().s[73]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: _types.ERROR_CODES.UNAUTHORIZED,
          message: 'Authentication required'
        }
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_23h2tlezv3().b[27][1]++;
    }
    // Get guest ID from session
    const {
      data: guest,
      error: guestError
    } =
    /* istanbul ignore next */
    (cov_23h2tlezv3().s[74]++, await supabase.from('guests').select('id').eq('email', session.user.email).single());
    /* istanbul ignore next */
    cov_23h2tlezv3().s[75]++;
    if (
    /* istanbul ignore next */
    (cov_23h2tlezv3().b[30][0]++, guestError) ||
    /* istanbul ignore next */
    (cov_23h2tlezv3().b[30][1]++, !guest)) {
      /* istanbul ignore next */
      cov_23h2tlezv3().b[29][0]++;
      cov_23h2tlezv3().s[76]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: _types.ERROR_CODES.NOT_FOUND,
          message: 'Guest not found'
        }
      }, {
        status: 404
      });
    } else
    /* istanbul ignore next */
    {
      cov_23h2tlezv3().b[29][1]++;
    }
    // 2. Parse and validate request body
    const body =
    /* istanbul ignore next */
    (cov_23h2tlezv3().s[77]++, await request.json());
    const validation =
    /* istanbul ignore next */
    (cov_23h2tlezv3().s[78]++, _rsvpSchemas.createRSVPSchema.safeParse({
      ...body,
      guest_id: guest.id
    }));
    /* istanbul ignore next */
    cov_23h2tlezv3().s[79]++;
    if (!validation.success) {
      /* istanbul ignore next */
      cov_23h2tlezv3().b[31][0]++;
      cov_23h2tlezv3().s[80]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: _types.ERROR_CODES.VALIDATION_ERROR,
          message: 'Invalid request',
          details: validation.error.issues
        }
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_23h2tlezv3().b[31][1]++;
    }
    // 3. Check capacity if activity_id is provided and status is 'attending'
    cov_23h2tlezv3().s[81]++;
    if (
    /* istanbul ignore next */
    (cov_23h2tlezv3().b[33][0]++, validation.data.activity_id) &&
    /* istanbul ignore next */
    (cov_23h2tlezv3().b[33][1]++, validation.data.status === 'attending')) {
      /* istanbul ignore next */
      cov_23h2tlezv3().b[32][0]++;
      const guestCount =
      /* istanbul ignore next */
      (cov_23h2tlezv3().s[82]++,
      /* istanbul ignore next */
      (cov_23h2tlezv3().b[34][0]++, validation.data.guest_count) ||
      /* istanbul ignore next */
      (cov_23h2tlezv3().b[34][1]++, 1));
      const capacityCheck =
      /* istanbul ignore next */
      (cov_23h2tlezv3().s[83]++, await _rsvpService.enforceCapacityLimit(validation.data.activity_id, guestCount));
      /* istanbul ignore next */
      cov_23h2tlezv3().s[84]++;
      if (!capacityCheck.success) {
        /* istanbul ignore next */
        cov_23h2tlezv3().b[35][0]++;
        cov_23h2tlezv3().s[85]++;
        return _server.NextResponse.json(capacityCheck, {
          status: 400
        });
      } else
      /* istanbul ignore next */
      {
        cov_23h2tlezv3().b[35][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_23h2tlezv3().b[32][1]++;
    }
    // Check deadline for events
    cov_23h2tlezv3().s[86]++;
    if (validation.data.event_id) {
      /* istanbul ignore next */
      cov_23h2tlezv3().b[36][0]++;
      const eventResult =
      /* istanbul ignore next */
      (cov_23h2tlezv3().s[87]++, await _eventService.get(validation.data.event_id));
      /* istanbul ignore next */
      cov_23h2tlezv3().s[88]++;
      if (
      /* istanbul ignore next */
      (cov_23h2tlezv3().b[38][0]++, eventResult.success) &&
      /* istanbul ignore next */
      (cov_23h2tlezv3().b[38][1]++, eventResult.data.rsvpDeadline)) {
        /* istanbul ignore next */
        cov_23h2tlezv3().b[37][0]++;
        const deadline =
        /* istanbul ignore next */
        (cov_23h2tlezv3().s[89]++, new Date(eventResult.data.rsvpDeadline));
        /* istanbul ignore next */
        cov_23h2tlezv3().s[90]++;
        if (deadline < new Date()) {
          /* istanbul ignore next */
          cov_23h2tlezv3().b[39][0]++;
          cov_23h2tlezv3().s[91]++;
          return _server.NextResponse.json({
            success: false,
            error: {
              code: _types.ERROR_CODES.VALIDATION_ERROR,
              message: 'RSVP deadline has passed'
            }
          }, {
            status: 400
          });
        } else
        /* istanbul ignore next */
        {
          cov_23h2tlezv3().b[39][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_23h2tlezv3().b[37][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_23h2tlezv3().b[36][1]++;
    }
    // 4. Create RSVP via service
    const result =
    /* istanbul ignore next */
    (cov_23h2tlezv3().s[92]++, await _rsvpService.create(validation.data));
    /* istanbul ignore next */
    cov_23h2tlezv3().s[93]++;
    if (!result.success) {
      /* istanbul ignore next */
      cov_23h2tlezv3().b[40][0]++;
      const statusCode =
      /* istanbul ignore next */
      (cov_23h2tlezv3().s[94]++, result.error.code === _types.ERROR_CODES.DUPLICATE_ENTRY ?
      /* istanbul ignore next */
      (cov_23h2tlezv3().b[41][0]++, 409) :
      /* istanbul ignore next */
      (cov_23h2tlezv3().b[41][1]++, 500));
      /* istanbul ignore next */
      cov_23h2tlezv3().s[95]++;
      return _server.NextResponse.json(result, {
        status: statusCode
      });
    } else
    /* istanbul ignore next */
    {
      cov_23h2tlezv3().b[40][1]++;
    }
    cov_23h2tlezv3().s[96]++;
    return _server.NextResponse.json(result, {
      status: 201
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_23h2tlezv3().s[97]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: _types.ERROR_CODES.UNKNOWN_ERROR,
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_23h2tlezv3().b[42][0]++, error.message) :
        /* istanbul ignore next */
        (cov_23h2tlezv3().b[42][1]++, 'Unknown error')
      }
    }, {
      status: 500
    });
  }
}
