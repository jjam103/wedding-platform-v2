      status: 'delivered',
      attempts: 1,
      last_attempt_at: new Date().toISOString(),
      response_status: 200
    });
    /* istanbul ignore next */
    cov_z879ubex7().s[57]++;
    return _server.NextResponse.json({
      success: true,
      message: 'Webhook processed successfully'
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_z879ubex7().s[58]++;
    console.error('Webhook processing error:', error);
    /* istanbul ignore next */
    cov_z879ubex7().s[59]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: 'WEBHOOK_ERROR',
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_z879ubex7().b[13][0]++, error.message) :
        /* istanbul ignore next */
        (cov_z879ubex7().b[13][1]++, 'Webhook processing failed')
      }
    }, {
      status: 500
    });
  }
}
