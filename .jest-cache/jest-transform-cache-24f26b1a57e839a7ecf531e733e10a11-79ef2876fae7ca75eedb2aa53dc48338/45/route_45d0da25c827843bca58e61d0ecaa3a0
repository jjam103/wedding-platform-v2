    (cov_2jvd0o1xue().s[22]++, (0, _sanitization.sanitizeInput)(validation.data.shuttleName));
    // 4. Update guest shuttle assignment
    const {
      error
    } =
    /* istanbul ignore next */
    (cov_2jvd0o1xue().s[23]++, await supabase.from('guests').update({
      shuttle_assignment:
      /* istanbul ignore next */
      (cov_2jvd0o1xue().b[3][0]++, sanitizedShuttleName) ||
      /* istanbul ignore next */
      (cov_2jvd0o1xue().b[3][1]++, null),
      updated_at: new Date().toISOString()
    }).eq('id', validation.data.guestId));
    /* istanbul ignore next */
    cov_2jvd0o1xue().s[24]++;
    if (error) {
      /* istanbul ignore next */
      cov_2jvd0o1xue().b[4][0]++;
      cov_2jvd0o1xue().s[25]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'DATABASE_ERROR',
          message: error.message,
          details: error
        }
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_2jvd0o1xue().b[4][1]++;
    }
    cov_2jvd0o1xue().s[26]++;
    return _server.NextResponse.json({
      success: true,
      data: undefined
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_2jvd0o1xue().s[27]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: 'UNKNOWN_ERROR',
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_2jvd0o1xue().b[5][0]++, error.message) :
        /* istanbul ignore next */
        (cov_2jvd0o1xue().b[5][1]++, 'Unknown error')
      }
    }, {
      status: 500
    });
  }
}
