        }), /*#__PURE__*/(0, _jsxruntime.jsx)(_Button.Button, {
          onClick: () => {
            /* istanbul ignore next */
            cov_25gi320ihn().f[35]++;
            cov_25gi320ihn().s[121]++;
            return saveConfig(true);
          },
          disabled:
          /* istanbul ignore next */
          (cov_25gi320ihn().b[36][0]++, saving) ||
          /* istanbul ignore next */
          (cov_25gi320ihn().b[36][1]++, !isDirty),
          children: saving ?
          /* istanbul ignore next */
          (cov_25gi320ihn().b[37][0]++, 'Saving...') :
          /* istanbul ignore next */
          (cov_25gi320ihn().b[37][1]++, 'Save Changes')
        })]
      }),
      /* istanbul ignore next */
      (cov_25gi320ihn().b[38][0]++, isDirty) &&
      /* istanbul ignore next */
      (cov_25gi320ihn().b[38][1]++, !saving) &&
      /* istanbul ignore next */
      (cov_25gi320ihn().b[38][2]++, /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
        className: "text-sm text-amber-600",
        children: "You have unsaved changes. They will be auto-saved in 30 seconds."
      }))]
    })]
  });
}
