        cov_25gi320ihn().b[14][1]++;
        cov_25gi320ihn().s[32]++;
        newObj[key] = obj[key];
      }
    } else
    /* istanbul ignore next */
    {
      cov_25gi320ihn().b[11][1]++;
    }
  }
  /* istanbul ignore next */
  cov_25gi320ihn().s[33]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_25gi320ihn().s[34]++;
  if (cache) {
    /* istanbul ignore next */
    cov_25gi320ihn().b[16][0]++;
    cov_25gi320ihn().s[35]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_25gi320ihn().b[16][1]++;
  }
  cov_25gi320ihn().s[36]++;
  return newObj;
}
