    (cov_ofvc6snv9().s[13]++, await supabase.from('guests').select('id').eq('email', user.email).single());
    /* istanbul ignore next */
    cov_ofvc6snv9().s[14]++;
    if (
    /* istanbul ignore next */
    (cov_ofvc6snv9().b[2][0]++, currentGuestError) ||
    /* istanbul ignore next */
    (cov_ofvc6snv9().b[2][1]++, !currentGuest)) {
      /* istanbul ignore next */
      cov_ofvc6snv9().b[1][0]++;
      cov_ofvc6snv9().s[15]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'GUEST_NOT_FOUND',
          message: 'Guest not found'
        }
      }, {
        status: 404
      });
    } else
    /* istanbul ignore next */
    {
      cov_ofvc6snv9().b[1][1]++;
    }
    // Parse and validate request body
    const body =
    /* istanbul ignore next */
    (cov_ofvc6snv9().s[16]++, await request.json());
    const validation =
    /* istanbul ignore next */
    (cov_ofvc6snv9().s[17]++, updateTransportationSchema.safeParse(body));
    /* istanbul ignore next */
    cov_ofvc6snv9().s[18]++;
    if (!validation.success) {
      /* istanbul ignore next */
      cov_ofvc6snv9().b[3][0]++;
      cov_ofvc6snv9().s[19]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'VALIDATION_ERROR',
          message: 'Invalid request data',
          details: validation.error.issues
        }
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_ofvc6snv9().b[3][1]++;
    }
    // Sanitize input
    const sanitized =
    /* istanbul ignore next */
    (cov_ofvc6snv9().s[20]++, {
      airport_code: validation.data.airport_code,
      updated_at: new Date().toISOString()
    });
    /* istanbul ignore next */
    cov_ofvc6snv9().s[21]++;
    if (validation.data.flight_number) {
      /* istanbul ignore next */
      cov_ofvc6snv9().b[4][0]++;
      cov_ofvc6snv9().s[22]++;
      sanitized.flight_number = (0, _sanitization.sanitizeInput)(validation.data.flight_number);
    } else
    /* istanbul ignore next */
    {
      cov_ofvc6snv9().b[4][1]++;
    }
    cov_ofvc6snv9().s[23]++;
    if (validation.data.arrival_date) {
      /* istanbul ignore next */
      cov_ofvc6snv9().b[5][0]++;
      cov_ofvc6snv9().s[24]++;
      sanitized.arrival_date = validation.data.arrival_date;
    } else
    /* istanbul ignore next */
    {
      cov_ofvc6snv9().b[5][1]++;
    }
    cov_ofvc6snv9().s[25]++;
    if (validation.data.departure_date) {
      /* istanbul ignore next */
      cov_ofvc6snv9().b[6][0]++;
      cov_ofvc6snv9().s[26]++;
      sanitized.departure_date = validation.data.departure_date;
    } else
    /* istanbul ignore next */
    {
      cov_ofvc6snv9().b[6][1]++;
    }
    // Update guest transportation information
    const {
      data: updated,
      error: updateError
    } =
    /* istanbul ignore next */
    (cov_ofvc6snv9().s[27]++, await supabase.from('guests').update(sanitized).eq('id', currentGuest.id).select().single());
    /* istanbul ignore next */
    cov_ofvc6snv9().s[28]++;
    if (updateError) {
      /* istanbul ignore next */
      cov_ofvc6snv9().b[7][0]++;
      cov_ofvc6snv9().s[29]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'DATABASE_ERROR',
          message: updateError.message
        }
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_ofvc6snv9().b[7][1]++;
    }
    cov_ofvc6snv9().s[30]++;
    return _server.NextResponse.json({
      success: true,
      data: updated
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_ofvc6snv9().s[31]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: 'UNKNOWN_ERROR',
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_ofvc6snv9().b[8][0]++, error.message) :
        /* istanbul ignore next */
        (cov_ofvc6snv9().b[8][1]++, 'Unknown error')
      }
    }, {
      status: 500
    });
  }
}
