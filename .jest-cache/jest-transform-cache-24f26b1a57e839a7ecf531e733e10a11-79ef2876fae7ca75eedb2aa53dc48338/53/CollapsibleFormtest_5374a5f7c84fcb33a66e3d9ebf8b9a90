            expect(nameInput).toHaveAttribute('placeholder', 'Enter name');
        });
        it('should render email input fields', ()=>{
            (0, _react.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_CollapsibleForm.CollapsibleForm, {
                title: "Add New Item",
