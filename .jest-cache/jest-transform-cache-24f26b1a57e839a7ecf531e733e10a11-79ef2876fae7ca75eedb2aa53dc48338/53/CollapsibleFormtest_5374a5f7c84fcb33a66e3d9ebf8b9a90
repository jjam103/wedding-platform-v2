const _jsxruntime = require("react/jsx-runtime");
const _react = require("@testing-library/react");
const _zod = require("zod");
const _CollapsibleForm = require("./CollapsibleForm");
// Test schema
const testSchema = _zod.z.object({
    name: _zod.z.string().min(1, 'Name is required'),
    email: _zod.z.string().email('Invalid email'),
    age: _zod.z.number().min(1, 'Age is required'),
    description: _zod.z.string().optional()
});
// Test fields
const testFields = [
    {
        name: 'name',
        label: 'Name',
        type: 'text',
        required: true,
        placeholder: 'Enter name'
    },
    {
        name: 'email',
        label: 'Email',
        type: 'email',
        required: true,
        placeholder: 'Enter email'
    },
    {
        name: 'age',
        label: 'Age',
        type: 'number',
        required: true
    },
    {
        name: 'description',
        label: 'Description',
        type: 'textarea',
        rows: 4
    }
];
