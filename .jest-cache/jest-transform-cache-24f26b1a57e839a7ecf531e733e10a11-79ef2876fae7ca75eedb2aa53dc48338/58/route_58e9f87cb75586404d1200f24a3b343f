      cov_1fzryh21pa().s[17]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'VALIDATION_ERROR',
          message: 'Guest ID is required'
        }
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_1fzryh21pa().b[2][1]++;
    }
    // 3. Fetch activity RSVPs with capacity info
    const {
      data: activityRSVPs,
      error: activityError
    } =
    /* istanbul ignore next */
    (cov_1fzryh21pa().s[18]++, await supabase.from('activity_rsvps').select(`
        id,
        status,
        guest_count,
        dietary_restrictions,
        activities (
          id,
          title,
          date,
          time,
          location,
          capacity,
          requires_guest_count,
          requires_dietary_info
        )
      `).eq('guest_id', guestId));
    /* istanbul ignore next */
    cov_1fzryh21pa().s[19]++;
    if (activityError) {
      /* istanbul ignore next */
      cov_1fzryh21pa().b[3][0]++;
      cov_1fzryh21pa().s[20]++;
      console.error('Error fetching activity RSVPs:', activityError);
      /* istanbul ignore next */
      cov_1fzryh21pa().s[21]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'DATABASE_ERROR',
          message: 'Failed to fetch activity RSVPs',
          details: activityError
        }
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_1fzryh21pa().b[3][1]++;
    }
    // 4. Fetch event RSVPs
    const {
      data: eventRSVPs,
      error: eventError
    } =
    /* istanbul ignore next */
    (cov_1fzryh21pa().s[22]++, await supabase.from('event_rsvps').select(`
        id,
        status,
        events (
          id,
          title,
          date,
          time,
          location
        )
      `).eq('guest_id', guestId));
    /* istanbul ignore next */
    cov_1fzryh21pa().s[23]++;
    if (eventError) {
      /* istanbul ignore next */
      cov_1fzryh21pa().b[4][0]++;
      cov_1fzryh21pa().s[24]++;
      console.error('Error fetching event RSVPs:', eventError);
      /* istanbul ignore next */
      cov_1fzryh21pa().s[25]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'DATABASE_ERROR',
          message: 'Failed to fetch event RSVPs',
          details: eventError
        }
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_1fzryh21pa().b[4][1]++;
    }
    // 5. Fetch accommodation assignments
    const {
      data: accommodations,
      error: accommodationError
    } =
    /* istanbul ignore next */
    (cov_1fzryh21pa().s[26]++, await supabase.from('guest_accommodations').select(`
        id,
        check_in_date,
        check_out_date,
        room_types (
          id,
          name,
          accommodations (
            id,
            name,
            location
          )
        )
      `).eq('guest_id', guestId));
    /* istanbul ignore next */
    cov_1fzryh21pa().s[27]++;
    if (accommodationError) {
      /* istanbul ignore next */
      cov_1fzryh21pa().b[5][0]++;
      cov_1fzryh21pa().s[28]++;
      console.error('Error fetching accommodations:', accommodationError);
      /* istanbul ignore next */
      cov_1fzryh21pa().s[29]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'DATABASE_ERROR',
          message: 'Failed to fetch accommodations',
          details: accommodationError
        }
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_1fzryh21pa().b[5][1]++;
    }
    // 6. Calculate capacity remaining for each activity
    const activitiesWithCapacity =
    /* istanbul ignore next */
    (cov_1fzryh21pa().s[30]++, await Promise.all((
    /* istanbul ignore next */
    (cov_1fzryh21pa().b[6][0]++, activityRSVPs) ||
    /* istanbul ignore next */
    (cov_1fzryh21pa().b[6][1]++, [])).map(async rsvp => {
      /* istanbul ignore next */
      cov_1fzryh21pa().f[5]++;
      const activity =
      /* istanbul ignore next */
      (cov_1fzryh21pa().s[31]++, rsvp.activities);
      /* istanbul ignore next */
      cov_1fzryh21pa().s[32]++;
      if (
      /* istanbul ignore next */
      (cov_1fzryh21pa().b[8][0]++, !activity) ||
      /* istanbul ignore next */
      (cov_1fzryh21pa().b[8][1]++, !activity.capacity)) {
        /* istanbul ignore next */
        cov_1fzryh21pa().b[7][0]++;
        cov_1fzryh21pa().s[33]++;
        return {
          id: rsvp.id,
          name:
          /* istanbul ignore next */
          (cov_1fzryh21pa().b[9][0]++, activity?.title) ||
          /* istanbul ignore next */
          (cov_1fzryh21pa().b[9][1]++, 'Unknown Activity'),
          type: 'activity',
          status:
          /* istanbul ignore next */
          (cov_1fzryh21pa().b[10][0]++, rsvp.status) ||
          /* istanbul ignore next */
          (cov_1fzryh21pa().b[10][1]++, 'pending'),
          guestCount: rsvp.guest_count,
          dietaryRestrictions: rsvp.dietary_restrictions,
          requiresGuestCount:
          /* istanbul ignore next */
          (cov_1fzryh21pa().b[11][0]++, activity?.requires_guest_count) ||
          /* istanbul ignore next */
          (cov_1fzryh21pa().b[11][1]++, false),
          requiresDietaryInfo:
          /* istanbul ignore next */
          (cov_1fzryh21pa().b[12][0]++, activity?.requires_dietary_info) ||
          /* istanbul ignore next */
          (cov_1fzryh21pa().b[12][1]++, false),
          date: activity?.date,
          time: activity?.time,
          location: activity?.location
        };
      } else
      /* istanbul ignore next */
      {
        cov_1fzryh21pa().b[7][1]++;
      }
      // Count attending RSVPs for this activity
      const {
        count,
        error: countError
      } =
      /* istanbul ignore next */
      (cov_1fzryh21pa().s[34]++, await supabase.from('activity_rsvps').select('*', {
        count: 'exact',
        head: true
      }).eq('activity_id', activity.id).eq('status', 'attending'));
      /* istanbul ignore next */
      cov_1fzryh21pa().s[35]++;
      if (countError) {
        /* istanbul ignore next */
        cov_1fzryh21pa().b[13][0]++;
        cov_1fzryh21pa().s[36]++;
        console.error('Error counting RSVPs:', countError);
      } else
      /* istanbul ignore next */
      {
        cov_1fzryh21pa().b[13][1]++;
      }
      const capacityRemaining =
      /* istanbul ignore next */
      (cov_1fzryh21pa().s[37]++, activity.capacity - (
      /* istanbul ignore next */
      (cov_1fzryh21pa().b[14][0]++, count) ||
      /* istanbul ignore next */
      (cov_1fzryh21pa().b[14][1]++, 0)));
      /* istanbul ignore next */
      cov_1fzryh21pa().s[38]++;
      return {
        id: rsvp.id,
        name: activity.title,
        type: 'activity',
        status:
        /* istanbul ignore next */
        (cov_1fzryh21pa().b[15][0]++, rsvp.status) ||
        /* istanbul ignore next */
        (cov_1fzryh21pa().b[15][1]++, 'pending'),
        guestCount: rsvp.guest_count,
        dietaryRestrictions: rsvp.dietary_restrictions,
        capacity: activity.capacity,
        capacityRemaining,
        requiresGuestCount:
        /* istanbul ignore next */
        (cov_1fzryh21pa().b[16][0]++, activity.requires_guest_count) ||
        /* istanbul ignore next */
        (cov_1fzryh21pa().b[16][1]++, false),
        requiresDietaryInfo:
        /* istanbul ignore next */
        (cov_1fzryh21pa().b[17][0]++, activity.requires_dietary_info) ||
        /* istanbul ignore next */
        (cov_1fzryh21pa().b[17][1]++, false),
        date: activity.date,
        time: activity.time,
        location: activity.location
      };
    })));
    // 7. Format event RSVPs
    const formattedEvents =
    /* istanbul ignore next */
    (cov_1fzryh21pa().s[39]++, (
    /* istanbul ignore next */
    (cov_1fzryh21pa().b[18][0]++, eventRSVPs) ||
    /* istanbul ignore next */
    (cov_1fzryh21pa().b[18][1]++, [])).map(rsvp => {
      /* istanbul ignore next */
      cov_1fzryh21pa().f[6]++;
      cov_1fzryh21pa().s[40]++;
      return {
        id: rsvp.id,
        name:
        /* istanbul ignore next */
        (cov_1fzryh21pa().b[19][0]++, rsvp.events?.title) ||
        /* istanbul ignore next */
        (cov_1fzryh21pa().b[19][1]++, 'Unknown Event'),
        type: 'event',
        status:
        /* istanbul ignore next */
        (cov_1fzryh21pa().b[20][0]++, rsvp.status) ||
        /* istanbul ignore next */
        (cov_1fzryh21pa().b[20][1]++, 'pending'),
        date: rsvp.events?.date,
        time: rsvp.events?.time,
        location: rsvp.events?.location
      };
    }));
    // 8. Format accommodations
    const formattedAccommodations =
    /* istanbul ignore next */
    (cov_1fzryh21pa().s[41]++, (
    /* istanbul ignore next */
    (cov_1fzryh21pa().b[21][0]++, accommodations) ||
    /* istanbul ignore next */
    (cov_1fzryh21pa().b[21][1]++, [])).map(acc => {
      /* istanbul ignore next */
      cov_1fzryh21pa().f[7]++;
      cov_1fzryh21pa().s[42]++;
      return {
        id: acc.id,
        name: `${
        /* istanbul ignore next */
        (cov_1fzryh21pa().b[22][0]++, acc.room_types?.accommodations?.name) ||
        /* istanbul ignore next */
        (cov_1fzryh21pa().b[22][1]++, 'Unknown')} - ${
        /* istanbul ignore next */
        (cov_1fzryh21pa().b[23][0]++, acc.room_types?.name) ||
        /* istanbul ignore next */
        (cov_1fzryh21pa().b[23][1]++, 'Unknown Room')}`,
        type: 'accommodation',
        status: 'attending',
        date: acc.check_in_date,
        location: acc.room_types?.accommodations?.location
      };
    }));
    // 9. Return formatted data
    /* istanbul ignore next */
    cov_1fzryh21pa().s[43]++;
    return _server.NextResponse.json({
      success: true,
      data: {
        activities: activitiesWithCapacity,
        events: formattedEvents,
        accommodations: formattedAccommodations
      }
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_1fzryh21pa().s[44]++;
    console.error('Error in GET /api/admin/guests/[id]/rsvps:', error);
    /* istanbul ignore next */
    cov_1fzryh21pa().s[45]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: 'UNKNOWN_ERROR',
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_1fzryh21pa().b[24][0]++, error.message) :
        /* istanbul ignore next */
        (cov_1fzryh21pa().b[24][1]++, 'Unknown error occurred')
      }
    }, {
      status: 500
    });
  }
}
