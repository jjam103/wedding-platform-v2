    (cov_2neupgujjv().b[2][0]++, this.state.hasError) &&
    /* istanbul ignore next */
    (cov_2neupgujjv().b[2][1]++, this.state.error)) {
      /* istanbul ignore next */
      cov_2neupgujjv().b[1][0]++;
      cov_2neupgujjv().s[19]++;
      // Use custom fallback if provided
      if (this.props.fallback) {
        /* istanbul ignore next */
        cov_2neupgujjv().b[3][0]++;
        cov_2neupgujjv().s[20]++;
        return this.props.fallback(this.state.error, this.handleReset);
      } else
      /* istanbul ignore next */
      {
        cov_2neupgujjv().b[3][1]++;
      }
      // Default fallback UI
      cov_2neupgujjv().s[21]++;
      return /*#__PURE__*/(0, _jsxruntime.jsx)(DefaultErrorFallback, {
        error: this.state.error,
        onReset: this.handleReset
      });
    } else
    /* istanbul ignore next */
    {
      cov_2neupgujjv().b[1][1]++;
    }
    cov_2neupgujjv().s[22]++;
    return this.props.children;
  }
}
function DefaultErrorFallback({
  error,
  onReset
}) {
  /* istanbul ignore next */
  cov_2neupgujjv().f[9]++;
