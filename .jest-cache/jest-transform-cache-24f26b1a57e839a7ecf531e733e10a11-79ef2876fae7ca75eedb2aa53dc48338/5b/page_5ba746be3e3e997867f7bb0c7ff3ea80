    } catch (error) {
      /* istanbul ignore next */
      cov_17muq5pwf5().s[25]++;
      addToast({
        type: 'error',
        message: 'Failed to load activity details'
      });
    } finally {
      /* istanbul ignore next */
      cov_17muq5pwf5().s[26]++;
      setLoading(false);
    }
  };
  /* istanbul ignore next */
  cov_17muq5pwf5().s[27]++;
  const handleSave = () => {
    /* istanbul ignore next */
    cov_17muq5pwf5().f[5]++;
    cov_17muq5pwf5().s[28]++;
    addToast({
      type: 'success',
      message: 'Sections saved successfully'
    });
  };
  /* istanbul ignore next */
  cov_17muq5pwf5().s[29]++;
  const handleClose = () => {
    /* istanbul ignore next */
    cov_17muq5pwf5().f[6]++;
    cov_17muq5pwf5().s[30]++;
    router.push('/admin/activities');
  };
  /* istanbul ignore next */
  cov_17muq5pwf5().s[31]++;
  if (
  /* istanbul ignore next */
  (cov_17muq5pwf5().b[3][0]++, loading) ||
  /* istanbul ignore next */
  (cov_17muq5pwf5().b[3][1]++, !activityId)) {
    /* istanbul ignore next */
    cov_17muq5pwf5().b[2][0]++;
    cov_17muq5pwf5().s[32]++;
    return /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "flex items-center justify-center min-h-screen",
      children: /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: "text-gray-600",
        children: "Loading..."
      })
    });
  } else
  /* istanbul ignore next */
  {
    cov_17muq5pwf5().b[2][1]++;
  }
  cov_17muq5pwf5().s[33]++;
  return /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
    className: "space-y-6 p-6",
    children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
      className: "flex items-center justify-between border-b border-gray-200 pb-4",
      children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("h1", {
          className: "text-3xl font-bold text-sage-900",
          children: "Manage Sections"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
          className: "text-sage-600 mt-1",
          children: activityName ?
          /* istanbul ignore next */
          (cov_17muq5pwf5().b[4][0]++, `Activity: ${activityName}`) :
          /* istanbul ignore next */
          (cov_17muq5pwf5().b[4][1]++, 'Activity Sections')
        })]
      }), /*#__PURE__*/(0, _jsxruntime.jsx)(_Button.Button, {
        variant: "secondary",
        onClick: handleClose,
        "aria-label": "Back to activities",
        children: "‚Üê Back to Activities"
      })]
    }), /*#__PURE__*/(0, _jsxruntime.jsx)(_SectionEditor.SectionEditor, {
      pageType: "activity",
      pageId: activityId,
      onSave: handleSave,
      onClose: handleClose
    })]
  });
}
