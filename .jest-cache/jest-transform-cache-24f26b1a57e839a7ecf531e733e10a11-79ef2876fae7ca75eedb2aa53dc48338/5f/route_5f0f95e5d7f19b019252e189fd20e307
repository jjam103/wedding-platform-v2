  cov_292iy3ggrj().s[32]++;
  if (cache) {
    /* istanbul ignore next */
    cov_292iy3ggrj().b[14][0]++;
    cov_292iy3ggrj().s[33]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_292iy3ggrj().b[14][1]++;
  }
  cov_292iy3ggrj().s[34]++;
  return newObj;
}
async function GET(request, {
  params
}) {
  /* istanbul ignore next */
  cov_292iy3ggrj().f[4]++;
  cov_292iy3ggrj().s[35]++;
  try {
    // Await params for Next.js 15+
    const {
      slug
    } =
    /* istanbul ignore next */
    (cov_292iy3ggrj().s[36]++, await params);
    // 1. AUTHENTICATION
    const supabase =
    /* istanbul ignore next */
    (cov_292iy3ggrj().s[37]++, (0, _authhelpersnextjs.createRouteHandlerClient)({
      cookies: _headers.cookies
    }));
    const {
      data: {
        session
      },
      error: authError
    } =
    /* istanbul ignore next */
    (cov_292iy3ggrj().s[38]++, await supabase.auth.getSession());
    /* istanbul ignore next */
