        /* istanbul ignore next */
        (cov_ahtkxhgiq().b[32][0]++, rsvp.guest_count) ||
        /* istanbul ignore next */
        (cov_ahtkxhgiq().b[32][1]++, 1));
      }, 0)) :
      /* istanbul ignore next */
      (cov_ahtkxhgiq().b[31][1]++, 0));
      // Calculate capacity remaining
      const capacityRemaining =
      /* istanbul ignore next */
      (cov_ahtkxhgiq().s[61]++, activity.capacity ?
      /* istanbul ignore next */
      (cov_ahtkxhgiq().b[33][0]++, activity.capacity - currentAttendees) :
      /* istanbul ignore next */
      (cov_ahtkxhgiq().b[33][1]++, null));
      // Calculate net cost (cost per person - host subsidy)
      const netCost =
      /* istanbul ignore next */
      (cov_ahtkxhgiq().s[62]++, activity.costPerPerson ?
      /* istanbul ignore next */
      (cov_ahtkxhgiq().b[34][0]++, Math.max(0, activity.costPerPerson - (
      /* istanbul ignore next */
      (cov_ahtkxhgiq().b[35][0]++, activity.hostSubsidy) ||
      /* istanbul ignore next */
      (cov_ahtkxhgiq().b[35][1]++, 0)))) :
      /* istanbul ignore next */
      (cov_ahtkxhgiq().b[34][1]++, 0));
      /* istanbul ignore next */
      cov_ahtkxhgiq().s[63]++;
      return {
        ...activity,
        rsvpStatus,
        capacityRemaining,
        netCost
      };
    })));
    // 4. RESPONSE
    /* istanbul ignore next */
    cov_ahtkxhgiq().s[64]++;
    return _server.NextResponse.json({
      success: true,
      data: activitiesWithDetails
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_ahtkxhgiq().s[65]++;
    console.error('API Error:', {
      path: request.url,
      error
    });
    /* istanbul ignore next */
    cov_ahtkxhgiq().s[66]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: 'INTERNAL_ERROR',
        message: 'An unexpected error occurred'
      }
    }, {
      status: 500
    });
  }
}
