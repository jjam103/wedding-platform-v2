    /* istanbul ignore next */
    cov_1kc7jbrevq().b[14][0]++;
    cov_1kc7jbrevq().s[30]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_1kc7jbrevq().b[14][1]++;
  }
  cov_1kc7jbrevq().s[31]++;
  return newObj;
}
