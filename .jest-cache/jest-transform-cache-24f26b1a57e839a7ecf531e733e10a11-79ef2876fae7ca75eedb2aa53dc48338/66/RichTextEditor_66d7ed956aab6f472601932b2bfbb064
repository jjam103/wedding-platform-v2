}
function _interop_require_wildcard(obj, nodeInterop) {
  /* istanbul ignore next */
  cov_1z17x80b5().f[4]++;
  cov_1z17x80b5().s[14]++;
  if (
  /* istanbul ignore next */
  (cov_1z17x80b5().b[5][0]++, !nodeInterop) &&
  /* istanbul ignore next */
  (cov_1z17x80b5().b[5][1]++, obj) &&
  /* istanbul ignore next */
  (cov_1z17x80b5().b[5][2]++, obj.__esModule)) {
    /* istanbul ignore next */
    cov_1z17x80b5().b[4][0]++;
    cov_1z17x80b5().s[15]++;
    return obj;
  } else
  /* istanbul ignore next */
  {
    cov_1z17x80b5().b[4][1]++;
  }
  cov_1z17x80b5().s[16]++;
  if (
  /* istanbul ignore next */
  (cov_1z17x80b5().b[7][0]++, obj === null) ||
  /* istanbul ignore next */
  (cov_1z17x80b5().b[7][1]++, typeof obj !== "object") &&
  /* istanbul ignore next */
  (cov_1z17x80b5().b[7][2]++, typeof obj !== "function")) {
    /* istanbul ignore next */
    cov_1z17x80b5().b[6][0]++;
    cov_1z17x80b5().s[17]++;
    return {
      default: obj
    };
  } else
  /* istanbul ignore next */
  {
    cov_1z17x80b5().b[6][1]++;
  }
  var cache =
  /* istanbul ignore next */
  (cov_1z17x80b5().s[18]++, _getRequireWildcardCache(nodeInterop));
  /* istanbul ignore next */
  cov_1z17x80b5().s[19]++;
  if (
  /* istanbul ignore next */
  (cov_1z17x80b5().b[9][0]++, cache) &&
  /* istanbul ignore next */
  (cov_1z17x80b5().b[9][1]++, cache.has(obj))) {
    /* istanbul ignore next */
    cov_1z17x80b5().b[8][0]++;
    cov_1z17x80b5().s[20]++;
    return cache.get(obj);
  } else
  /* istanbul ignore next */
  {
    cov_1z17x80b5().b[8][1]++;
  }
  var newObj =
  /* istanbul ignore next */
  (cov_1z17x80b5().s[21]++, {
    __proto__: null
  });
  var hasPropertyDescriptor =
  /* istanbul ignore next */
  (cov_1z17x80b5().s[22]++,
  /* istanbul ignore next */
  (cov_1z17x80b5().b[10][0]++, Object.defineProperty) &&
  /* istanbul ignore next */
  (cov_1z17x80b5().b[10][1]++, Object.getOwnPropertyDescriptor));
  /* istanbul ignore next */
  cov_1z17x80b5().s[23]++;
  for (var key in obj) {
    /* istanbul ignore next */
    cov_1z17x80b5().s[24]++;
    if (
    /* istanbul ignore next */
    (cov_1z17x80b5().b[12][0]++, key !== "default") &&
    /* istanbul ignore next */
    (cov_1z17x80b5().b[12][1]++, Object.prototype.hasOwnProperty.call(obj, key))) {
      /* istanbul ignore next */
      cov_1z17x80b5().b[11][0]++;
      var desc =
      /* istanbul ignore next */
      (cov_1z17x80b5().s[25]++, hasPropertyDescriptor ?
      /* istanbul ignore next */
      (cov_1z17x80b5().b[13][0]++, Object.getOwnPropertyDescriptor(obj, key)) :
      /* istanbul ignore next */
      (cov_1z17x80b5().b[13][1]++, null));
      /* istanbul ignore next */
      cov_1z17x80b5().s[26]++;
      if (
      /* istanbul ignore next */
      (cov_1z17x80b5().b[15][0]++, desc) && (
      /* istanbul ignore next */
      (cov_1z17x80b5().b[15][1]++, desc.get) ||
      /* istanbul ignore next */
      (cov_1z17x80b5().b[15][2]++, desc.set))) {
        /* istanbul ignore next */
        cov_1z17x80b5().b[14][0]++;
        cov_1z17x80b5().s[27]++;
        Object.defineProperty(newObj, key, desc);
      } else {
        /* istanbul ignore next */
        cov_1z17x80b5().b[14][1]++;
        cov_1z17x80b5().s[28]++;
