                                    selectable && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                        className: "mb-3 flex items-center",
                                        onClick: (e)=>e.stopPropagation(),
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("input", {
                                                type: "checkbox",
                                                checked: isSelected,
                                                onChange: (e)=>handleSelectRow(rowId, e.target.checked),
                                                className: "h-5 w-5 rounded border-sage-300 text-jungle-500 focus:ring-jungle-500",
                                                "aria-label": `Select ${entityType.slice(0, -1)} ${rowIndex + 1}`
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                className: "ml-2 text-sm text-sage-600",
                                                children: "Select"
                                            })
                                        ]
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                        className: "space-y-2",
                                        children: columns.filter((col)=>!col.mobileHidden).map((col)=>/*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                className: "flex flex-col",
                                                children: [
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                        className: "text-xs font-medium text-sage-600 uppercase tracking-wider",
                                                        children: col.label
                                                    }),
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                        className: "mt-1 text-sm text-sage-900",
                                                        children: col.render ? col.render(row[col.key], row) : String(row[col.key] ?? '')
                                                    })
                                                ]
                                            }, String(col.key)))
                                    }),
                                    onDelete && /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                        className: "mt-3 pt-3 border-t border-sage-200",
                                        onClick: (e)=>e.stopPropagation(),
                                        children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_Button.Button, {
                                            variant: "danger",
                                            size: "sm",
                                            onClick: ()=>onDelete(row),
                                            className: "w-full min-h-[44px]",
                                            "aria-label": `Delete ${entityType.slice(0, -1)} ${rowIndex + 1}`,
                                            children: "Delete"
                                        })
                                    })
                                ]
                            }, rowId || rowIndex);
                        })
                    }),
                    totalPages > 1 && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "px-4 md:px-6 py-4 border-t border-sage-200 flex flex-col md:flex-row items-stretch md:items-center justify-between gap-4",
                        role: "navigation",
                        "aria-label": "Pagination",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                className: "flex items-center justify-center md:justify-start gap-2",
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("label", {
                                        htmlFor: "page-size-select",
                                        className: "text-sm text-sage-600",
                                        children: "Rows per page:"
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("select", {
                                        id: "page-size-select",
                                        value: localPageSize,
                                        onChange: (e)=>handlePageSizeChange(Number(e.target.value)),
                                        className: "px-3 py-2 border border-sage-300 rounded text-sm focus:outline-none focus:ring-2 focus:ring-jungle-500 min-h-[44px]",
                                        "aria-label": "Select number of rows per page",
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("option", {
                                                value: 25,
                                                children: "25"
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("option", {
                                                value: 50,
                                                children: "50"
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("option", {
                                                value: 100,
                                                children: "100"
                                            })
                                        ]
                                    })
                                ]
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                className: "flex flex-col md:flex-row items-center gap-3",
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                        className: "text-sm text-sage-600 text-center",
                                        "aria-live": "polite",
                                        "aria-atomic": "true",
                                        children: [
                                            "Page ",
                                            localPage,
                                            " of ",
                                            totalPages
                                        ]
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                        className: "flex gap-2 w-full md:w-auto",
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)(_Button.Button, {
                                                variant: "ghost",
                                                size: "sm",
                                                onClick: ()=>handlePageChange(localPage - 1),
                                                disabled: localPage === 1,
                                                className: "flex-1 md:flex-none",
                                                "aria-label": "Go to previous page",
                                                children: "Previous"
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)(_Button.Button, {
                                                variant: "ghost",
                                                size: "sm",
                                                onClick: ()=>handlePageChange(localPage + 1),
                                                disabled: localPage === totalPages,
                                                className: "flex-1 md:flex-none",
                                                "aria-label": "Go to next page",
                                                children: "Next"
                                            })
                                        ]
                                    })
                                ]
                            })
                        ]
                    })
                ]
            })
        ]
    });
}

