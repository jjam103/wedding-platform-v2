    }
    // Await params
    const resolvedParams =
    /* istanbul ignore next */
    (cov_2bolri38dj().s[20]++, await params);
    // 3. Update RSVP
    const {
      data,
      error
    } =
    /* istanbul ignore next */
    (cov_2bolri38dj().s[21]++, await supabase.from('rsvps').update({
      ...validation.data,
      updated_at: new Date().toISOString(),
      responded_at: validation.data.status !== 'pending' ?
      /* istanbul ignore next */
      (cov_2bolri38dj().b[3][0]++, new Date().toISOString()) :
      /* istanbul ignore next */
      (cov_2bolri38dj().b[3][1]++, undefined)
    }).eq('id', resolvedParams.id).select().single());
    /* istanbul ignore next */
    cov_2bolri38dj().s[22]++;
    if (error) {
      /* istanbul ignore next */
      cov_2bolri38dj().b[4][0]++;
      cov_2bolri38dj().s[23]++;
      if (error.code === 'PGRST116') {
        /* istanbul ignore next */
        cov_2bolri38dj().b[5][0]++;
        cov_2bolri38dj().s[24]++;
        return _server.NextResponse.json({
          success: false,
          error: {
            code: 'NOT_FOUND',
            message: 'RSVP not found'
          }
        }, {
          status: 404
        });
      } else
      /* istanbul ignore next */
      {
        cov_2bolri38dj().b[5][1]++;
      }
      cov_2bolri38dj().s[25]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'DATABASE_ERROR',
          message: error.message
        }
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_2bolri38dj().b[4][1]++;
    }
    cov_2bolri38dj().s[26]++;
    return _server.NextResponse.json({
      success: true,
      data
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_2bolri38dj().s[27]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: 'INTERNAL_ERROR',
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_2bolri38dj().b[6][0]++, error.message) :
        /* istanbul ignore next */
        (cov_2bolri38dj().b[6][1]++, 'Internal server error')
      }
    }, {
      status: 500
    });
  }
}
