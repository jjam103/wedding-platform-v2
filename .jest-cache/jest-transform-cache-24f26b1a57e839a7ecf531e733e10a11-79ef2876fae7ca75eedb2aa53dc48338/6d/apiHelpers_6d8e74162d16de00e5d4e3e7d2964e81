    if (value) {
      /* istanbul ignore next */
      cov_m8r43dg9d().b[9][0]++;
      cov_m8r43dg9d().s[53]++;
      filters[key] = value;
    } else
    /* istanbul ignore next */
    {
      cov_m8r43dg9d().b[9][1]++;
    }
  });
  /* istanbul ignore next */
  cov_m8r43dg9d().s[54]++;
  return filters;
}
function validateBody(body, schema) {
  /* istanbul ignore next */
  cov_m8r43dg9d().f[23]++;
  const validation =
  /* istanbul ignore next */
  (cov_m8r43dg9d().s[55]++, schema.safeParse(body));
  /* istanbul ignore next */
  cov_m8r43dg9d().s[56]++;
  if (!validation.success) {
    /* istanbul ignore next */
    cov_m8r43dg9d().b[10][0]++;
    // Format field-level errors for better client consumption
    const fieldErrors =
    /* istanbul ignore next */
    (cov_m8r43dg9d().s[57]++, validation.error.issues.map(issue => {
      /* istanbul ignore next */
      cov_m8r43dg9d().f[24]++;
      cov_m8r43dg9d().s[58]++;
      return {
        field: issue.path.join('.'),
        message: issue.message,
        code: issue.code
      };
    }));
    /* istanbul ignore next */
    cov_m8r43dg9d().s[59]++;
    return {
      success: false,
      error: {
        code: 'VALIDATION_ERROR',
        message: 'Invalid request data',
        details: {
          fields: fieldErrors,
          raw: validation.error.issues
        }
      }
    };
  } else
  /* istanbul ignore next */
  {
    cov_m8r43dg9d().b[10][1]++;
  }
  cov_m8r43dg9d().s[60]++;
  return {
    success: true,
    data: validation.data
  };
}
function handleApiError(error) {
  /* istanbul ignore next */
  cov_m8r43dg9d().f[25]++;
  cov_m8r43dg9d().s[61]++;
  console.error('API Error:', error);
  /* istanbul ignore next */
  cov_m8r43dg9d().s[62]++;
  if (error instanceof Error) {
    /* istanbul ignore next */
    cov_m8r43dg9d().b[11][0]++;
    cov_m8r43dg9d().s[63]++;
    // Check for specific error types
    if (error.message.includes('not found')) {
      /* istanbul ignore next */
      cov_m8r43dg9d().b[12][0]++;
      cov_m8r43dg9d().s[64]++;
      return errorResponse('NOT_FOUND', error.message, 404);
    } else
    /* istanbul ignore next */
    {
      cov_m8r43dg9d().b[12][1]++;
    }
    cov_m8r43dg9d().s[65]++;
    if (
    /* istanbul ignore next */
    (cov_m8r43dg9d().b[14][0]++, error.message.includes('duplicate')) ||
    /* istanbul ignore next */
    (cov_m8r43dg9d().b[14][1]++, error.message.includes('conflict'))) {
      /* istanbul ignore next */
      cov_m8r43dg9d().b[13][0]++;
      cov_m8r43dg9d().s[66]++;
      return errorResponse('CONFLICT', error.message, 409);
    } else
    /* istanbul ignore next */
    {
      cov_m8r43dg9d().b[13][1]++;
    }
    cov_m8r43dg9d().s[67]++;
    if (
    /* istanbul ignore next */
    (cov_m8r43dg9d().b[16][0]++, error.message.includes('unauthorized')) ||
    /* istanbul ignore next */
    (cov_m8r43dg9d().b[16][1]++, error.message.includes('permission'))) {
      /* istanbul ignore next */
      cov_m8r43dg9d().b[15][0]++;
      cov_m8r43dg9d().s[68]++;
      return errorResponse('FORBIDDEN', error.message, 403);
    } else
    /* istanbul ignore next */
    {
      cov_m8r43dg9d().b[15][1]++;
    }
    cov_m8r43dg9d().s[69]++;
    return errorResponse('INTERNAL_ERROR', error.message, 500);
  } else
  /* istanbul ignore next */
  {
    cov_m8r43dg9d().b[11][1]++;
  }
  cov_m8r43dg9d().s[70]++;
  return errorResponse('UNKNOWN_ERROR', 'An unexpected error occurred', 500);
}
async function withAuth(handler) {
  /* istanbul ignore next */
  cov_m8r43dg9d().f[26]++;
  cov_m8r43dg9d().s[71]++;
  try {
    const auth =
    /* istanbul ignore next */
    (cov_m8r43dg9d().s[72]++, await verifyAuth());
    /* istanbul ignore next */
    cov_m8r43dg9d().s[73]++;
    if (
    /* istanbul ignore next */
    (cov_m8r43dg9d().b[18][0]++, !auth.success) ||
    /* istanbul ignore next */
    (cov_m8r43dg9d().b[18][1]++, !auth.userId)) {
      /* istanbul ignore next */
      cov_m8r43dg9d().b[17][0]++;
      cov_m8r43dg9d().s[74]++;
      return errorResponse(
      /* istanbul ignore next */
      (cov_m8r43dg9d().b[19][0]++, auth.error?.code) ||
      /* istanbul ignore next */
      (cov_m8r43dg9d().b[19][1]++, 'UNAUTHORIZED'),
      /* istanbul ignore next */
      (cov_m8r43dg9d().b[20][0]++, auth.error?.message) ||
      /* istanbul ignore next */
      (cov_m8r43dg9d().b[20][1]++, 'Authentication required'), 401);
    } else
    /* istanbul ignore next */
    {
      cov_m8r43dg9d().b[17][1]++;
    }
    cov_m8r43dg9d().s[75]++;
    return await handler(auth.userId);
  } catch (error) {
    /* istanbul ignore next */
    cov_m8r43dg9d().s[76]++;
    return handleApiError(error);
  }
}
