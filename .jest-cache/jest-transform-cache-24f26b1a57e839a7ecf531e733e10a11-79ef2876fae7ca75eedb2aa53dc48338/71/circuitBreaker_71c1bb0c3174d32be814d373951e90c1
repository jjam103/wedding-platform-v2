      cov_2owwd350t1().s[50]++;
      if (this.state === 'closed') {
        /* istanbul ignore next */
        cov_2owwd350t1().b[8][0]++;
        cov_2owwd350t1().s[51]++;
        // Check if failure threshold reached
        if (this.failures >= this.config.failureThreshold) {
          /* istanbul ignore next */
          cov_2owwd350t1().b[9][0]++;
          cov_2owwd350t1().s[52]++;
          this.state = 'open';
          /* istanbul ignore next */
          cov_2owwd350t1().s[53]++;
          this.nextAttemptTime = Date.now() + this.config.timeout;
        } else
        /* istanbul ignore next */
        {
          cov_2owwd350t1().b[9][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_2owwd350t1().b[8][1]++;
      }
    }
  }
}
/**
 * Global circuit breakers for external services.
 */
const circuitBreakers =
/* istanbul ignore next */
(cov_2owwd350t1().s[54]++, new Map());
function getCircuitBreaker(serviceName, config) {
  /* istanbul ignore next */
  cov_2owwd350t1().f[11]++;
  cov_2owwd350t1().s[55]++;
  if (!circuitBreakers.has(serviceName)) {
    /* istanbul ignore next */
    cov_2owwd350t1().b[10][0]++;
    const defaultConfig =
    /* istanbul ignore next */
    (cov_2owwd350t1().s[56]++, {
      failureThreshold: 5,
      successThreshold: 2,
      timeout: 60000,
      resetTimeout: 300000
    });
    /* istanbul ignore next */
    cov_2owwd350t1().s[57]++;
    circuitBreakers.set(serviceName, new CircuitBreaker({
      ...defaultConfig,
      ...config
    }));
  } else
  /* istanbul ignore next */
  {
    cov_2owwd350t1().b[10][1]++;
  }
  cov_2owwd350t1().s[58]++;
  return circuitBreakers.get(serviceName);
}
function resetAllCircuitBreakers() {
  /* istanbul ignore next */
  cov_2owwd350t1().f[12]++;
  cov_2owwd350t1().s[59]++;
  circuitBreakers.forEach(breaker => {
    /* istanbul ignore next */
    cov_2owwd350t1().f[13]++;
    cov_2owwd350t1().s[60]++;
    return breaker.reset();
  });
}
function getAllCircuitBreakerStats() {
  /* istanbul ignore next */
  cov_2owwd350t1().f[14]++;
