    /* istanbul ignore next */
    {
      cov_2bthblz3wu().b[0][1]++;
    }
    // Check if the window has expired
    cov_2bthblz3wu().s[14]++;
    if (Date.now() > entry.resetAt) {
      /* istanbul ignore next */
      cov_2bthblz3wu().b[1][0]++;
      cov_2bthblz3wu().s[15]++;
      this.store.delete(key);
      /* istanbul ignore next */
      cov_2bthblz3wu().s[16]++;
      return null;
    } else
    /* istanbul ignore next */
    {
      cov_2bthblz3wu().b[1][1]++;
