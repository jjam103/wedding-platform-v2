function checkRateLimit(identifier, action, config) {
  /* istanbul ignore next */
  cov_2bthblz3wu().f[13]++;
  const key =
  /* istanbul ignore next */
  (cov_2bthblz3wu().s[36]++, `${identifier}:${action}`);
  const entry =
  /* istanbul ignore next */
  (cov_2bthblz3wu().s[37]++, store.increment(key, config.windowMs));
  const allowed =
  /* istanbul ignore next */
  (cov_2bthblz3wu().s[38]++, entry.count <= config.maxRequests);
  const remaining =
  /* istanbul ignore next */
  (cov_2bthblz3wu().s[39]++, Math.max(0, config.maxRequests - entry.count));
  const reset =
  /* istanbul ignore next */
  (cov_2bthblz3wu().s[40]++, Math.floor(entry.resetAt / 1000)); // Convert to seconds
  const retryAfter =
  /* istanbul ignore next */
  (cov_2bthblz3wu().s[41]++, allowed ?
  /* istanbul ignore next */
  (cov_2bthblz3wu().b[5][0]++, undefined) :
  /* istanbul ignore next */
  (cov_2bthblz3wu().b[5][1]++, Math.ceil((entry.resetAt - Date.now()) / 1000)));
  /* istanbul ignore next */
  cov_2bthblz3wu().s[42]++;
  return {
    allowed,
    limit: config.maxRequests,
    remaining,
    reset,
    retryAfter
  };
}
