                password: _zod.z.string().min(8, 'Too short')
            });
            (0, _react.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_DynamicForm.DynamicForm, {
                fields: fields,
                schema: schema,
                onSubmit: mockOnSubmit
            }));
            const input = _react.screen.getByLabelText('Password');
            await _userevent.default.type(input, 'short');
            _react.fireEvent.blur(input);
            await (0, _react.waitFor)(()=>{
                expect(input).toHaveAttribute('aria-describedby', 'password-error');
                expect(_react.screen.getByRole('alert')).toHaveAttribute('id', 'password-error');
            });
        });
        it('should associate help text with input using aria-describedby', ()=>{
            const fields = [
                {
                    name: 'username',
                    label: 'Username',
                    type: 'text',
                    helpText: 'Choose wisely'
                }
            ];
            const schema = _zod.z.object({
                username: _zod.z.string()
            });
            (0, _react.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_DynamicForm.DynamicForm, {
                fields: fields,
                schema: schema,
                onSubmit: mockOnSubmit
            }));
            const input = _react.screen.getByLabelText('Username');
            expect(input).toHaveAttribute('aria-describedby', 'username-help');
        });
    });
});

