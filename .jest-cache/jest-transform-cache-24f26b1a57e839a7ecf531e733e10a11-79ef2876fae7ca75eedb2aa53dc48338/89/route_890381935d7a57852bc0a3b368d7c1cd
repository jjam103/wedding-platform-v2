  /* istanbul ignore next */
  (cov_2qk7gcay12().b[19][1]++, !session)) {
    /* istanbul ignore next */
    cov_2qk7gcay12().b[18][0]++;
    cov_2qk7gcay12().s[43]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: 'UNAUTHORIZED',
        message: 'Authentication required'
      }
    }, {
      status: 401
    });
  } else
  /* istanbul ignore next */
  {
    cov_2qk7gcay12().b[18][1]++;
  }
  // 2. Parse and validate
  const body =
  /* istanbul ignore next */
  (cov_2qk7gcay12().s[44]++, await request.json());
  // 3. Update each setting individually
  const updates =
  /* istanbul ignore next */
  (cov_2qk7gcay12().s[45]++, []);
  const errors =
  /* istanbul ignore next */
  (cov_2qk7gcay12().s[46]++, []);
  /* istanbul ignore next */
  cov_2qk7gcay12().s[47]++;
  for (const [key, value] of Object.entries(body)) {
    const result =
    /* istanbul ignore next */
    (cov_2qk7gcay12().s[48]++, await _settingsService.updateSetting(key, value));
    /* istanbul ignore next */
    cov_2qk7gcay12().s[49]++;
    if (!result.success) {
      /* istanbul ignore next */
      cov_2qk7gcay12().b[20][0]++;
      cov_2qk7gcay12().s[50]++;
      // If setting doesn't exist, try to create it
      if (result.error.code === 'NOT_FOUND') {
        /* istanbul ignore next */
        cov_2qk7gcay12().b[21][0]++;
        const createResult =
        /* istanbul ignore next */
        (cov_2qk7gcay12().s[51]++, await _settingsService.createSetting(key, value));
        /* istanbul ignore next */
        cov_2qk7gcay12().s[52]++;
        if (!createResult.success) {
          /* istanbul ignore next */
          cov_2qk7gcay12().b[22][0]++;
          cov_2qk7gcay12().s[53]++;
          errors.push({
            key,
            error: createResult.error
          });
        } else {
          /* istanbul ignore next */
          cov_2qk7gcay12().b[22][1]++;
          cov_2qk7gcay12().s[54]++;
          updates.push({
            key,
            value
          });
        }
      } else {
        /* istanbul ignore next */
        cov_2qk7gcay12().b[21][1]++;
        cov_2qk7gcay12().s[55]++;
        errors.push({
          key,
          error: result.error
        });
      }
    } else {
      /* istanbul ignore next */
      cov_2qk7gcay12().b[20][1]++;
      cov_2qk7gcay12().s[56]++;
      updates.push({
        key,
        value
      });
    }
  }
  // 4. Return response
  /* istanbul ignore next */
  cov_2qk7gcay12().s[57]++;
  if (errors.length > 0) {
    /* istanbul ignore next */
    cov_2qk7gcay12().b[23][0]++;
    cov_2qk7gcay12().s[58]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: 'PARTIAL_UPDATE_FAILURE',
        message: `Failed to update ${errors.length} setting(s)`,
        details: errors
      }
    }, {
      status: 500
    });
  } else
  /* istanbul ignore next */
  {
    cov_2qk7gcay12().b[23][1]++;
  }
  cov_2qk7gcay12().s[59]++;
  return _server.NextResponse.json({
    success: true,
    data: updates
  }, {
    status: 200
  });
}
