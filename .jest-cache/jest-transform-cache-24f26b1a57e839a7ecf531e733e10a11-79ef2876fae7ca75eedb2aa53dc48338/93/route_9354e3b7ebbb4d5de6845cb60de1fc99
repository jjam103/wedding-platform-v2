        success: false,
        error: {
          code: 'UNAUTHORIZED',
          message: 'Authentication required'
        }
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_3wvuk863v().b[4][1]++;
    }
    // Get current guest
    const {
      data: guest,
      error: guestError
    } =
    /* istanbul ignore next */
    (cov_3wvuk863v().s[29]++, await supabase.from('guests').select('*').eq('email', session.user.email).single());
    /* istanbul ignore next */
    cov_3wvuk863v().s[30]++;
    if (
    /* istanbul ignore next */
    (cov_3wvuk863v().b[7][0]++, guestError) ||
    /* istanbul ignore next */
    (cov_3wvuk863v().b[7][1]++, !guest)) {
      /* istanbul ignore next */
      cov_3wvuk863v().b[6][0]++;
      cov_3wvuk863v().s[31]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'NOT_FOUND',
          message: 'Guest profile not found'
        }
      }, {
        status: 404
      });
    } else
    /* istanbul ignore next */
    {
      cov_3wvuk863v().b[6][1]++;
    }
    // Parse and validate request body
    const body =
    /* istanbul ignore next */
    (cov_3wvuk863v().s[32]++, await request.json());
    const validation =
    /* istanbul ignore next */
    (cov_3wvuk863v().s[33]++, updateProfileSchema.safeParse(body));
    /* istanbul ignore next */
    cov_3wvuk863v().s[34]++;
    if (!validation.success) {
      /* istanbul ignore next */
      cov_3wvuk863v().b[8][0]++;
      cov_3wvuk863v().s[35]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'VALIDATION_ERROR',
          message: 'Invalid request data',
          details: validation.error.issues
        }
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_3wvuk863v().b[8][1]++;
    }
    // Sanitize inputs
    const sanitized =
    /* istanbul ignore next */
    (cov_3wvuk863v().s[36]++, {});
    /* istanbul ignore next */
    cov_3wvuk863v().s[37]++;
    if (validation.data.first_name) {
      /* istanbul ignore next */
      cov_3wvuk863v().b[9][0]++;
      cov_3wvuk863v().s[38]++;
      sanitized.first_name = (0, _sanitization.sanitizeInput)(validation.data.first_name);
    } else
    /* istanbul ignore next */
    {
      cov_3wvuk863v().b[9][1]++;
    }
    cov_3wvuk863v().s[39]++;
    if (validation.data.last_name) {
      /* istanbul ignore next */
      cov_3wvuk863v().b[10][0]++;
      cov_3wvuk863v().s[40]++;
      sanitized.last_name = (0, _sanitization.sanitizeInput)(validation.data.last_name);
    } else
    /* istanbul ignore next */
    {
      cov_3wvuk863v().b[10][1]++;
    }
    cov_3wvuk863v().s[41]++;
    if (validation.data.email) {
      /* istanbul ignore next */
      cov_3wvuk863v().b[11][0]++;
      cov_3wvuk863v().s[42]++;
      sanitized.email = (0, _sanitization.sanitizeInput)(validation.data.email);
    } else
    /* istanbul ignore next */
    {
      cov_3wvuk863v().b[11][1]++;
    }
    cov_3wvuk863v().s[43]++;
    if (validation.data.phone) {
      /* istanbul ignore next */
      cov_3wvuk863v().b[12][0]++;
      cov_3wvuk863v().s[44]++;
      sanitized.phone = (0, _sanitization.sanitizeInput)(validation.data.phone);
    } else
    /* istanbul ignore next */
    {
      cov_3wvuk863v().b[12][1]++;
    }
    cov_3wvuk863v().s[45]++;
    if (validation.data.dietary_restrictions !== undefined) {
      /* istanbul ignore next */
      cov_3wvuk863v().b[13][0]++;
      cov_3wvuk863v().s[46]++;
      sanitized.dietary_restrictions = validation.data.dietary_restrictions ?
      /* istanbul ignore next */
      (cov_3wvuk863v().b[14][0]++, (0, _sanitization.sanitizeInput)(validation.data.dietary_restrictions)) :
      /* istanbul ignore next */
      (cov_3wvuk863v().b[14][1]++, null);
    } else
    /* istanbul ignore next */
    {
      cov_3wvuk863v().b[13][1]++;
    }
    // Update profile
    const {
      data: updatedGuest,
      error: updateError
    } =
    /* istanbul ignore next */
    (cov_3wvuk863v().s[47]++, await supabase.from('guests').update(sanitized).eq('id', guest.id).select().single());
    /* istanbul ignore next */
    cov_3wvuk863v().s[48]++;
    if (updateError) {
      /* istanbul ignore next */
      cov_3wvuk863v().b[15][0]++;
      cov_3wvuk863v().s[49]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'DATABASE_ERROR',
          message: updateError.message
        }
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_3wvuk863v().b[15][1]++;
    }
    // Send admin notification for critical updates
    const criticalFields =
    /* istanbul ignore next */
    (cov_3wvuk863v().s[50]++, ['email', 'dietary_restrictions']);
    const hasCriticalUpdate =
    /* istanbul ignore next */
    (cov_3wvuk863v().s[51]++, Object.keys(sanitized).some(key => {
      /* istanbul ignore next */
      cov_3wvuk863v().f[5]++;
      cov_3wvuk863v().s[52]++;
      return criticalFields.includes(key);
    }));
    /* istanbul ignore next */
    cov_3wvuk863v().s[53]++;
    if (hasCriticalUpdate) {
      /* istanbul ignore next */
      cov_3wvuk863v().b[16][0]++;
      cov_3wvuk863v().s[54]++;
      try {
        /* istanbul ignore next */
        cov_3wvuk863v().s[55]++;
        await (0, _emailService.sendEmail)({
          to:
          /* istanbul ignore next */
          (cov_3wvuk863v().b[17][0]++, process.env.ADMIN_EMAIL) ||
          /* istanbul ignore next */
          (cov_3wvuk863v().b[17][1]++, 'admin@example.com'),
          subject: 'Guest Profile Update',
          html: `
            <h2>Guest Profile Updated</h2>
            <p><strong>Guest:</strong> ${updatedGuest.first_name} ${updatedGuest.last_name}</p>
            <p><strong>Updated Fields:</strong></p>
            <ul>
              ${Object.keys(sanitized).map(key => {
            /* istanbul ignore next */
            cov_3wvuk863v().f[6]++;
            cov_3wvuk863v().s[56]++;
            return `<li>${key}: ${sanitized[key]}</li>`;
          }).join('')}
            </ul>
          `
        });
      } catch (emailError) {
        /* istanbul ignore next */
        cov_3wvuk863v().s[57]++;
        console.error('Failed to send admin notification:', emailError);
        // Don't fail the request if email fails
      }
    } else
    /* istanbul ignore next */
    {
      cov_3wvuk863v().b[16][1]++;
    }
    cov_3wvuk863v().s[58]++;
    return _server.NextResponse.json({
      success: true,
      data: updatedGuest
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_3wvuk863v().s[59]++;
    console.error('Error updating guest profile:', error);
    /* istanbul ignore next */
    cov_3wvuk863v().s[60]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: 'INTERNAL_ERROR',
        message: 'Failed to update profile'
      }
    }, {
      status: 500
    });
  }
}
