    if (
    /* istanbul ignore next */
    (cov_2g1ff2h6r().b[20][0]++, authError) ||
    /* istanbul ignore next */
    (cov_2g1ff2h6r().b[20][1]++, !authUser)) {
      /* istanbul ignore next */
      cov_2g1ff2h6r().b[19][0]++;
      cov_2g1ff2h6r().s[45]++;
      console.error('User not found in auth.users:', authError);
      // Try again after another delay
      /* istanbul ignore next */
      cov_2g1ff2h6r().s[46]++;
      await new Promise(resolve => {
        /* istanbul ignore next */
        cov_2g1ff2h6r().f[7]++;
        cov_2g1ff2h6r().s[47]++;
        return setTimeout(resolve, 2000);
      });
      const {
        data: retryAuthUser,
        error: retryAuthError
      } =
      /* istanbul ignore next */
      (cov_2g1ff2h6r().s[48]++, await supabaseAdmin.auth.admin.getUserById(userId));
      /* istanbul ignore next */
      cov_2g1ff2h6r().s[49]++;
      if (
      /* istanbul ignore next */
      (cov_2g1ff2h6r().b[22][0]++, retryAuthError) ||
      /* istanbul ignore next */
      (cov_2g1ff2h6r().b[22][1]++, !retryAuthUser)) {
        /* istanbul ignore next */
        cov_2g1ff2h6r().b[21][0]++;
        cov_2g1ff2h6r().s[50]++;
        return _server.NextResponse.json({
          error: 'User not found in authentication system'
        }, {
          status: 404
        });
      } else
      /* istanbul ignore next */
      {
        cov_2g1ff2h6r().b[21][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_2g1ff2h6r().b[19][1]++;
    }
    // Insert user record
    const {
      error: insertError
    } =
    /* istanbul ignore next */
    (cov_2g1ff2h6r().s[51]++, await supabaseAdmin.from('users').insert({
      id: userId,
      email: email,
      role: role
    }));
    /* istanbul ignore next */
    cov_2g1ff2h6r().s[52]++;
    if (insertError) {
      /* istanbul ignore next */
      cov_2g1ff2h6r().b[23][0]++;
      cov_2g1ff2h6r().s[53]++;
      console.error('Error inserting user:', insertError);
      /* istanbul ignore next */
      cov_2g1ff2h6r().s[54]++;
      return _server.NextResponse.json({
        error: 'Failed to create user record',
        details: insertError.message
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_2g1ff2h6r().b[23][1]++;
    }
    cov_2g1ff2h6r().s[55]++;
    return _server.NextResponse.json({
      success: true
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_2g1ff2h6r().s[56]++;
    console.error('Unexpected error:', error);
    /* istanbul ignore next */
    cov_2g1ff2h6r().s[57]++;
    return _server.NextResponse.json({
      error: 'Internal server error'
    }, {
      status: 500
    });
  }
}
