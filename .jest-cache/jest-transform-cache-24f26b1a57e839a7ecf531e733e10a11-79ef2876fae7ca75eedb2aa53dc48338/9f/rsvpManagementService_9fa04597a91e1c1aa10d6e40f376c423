    if (error) {
      /* istanbul ignore next */
      cov_zail9kjj8().b[9][0]++;
      cov_zail9kjj8().s[44]++;
      return {
        success: false,
        error: {
          code: _types.ERROR_CODES.DATABASE_ERROR,
          message: error.message,
          details: error
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_zail9kjj8().b[9][1]++;
    }
    // 4. Transform to view model
    const viewModels =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[45]++, (
    /* istanbul ignore next */
    (cov_zail9kjj8().b[10][0]++, data) ||
    /* istanbul ignore next */
    (cov_zail9kjj8().b[10][1]++, [])).map(rsvp => {
      /* istanbul ignore next */
      cov_zail9kjj8().f[6]++;
      cov_zail9kjj8().s[46]++;
      return {
        id: rsvp.id,
        guestId: rsvp.guest_id,
        guestFirstName:
        /* istanbul ignore next */
        (cov_zail9kjj8().b[11][0]++, rsvp.guests?.first_name) ||
        /* istanbul ignore next */
        (cov_zail9kjj8().b[11][1]++, ''),
        guestLastName:
        /* istanbul ignore next */
        (cov_zail9kjj8().b[12][0]++, rsvp.guests?.last_name) ||
        /* istanbul ignore next */
        (cov_zail9kjj8().b[12][1]++, ''),
        guestEmail:
        /* istanbul ignore next */
        (cov_zail9kjj8().b[13][0]++, rsvp.guests?.email) ||
        /* istanbul ignore next */
        (cov_zail9kjj8().b[13][1]++, null),
        eventId: rsvp.event_id,
        eventName:
        /* istanbul ignore next */
        (cov_zail9kjj8().b[14][0]++, rsvp.events?.name) ||
        /* istanbul ignore next */
        (cov_zail9kjj8().b[14][1]++, null),
        activityId: rsvp.activity_id,
        activityName:
        /* istanbul ignore next */
        (cov_zail9kjj8().b[15][0]++, rsvp.activities?.name) ||
        /* istanbul ignore next */
        (cov_zail9kjj8().b[15][1]++, null),
        status: rsvp.status,
        guestCount: rsvp.guest_count,
        dietaryNotes: rsvp.dietary_notes,
        specialRequirements: rsvp.special_requirements,
        notes: rsvp.notes,
        respondedAt: rsvp.responded_at,
        createdAt: rsvp.created_at,
        updatedAt: rsvp.updated_at
      };
    }));
    // 5. Calculate statistics
    const statisticsResult =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[47]++, await getRSVPStatistics(validFilters));
    const statistics =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[48]++, statisticsResult.success ?
    /* istanbul ignore next */
    (cov_zail9kjj8().b[16][0]++, statisticsResult.data) :
    /* istanbul ignore next */
    (cov_zail9kjj8().b[16][1]++, {
      totalRSVPs:
      /* istanbul ignore next */
      (cov_zail9kjj8().b[17][0]++, count) ||
      /* istanbul ignore next */
      (cov_zail9kjj8().b[17][1]++, 0),
      byStatus: {
        attending: 0,
        declined: 0,
        maybe: 0,
        pending: 0
      },
      totalGuestCount: 0
    }));
    // 6. Build response
    const totalPages =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[49]++, Math.ceil((
    /* istanbul ignore next */
    (cov_zail9kjj8().b[18][0]++, count) ||
    /* istanbul ignore next */
    (cov_zail9kjj8().b[18][1]++, 0)) / validPagination.limit));
    /* istanbul ignore next */
    cov_zail9kjj8().s[50]++;
    return {
      success: true,
      data: {
        data: viewModels,
        pagination: {
          page: validPagination.page,
          limit: validPagination.limit,
          total:
          /* istanbul ignore next */
          (cov_zail9kjj8().b[19][0]++, count) ||
          /* istanbul ignore next */
          (cov_zail9kjj8().b[19][1]++, 0),
          totalPages
        },
        statistics
      }
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_zail9kjj8().s[51]++;
    return {
      success: false,
      error: {
        code: _types.ERROR_CODES.UNKNOWN_ERROR,
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_zail9kjj8().b[20][0]++, error.message) :
        /* istanbul ignore next */
        (cov_zail9kjj8().b[20][1]++, 'Unknown error')
      }
    };
  }
}
async function getRSVPStatistics(filters =
/* istanbul ignore next */
(cov_zail9kjj8().b[21][0]++, {})) {
  /* istanbul ignore next */
  cov_zail9kjj8().f[7]++;
  cov_zail9kjj8().s[52]++;
  try {
    // 1. Validate filters
    const filterValidation =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[53]++, rsvpFiltersSchema.safeParse(filters));
    /* istanbul ignore next */
    cov_zail9kjj8().s[54]++;
    if (!filterValidation.success) {
      /* istanbul ignore next */
      cov_zail9kjj8().b[22][0]++;
      cov_zail9kjj8().s[55]++;
      return {
        success: false,
        error: {
          code: _types.ERROR_CODES.VALIDATION_ERROR,
          message: 'Invalid filter parameters',
          details: filterValidation.error.issues
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_zail9kjj8().b[22][1]++;
    }
    const validFilters =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[56]++, filterValidation.data);
    // 2. Build query
    let query =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[57]++, supabase.from('rsvps').select('status, guest_count'));
    // Apply filters
    /* istanbul ignore next */
    cov_zail9kjj8().s[58]++;
    if (validFilters.eventId) {
      /* istanbul ignore next */
      cov_zail9kjj8().b[23][0]++;
      cov_zail9kjj8().s[59]++;
      query = query.eq('event_id', validFilters.eventId);
    } else
    /* istanbul ignore next */
    {
      cov_zail9kjj8().b[23][1]++;
    }
    cov_zail9kjj8().s[60]++;
    if (validFilters.activityId) {
      /* istanbul ignore next */
      cov_zail9kjj8().b[24][0]++;
      cov_zail9kjj8().s[61]++;
      query = query.eq('activity_id', validFilters.activityId);
    } else
    /* istanbul ignore next */
    {
      cov_zail9kjj8().b[24][1]++;
    }
    cov_zail9kjj8().s[62]++;
    if (validFilters.status) {
      /* istanbul ignore next */
      cov_zail9kjj8().b[25][0]++;
      cov_zail9kjj8().s[63]++;
      query = query.eq('status', validFilters.status);
    } else
    /* istanbul ignore next */
    {
      cov_zail9kjj8().b[25][1]++;
    }
    cov_zail9kjj8().s[64]++;
    if (validFilters.guestId) {
      /* istanbul ignore next */
      cov_zail9kjj8().b[26][0]++;
      cov_zail9kjj8().s[65]++;
      query = query.eq('guest_id', validFilters.guestId);
    } else
    /* istanbul ignore next */
    {
      cov_zail9kjj8().b[26][1]++;
    }
    // Execute query
    const {
      data,
      error
    } =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[66]++, await query);
    /* istanbul ignore next */
    cov_zail9kjj8().s[67]++;
    if (error) {
      /* istanbul ignore next */
      cov_zail9kjj8().b[27][0]++;
      cov_zail9kjj8().s[68]++;
      return {
        success: false,
        error: {
          code: _types.ERROR_CODES.DATABASE_ERROR,
          message: error.message,
          details: error
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_zail9kjj8().b[27][1]++;
    }
    // 3. Calculate statistics
    const statistics =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[69]++, {
      totalRSVPs:
      /* istanbul ignore next */
      (cov_zail9kjj8().b[28][0]++, data?.length) ||
      /* istanbul ignore next */
      (cov_zail9kjj8().b[28][1]++, 0),
      byStatus: {
        attending: 0,
        declined: 0,
        maybe: 0,
        pending: 0
      },
      totalGuestCount: 0
    });
    /* istanbul ignore next */
    cov_zail9kjj8().s[70]++;
    if (data) {
      /* istanbul ignore next */
      cov_zail9kjj8().b[29][0]++;
      cov_zail9kjj8().s[71]++;
      for (const rsvp of data) {
        /* istanbul ignore next */
        cov_zail9kjj8().s[72]++;
        // Count by status
        if (rsvp.status in statistics.byStatus) {
          /* istanbul ignore next */
          cov_zail9kjj8().b[30][0]++;
          cov_zail9kjj8().s[73]++;
          statistics.byStatus[rsvp.status]++;
        } else
        /* istanbul ignore next */
        {
          cov_zail9kjj8().b[30][1]++;
        }
        // Sum guest counts (default to 1 if not specified)
        cov_zail9kjj8().s[74]++;
        if (rsvp.status === 'attending') {
          /* istanbul ignore next */
          cov_zail9kjj8().b[31][0]++;
          cov_zail9kjj8().s[75]++;
          statistics.totalGuestCount +=
          /* istanbul ignore next */
          (cov_zail9kjj8().b[32][0]++, rsvp.guest_count) ||
          /* istanbul ignore next */
          (cov_zail9kjj8().b[32][1]++, 1);
        } else
        /* istanbul ignore next */
        {
          cov_zail9kjj8().b[31][1]++;
        }
      }
    } else
    /* istanbul ignore next */
    {
      cov_zail9kjj8().b[29][1]++;
    }
    cov_zail9kjj8().s[76]++;
    return {
      success: true,
      data: statistics
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_zail9kjj8().s[77]++;
    return {
      success: false,
      error: {
        code: _types.ERROR_CODES.UNKNOWN_ERROR,
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_zail9kjj8().b[33][0]++, error.message) :
        /* istanbul ignore next */
        (cov_zail9kjj8().b[33][1]++, 'Unknown error')
      }
    };
  }
}
async function bulkUpdateRSVPs(rsvpIds, status, notes) {
  /* istanbul ignore next */
  cov_zail9kjj8().f[8]++;
  cov_zail9kjj8().s[78]++;
  try {
    // 1. Validate input
    const validation =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[79]++, bulkUpdateSchema.safeParse({
      rsvpIds,
      status,
      notes
    }));
    /* istanbul ignore next */
    cov_zail9kjj8().s[80]++;
    if (!validation.success) {
      /* istanbul ignore next */
      cov_zail9kjj8().b[34][0]++;
      cov_zail9kjj8().s[81]++;
      return {
        success: false,
        error: {
          code: _types.ERROR_CODES.VALIDATION_ERROR,
          message: 'Invalid bulk update parameters',
          details: validation.error.issues
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_zail9kjj8().b[34][1]++;
    }
    const {
      rsvpIds: validIds,
      status: validStatus,
      notes: validNotes
    } =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[82]++, validation.data);
    // 2. Prepare update data
    const updateData =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[83]++, {
      status: validStatus,
      updated_at: new Date().toISOString()
    });
    // Set responded_at if status is not pending
    /* istanbul ignore next */
    cov_zail9kjj8().s[84]++;
    if (validStatus !== 'pending') {
      /* istanbul ignore next */
      cov_zail9kjj8().b[35][0]++;
      cov_zail9kjj8().s[85]++;
      updateData.responded_at = new Date().toISOString();
    } else
    /* istanbul ignore next */
    {
      cov_zail9kjj8().b[35][1]++;
    }
    // Add notes if provided
    cov_zail9kjj8().s[86]++;
    if (validNotes) {
      /* istanbul ignore next */
      cov_zail9kjj8().b[36][0]++;
      cov_zail9kjj8().s[87]++;
      updateData.notes = validNotes;
    } else
    /* istanbul ignore next */
    {
      cov_zail9kjj8().b[36][1]++;
    }
    // 3. Execute bulk update with transaction handling
    // Note: Supabase doesn't support explicit transactions in the client library,
    // but the update operation is atomic at the database level
    const {
      data,
      error
    } =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[88]++, await supabase.from('rsvps').update(updateData).in('id', validIds).select('id'));
    /* istanbul ignore next */
    cov_zail9kjj8().s[89]++;
    if (error) {
      /* istanbul ignore next */
      cov_zail9kjj8().b[37][0]++;
      cov_zail9kjj8().s[90]++;
      return {
        success: false,
        error: {
          code: _types.ERROR_CODES.DATABASE_ERROR,
          message: error.message,
          details: error
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_zail9kjj8().b[37][1]++;
    }
    // 4. Return count of updated records
    const updatedCount =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[91]++,
    /* istanbul ignore next */
    (cov_zail9kjj8().b[38][0]++, data?.length) ||
    /* istanbul ignore next */
    (cov_zail9kjj8().b[38][1]++, 0));
    // Log if not all RSVPs were updated (some might not exist)
    /* istanbul ignore next */
    cov_zail9kjj8().s[92]++;
    if (updatedCount < validIds.length) {
      /* istanbul ignore next */
      cov_zail9kjj8().b[39][0]++;
      cov_zail9kjj8().s[93]++;
      console.warn(`Bulk update: ${updatedCount} of ${validIds.length} RSVPs updated. ` + `${validIds.length - updatedCount} RSVPs not found.`);
    } else
    /* istanbul ignore next */
    {
      cov_zail9kjj8().b[39][1]++;
    }
    cov_zail9kjj8().s[94]++;
    return {
      success: true,
      data: {
        updatedCount
      }
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_zail9kjj8().s[95]++;
    return {
      success: false,
      error: {
        code: _types.ERROR_CODES.UNKNOWN_ERROR,
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_zail9kjj8().b[40][0]++, error.message) :
        /* istanbul ignore next */
        (cov_zail9kjj8().b[40][1]++, 'Unknown error')
      }
    };
  }
}
async function exportRSVPsToCSV(filters =
/* istanbul ignore next */
(cov_zail9kjj8().b[41][0]++, {})) {
  /* istanbul ignore next */
  cov_zail9kjj8().f[9]++;
  cov_zail9kjj8().s[96]++;
  try {
    // 1. Validate filters
    const filterValidation =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[97]++, rsvpFiltersSchema.safeParse(filters));
    /* istanbul ignore next */
    cov_zail9kjj8().s[98]++;
    if (!filterValidation.success) {
      /* istanbul ignore next */
      cov_zail9kjj8().b[42][0]++;
      cov_zail9kjj8().s[99]++;
      return {
        success: false,
        error: {
          code: _types.ERROR_CODES.VALIDATION_ERROR,
          message: 'Invalid filter parameters',
          details: filterValidation.error.issues
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_zail9kjj8().b[42][1]++;
    }
    const validFilters =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[100]++, filterValidation.data);
    // 2. Fetch all matching RSVPs (limit to 10,000 for performance)
    const result =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[101]++, await listRSVPs(validFilters, {
      page: 1,
      limit: 10000
    }));
    /* istanbul ignore next */
    cov_zail9kjj8().s[102]++;
    if (!result.success) {
      /* istanbul ignore next */
      cov_zail9kjj8().b[43][0]++;
      cov_zail9kjj8().s[103]++;
      return result;
    } else
    /* istanbul ignore next */
    {
      cov_zail9kjj8().b[43][1]++;
    }
    const rsvps =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[104]++, result.data.data);
    // 3. Build CSV header
    const headers =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[105]++, ['RSVP ID', 'Guest First Name', 'Guest Last Name', 'Guest Email', 'Event Name', 'Activity Name', 'Status', 'Guest Count', 'Dietary Notes', 'Special Requirements', 'Notes', 'Responded At', 'Created At']);
    // 4. Build CSV rows
    const rows =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[106]++, rsvps.map(rsvp => {
      /* istanbul ignore next */
      cov_zail9kjj8().f[10]++;
      cov_zail9kjj8().s[107]++;
      return [rsvp.id, rsvp.guestFirstName, rsvp.guestLastName,
      /* istanbul ignore next */
      (cov_zail9kjj8().b[44][0]++, rsvp.guestEmail) ||
      /* istanbul ignore next */
      (cov_zail9kjj8().b[44][1]++, ''),
      /* istanbul ignore next */
      (cov_zail9kjj8().b[45][0]++, rsvp.eventName) ||
      /* istanbul ignore next */
      (cov_zail9kjj8().b[45][1]++, ''),
      /* istanbul ignore next */
      (cov_zail9kjj8().b[46][0]++, rsvp.activityName) ||
      /* istanbul ignore next */
      (cov_zail9kjj8().b[46][1]++, ''), rsvp.status,
      /* istanbul ignore next */
      (cov_zail9kjj8().b[47][0]++, rsvp.guestCount?.toString()) ||
      /* istanbul ignore next */
      (cov_zail9kjj8().b[47][1]++, '1'),
      /* istanbul ignore next */
      (cov_zail9kjj8().b[48][0]++, rsvp.dietaryNotes) ||
      /* istanbul ignore next */
      (cov_zail9kjj8().b[48][1]++, ''),
      /* istanbul ignore next */
      (cov_zail9kjj8().b[49][0]++, rsvp.specialRequirements) ||
      /* istanbul ignore next */
      (cov_zail9kjj8().b[49][1]++, ''),
      /* istanbul ignore next */
      (cov_zail9kjj8().b[50][0]++, rsvp.notes) ||
      /* istanbul ignore next */
      (cov_zail9kjj8().b[50][1]++, ''),
      /* istanbul ignore next */
      (cov_zail9kjj8().b[51][0]++, rsvp.respondedAt) ||
      /* istanbul ignore next */
      (cov_zail9kjj8().b[51][1]++, ''), rsvp.createdAt];
    }));
    // 5. Format as CSV
    /* istanbul ignore next */
    cov_zail9kjj8().s[108]++;
    const escapeCsvValue = value => {
      /* istanbul ignore next */
      cov_zail9kjj8().f[11]++;
      cov_zail9kjj8().s[109]++;
      // Escape double quotes and wrap in quotes if contains comma, newline, or quote
      if (
      /* istanbul ignore next */
      (cov_zail9kjj8().b[53][0]++, value.includes(',')) ||
      /* istanbul ignore next */
      (cov_zail9kjj8().b[53][1]++, value.includes('\n')) ||
      /* istanbul ignore next */
      (cov_zail9kjj8().b[53][2]++, value.includes('"'))) {
        /* istanbul ignore next */
        cov_zail9kjj8().b[52][0]++;
        cov_zail9kjj8().s[110]++;
        return `"${value.replace(/"/g, '""')}"`;
      } else
      /* istanbul ignore next */
      {
        cov_zail9kjj8().b[52][1]++;
      }
      cov_zail9kjj8().s[111]++;
      return value;
    };
    const csvLines =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[112]++, [headers.map(escapeCsvValue).join(','), ...rows.map(row => {
      /* istanbul ignore next */
      cov_zail9kjj8().f[12]++;
      cov_zail9kjj8().s[113]++;
      return row.map(escapeCsvValue).join(',');
    })]);
    const csv =
    /* istanbul ignore next */
    (cov_zail9kjj8().s[114]++, csvLines.join('\n'));
    /* istanbul ignore next */
    cov_zail9kjj8().s[115]++;
    return {
      success: true,
      data: csv
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_zail9kjj8().s[116]++;
    return {
      success: false,
      error: {
        code: _types.ERROR_CODES.UNKNOWN_ERROR,
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_zail9kjj8().b[54][0]++, error.message) :
        /* istanbul ignore next */
        (cov_zail9kjj8().b[54][1]++, 'Unknown error')
      }
    };
  }
}
//# sourceMappingURL=data:application/json;charset=utf-8;base64,