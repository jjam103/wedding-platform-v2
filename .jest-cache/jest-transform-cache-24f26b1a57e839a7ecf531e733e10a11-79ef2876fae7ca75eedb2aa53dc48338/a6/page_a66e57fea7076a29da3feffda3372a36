        children: /*#__PURE__*/(0, _jsxruntime.jsx)(_DataTable.DataTable, {
          data: templates,
          columns: columns,
          loading: loading
        })
      })
    }),
    /* istanbul ignore next */
    (cov_2k0dd8kkzu().b[20][0]++, deleteConfirm) &&
    /* istanbul ignore next */
    (cov_2k0dd8kkzu().b[20][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50",
      children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "bg-white rounded-lg shadow-xl max-w-md w-full mx-4 p-6",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("h3", {
          className: "text-lg font-semibold text-gray-900 mb-2",
          children: "Delete Template"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
          className: "text-gray-600 mb-6",
          children: "Are you sure you want to delete this template? This action cannot be undone."
        }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "flex items-center justify-end gap-3",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)("button", {
            onClick: () => {
              /* istanbul ignore next */
              cov_2k0dd8kkzu().f[26]++;
              cov_2k0dd8kkzu().s[92]++;
              return setDeleteConfirm(null);
            },
            className: "px-4 py-2 text-gray-700 bg-white border border-gray-300 rounded-lg hover:bg-gray-50",
            children: "Cancel"
          }), /*#__PURE__*/(0, _jsxruntime.jsx)("button", {
            onClick: () => {
              /* istanbul ignore next */
              cov_2k0dd8kkzu().f[27]++;
              cov_2k0dd8kkzu().s[93]++;
              return handleDelete(deleteConfirm);
            },
            className: "px-4 py-2 text-white bg-red-600 rounded-lg hover:bg-red-700",
            children: "Delete"
          })]
        })]
      })
    }))]
  });
}
