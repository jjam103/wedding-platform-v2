      cov_1xtog1bwu7().b[2][1]++;
    }
    // 3. Query guests with departure information
    let query =
    /* istanbul ignore next */
    (cov_1xtog1bwu7().s[20]++, supabase.from('guests').select('id, first_name, last_name, departure_date, departure_time, airport_code, flight_number').eq('departure_date', date).not('airport_code', 'is', null));
    // Apply airport filter if provided
    /* istanbul ignore next */
    cov_1xtog1bwu7().s[21]++;
    if (
    /* istanbul ignore next */
    (cov_1xtog1bwu7().b[4][0]++, airport) &&
    /* istanbul ignore next */
    (cov_1xtog1bwu7().b[4][1]++, airport !== 'all')) {
      /* istanbul ignore next */
      cov_1xtog1bwu7().b[3][0]++;
      cov_1xtog1bwu7().s[22]++;
      query = query.eq('airport_code', airport);
    } else
    /* istanbul ignore next */
    {
      cov_1xtog1bwu7().b[3][1]++;
    }
    const {
      data: guests,
      error
    } =
    /* istanbul ignore next */
    (cov_1xtog1bwu7().s[23]++, await query);
    /* istanbul ignore next */
    cov_1xtog1bwu7().s[24]++;
    if (error) {
      /* istanbul ignore next */
      cov_1xtog1bwu7().b[5][0]++;
      cov_1xtog1bwu7().s[25]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'DATABASE_ERROR',
          message: error.message,
          details: error
        }
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_1xtog1bwu7().b[5][1]++;
    }
    cov_1xtog1bwu7().s[26]++;
    return _server.NextResponse.json({
      success: true,
      data: {
        guests:
        /* istanbul ignore next */
        (cov_1xtog1bwu7().b[6][0]++, guests) ||
        /* istanbul ignore next */
        (cov_1xtog1bwu7().b[6][1]++, [])
      }
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_1xtog1bwu7().s[27]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: 'UNKNOWN_ERROR',
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_1xtog1bwu7().b[7][0]++, error.message) :
        /* istanbul ignore next */
        (cov_1xtog1bwu7().b[7][1]++, 'Unknown error')
      }
    }, {
      status: 500
    });
  }
}
