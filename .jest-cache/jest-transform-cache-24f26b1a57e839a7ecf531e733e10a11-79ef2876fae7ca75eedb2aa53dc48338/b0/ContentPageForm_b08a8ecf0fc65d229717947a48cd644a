          ...prev
        });
        /* istanbul ignore next */
        cov_3cv8u3bcv().s[62]++;
        delete next.slug;
        /* istanbul ignore next */
        cov_3cv8u3bcv().s[63]++;
        return next;
      });
    } else
    /* istanbul ignore next */
    {
      cov_3cv8u3bcv().b[16][1]++;
    }
  }, [errors.slug]));
  /* istanbul ignore next */
  cov_3cv8u3bcv().s[64]++;
  const handleSubmit = async e => {
    /* istanbul ignore next */
    cov_3cv8u3bcv().f[13]++;
    cov_3cv8u3bcv().s[65]++;
    e.preventDefault();
    // Validate
    const validationErrors =
    /* istanbul ignore next */
    (cov_3cv8u3bcv().s[66]++, {});
    /* istanbul ignore next */
    cov_3cv8u3bcv().s[67]++;
    if (!formTitle.trim()) {
      /* istanbul ignore next */
      cov_3cv8u3bcv().b[17][0]++;
      cov_3cv8u3bcv().s[68]++;
      validationErrors.title = 'Title is required';
    } else
    /* istanbul ignore next */
    {
      cov_3cv8u3bcv().b[17][1]++;
    }
    cov_3cv8u3bcv().s[69]++;
