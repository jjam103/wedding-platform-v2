      /* istanbul ignore next */
      cov_3cv8u3bcv().s[78]++;
      await onSubmit({
        title: formTitle,
        slug: formSlug,
        status: formStatus
      });
      // Form will be reset by useEffect when isOpen changes
    } catch (error) {
      /* istanbul ignore next */
      cov_3cv8u3bcv().s[79]++;
      console.error('Form submission error:', error);
      /* istanbul ignore next */
      cov_3cv8u3bcv().s[80]++;
      setErrors({
        _form: error instanceof Error ?
        /* istanbul ignore next */
        (cov_3cv8u3bcv().b[21][0]++, error.message) :
        /* istanbul ignore next */
        (cov_3cv8u3bcv().b[21][1]++, 'Submission failed')
      });
    } finally {
      /* istanbul ignore next */
      cov_3cv8u3bcv().s[81]++;
      setIsSubmitting(false);
    }
  };
  /* istanbul ignore next */
  cov_3cv8u3bcv().s[82]++;
  const handleCancel = () => {
    /* istanbul ignore next */
    cov_3cv8u3bcv().f[14]++;
    cov_3cv8u3bcv().s[83]++;
    if (isDirty) {
      /* istanbul ignore next */
      cov_3cv8u3bcv().b[22][0]++;
      const confirmed =
      /* istanbul ignore next */
      (cov_3cv8u3bcv().s[84]++, window.confirm('You have unsaved changes. Are you sure you want to cancel?'));
      /* istanbul ignore next */
      cov_3cv8u3bcv().s[85]++;
      if (!confirmed) {
        /* istanbul ignore next */
        cov_3cv8u3bcv().b[23][0]++;
        cov_3cv8u3bcv().s[86]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_3cv8u3bcv().b[23][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_3cv8u3bcv().b[22][1]++;
    }
    cov_3cv8u3bcv().s[87]++;
    onToggle();
    /* istanbul ignore next */
    cov_3cv8u3bcv().s[88]++;
    if (onCancel) {
      /* istanbul ignore next */
      cov_3cv8u3bcv().b[24][0]++;
      cov_3cv8u3bcv().s[89]++;
      onCancel();
    } else
    /* istanbul ignore next */
    {
      cov_3cv8u3bcv().b[24][1]++;
    }
  };
  const slugPreview =
  /* istanbul ignore next */
  (cov_3cv8u3bcv().s[90]++, formSlug ?
  /* istanbul ignore next */
  (cov_3cv8u3bcv().b[25][0]++, (0, _slugs.getSlugPreview)(formSlug, 'pages')) :
  /* istanbul ignore next */
  (cov_3cv8u3bcv().b[25][1]++, ''));
  /* istanbul ignore next */
  cov_3cv8u3bcv().s[91]++;
  return /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
    ref: formRef,
    className: "border border-sage-200 rounded-lg overflow-hidden bg-white shadow-sm",
    children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("button", {
      onClick: onToggle,
      className: "w-full flex items-center justify-between px-4 py-3 bg-sage-50 hover:bg-sage-100 transition-colors",
