      "aria-controls": "content-page-form-content",
      children: [/*#__PURE__*/(0, _jsxruntime.jsx)("span", {
        className: "font-medium text-sage-900",
        children: title
      }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
        className: `text-sage-600 transition-transform duration-300 ${isOpen ?
        /* istanbul ignore next */
        (cov_3cv8u3bcv().b[26][0]++, 'rotate-180') :
        /* istanbul ignore next */
        (cov_3cv8u3bcv().b[26][1]++, '')}`,
        "aria-hidden": "true",
        children: ""
      })]
    }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      ref: contentRef,
      id: "content-page-form-content",
      className: `transition-all duration-300 ease-in-out ${isOpen ?
      /* istanbul ignore next */
      (cov_3cv8u3bcv().b[27][0]++, '') :
      /* istanbul ignore next */
      (cov_3cv8u3bcv().b[27][1]++, 'overflow-hidden')}`,
      style: {
        maxHeight: isOpen ?
        /* istanbul ignore next */
        (cov_3cv8u3bcv().b[28][0]++, 'none') :
        /* istanbul ignore next */
        (cov_3cv8u3bcv().b[28][1]++, '0px'),
        opacity: isOpen ?
        /* istanbul ignore next */
        (cov_3cv8u3bcv().b[29][0]++, 1) :
        /* istanbul ignore next */
        (cov_3cv8u3bcv().b[29][1]++, 0)
      },
      "aria-hidden": !isOpen,
      children: /*#__PURE__*/(0, _jsxruntime.jsxs)("form", {
        onSubmit: handleSubmit,
        className: "p-4 space-y-4",
        children: [
        /* istanbul ignore next */
        (cov_3cv8u3bcv().b[30][0]++, errors._form) &&
        /* istanbul ignore next */
        (cov_3cv8u3bcv().b[30][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "p-3 bg-red-50 border border-red-200 rounded-lg",
          role: "alert",
          children: /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
            className: "text-sm text-red-700",
            children: errors._form
          })
        })), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "grid grid-cols-1 md:grid-cols-2 gap-4",
          children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "space-y-1",
            children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("label", {
              htmlFor: "title",
              className: "block text-sm font-medium text-gray-700",
              children: ["Title", /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
                className: "text-red-500 ml-1",
                children: "*"
              })]
            }), /*#__PURE__*/(0, _jsxruntime.jsx)("input", {
              id: "title",
              type: "text",
              value: formTitle,
              onChange: e => {
                /* istanbul ignore next */
                cov_3cv8u3bcv().f[15]++;
                cov_3cv8u3bcv().s[92]++;
                return handleTitleChange(e.target.value);
              },
              placeholder: "Our Story",
              required: true,
              className: `w-full px-3 py-2 border rounded-lg transition-colors focus:outline-none focus:ring-2 focus:ring-green-500 ${errors.title ?
              /* istanbul ignore next */
              (cov_3cv8u3bcv().b[31][0]++, 'border-red-500') :
              /* istanbul ignore next */
              (cov_3cv8u3bcv().b[31][1]++, 'border-gray-300')}`,
              "aria-invalid": !!errors.title,
              "aria-describedby": errors.title ?
              /* istanbul ignore next */
              (cov_3cv8u3bcv().b[32][0]++, 'title-error') :
              /* istanbul ignore next */
              (cov_3cv8u3bcv().b[32][1]++, undefined)
            }),
            /* istanbul ignore next */
            (cov_3cv8u3bcv().b[33][0]++, errors.title) &&
            /* istanbul ignore next */
            (cov_3cv8u3bcv().b[33][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
              id: "title-error",
              className: "text-sm text-red-600",
              role: "alert",
              children: errors.title
            }))]
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "space-y-1",
            children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("label", {
              htmlFor: "status",
              className: "block text-sm font-medium text-gray-700",
              children: ["Status", /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
                className: "text-red-500 ml-1",
                children: "*"
              })]
            }), /*#__PURE__*/(0, _jsxruntime.jsxs)("select", {
              id: "status",
              value: formStatus,
              onChange: e => {
                /* istanbul ignore next */
                cov_3cv8u3bcv().f[16]++;
                cov_3cv8u3bcv().s[93]++;
                setFormStatus(e.target.value);
                /* istanbul ignore next */
                cov_3cv8u3bcv().s[94]++;
                setIsDirty(true);
              },
              required: true,
              className: "w-full px-3 py-2 border border-gray-300 rounded-lg transition-colors focus:outline-none focus:ring-2 focus:ring-green-500",
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)("option", {
                value: "draft",
                children: "Draft"
              }), /*#__PURE__*/(0, _jsxruntime.jsx)("option", {
                value: "published",
                children: "Published"
              })]
            })]
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "md:col-span-2 space-y-1",
            children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("label", {
              htmlFor: "slug",
              className: "block text-sm font-medium text-gray-700",
              children: ["Slug", /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
                className: "text-red-500 ml-1",
                children: "*"
              })]
            }), /*#__PURE__*/(0, _jsxruntime.jsx)("input", {
              id: "slug",
              type: "text",
              value: formSlug,
              onChange: e => {
                /* istanbul ignore next */
                cov_3cv8u3bcv().f[17]++;
                cov_3cv8u3bcv().s[95]++;
                return handleSlugChange(e.target.value);
              },
              placeholder: "our-story",
              required: true,
              className: `w-full px-3 py-2 border rounded-lg transition-colors focus:outline-none focus:ring-2 focus:ring-green-500 ${errors.slug ?
              /* istanbul ignore next */
              (cov_3cv8u3bcv().b[34][0]++, 'border-red-500') :
              /* istanbul ignore next */
