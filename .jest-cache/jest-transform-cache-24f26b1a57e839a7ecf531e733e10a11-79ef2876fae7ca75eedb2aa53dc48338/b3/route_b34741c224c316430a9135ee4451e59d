      /* istanbul ignore next */
      cov_1vyka5abym().b[1][0]++;
      cov_1vyka5abym().s[13]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'UNAUTHORIZED',
          message: 'Authentication required'
        }
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_1vyka5abym().b[1][1]++;
    }
    // 2. VALIDATION
    const body =
    /* istanbul ignore next */
    (cov_1vyka5abym().s[14]++, await request.json());
    const validation =
    /* istanbul ignore next */
    (cov_1vyka5abym().s[15]++, bulkUpdateAuthMethodSchema.safeParse(body));
    /* istanbul ignore next */
    cov_1vyka5abym().s[16]++;
    if (!validation.success) {
      /* istanbul ignore next */
      cov_1vyka5abym().b[3][0]++;
      cov_1vyka5abym().s[17]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'VALIDATION_ERROR',
          message: 'Invalid request data',
          details: validation.error.issues
        }
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_1vyka5abym().b[3][1]++;
    }
    const {
      guest_ids,
      auth_method,
      send_notification
    } =
    /* istanbul ignore next */
    (cov_1vyka5abym().s[18]++, validation.data);
    // 3. SERVICE CALL
    // Update all guests in a single query
    const {
      data: updatedGuests,
      error: updateError
    } =
    /* istanbul ignore next */
    (cov_1vyka5abym().s[19]++, await supabase.from('guests').update({
      auth_method
    }).in('id', guest_ids).select('id, email, first_name, last_name, auth_method'));
    /* istanbul ignore next */
    cov_1vyka5abym().s[20]++;
    if (updateError) {
      /* istanbul ignore next */
      cov_1vyka5abym().b[4][0]++;
      cov_1vyka5abym().s[21]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'DATABASE_ERROR',
          message: 'Failed to update guests',
          details: updateError
        }
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_1vyka5abym().b[4][1]++;
    }
    // TODO: Send notification emails if requested (Task 8.3)
    // This will be implemented when email system is enhanced
    cov_1vyka5abym().s[22]++;
    if (
    /* istanbul ignore next */
    (cov_1vyka5abym().b[6][0]++, send_notification) &&
    /* istanbul ignore next */
    (cov_1vyka5abym().b[6][1]++, updatedGuests) &&
    /* istanbul ignore next */
    (cov_1vyka5abym().b[6][2]++, updatedGuests.length > 0)) {
      /* istanbul ignore next */
      cov_1vyka5abym().b[5][0]++;
      cov_1vyka5abym().s[23]++;
      // Future: Send email notifications to guests about auth method change
      console.log(`Would send notifications to ${updatedGuests.length} guests about auth method change`);
    } else
    /* istanbul ignore next */
    {
      cov_1vyka5abym().b[5][1]++;
    }
    // 4. RESPONSE
    cov_1vyka5abym().s[24]++;
    return _server.NextResponse.json({
      success: true,
      data: {
        updated_count:
        /* istanbul ignore next */
        (cov_1vyka5abym().b[7][0]++, updatedGuests?.length) ||
        /* istanbul ignore next */
        (cov_1vyka5abym().b[7][1]++, 0),
        guests:
        /* istanbul ignore next */
        (cov_1vyka5abym().b[8][0]++, updatedGuests) ||
        /* istanbul ignore next */
        (cov_1vyka5abym().b[8][1]++, [])
      }
    }, {
      status: 200
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_1vyka5abym().s[25]++;
    console.error('API Error:', {
      path: request.url,
      method: request.method,
      error
    });
    /* istanbul ignore next */
    cov_1vyka5abym().s[26]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: 'INTERNAL_ERROR',
        message: 'An unexpected error occurred'
      }
    }, {
      status: 500
    });
  }
}
