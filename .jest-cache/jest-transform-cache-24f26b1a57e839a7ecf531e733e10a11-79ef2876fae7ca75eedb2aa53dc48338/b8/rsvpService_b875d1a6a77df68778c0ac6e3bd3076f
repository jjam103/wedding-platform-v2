async function update(id, data) {
  /* istanbul ignore next */
  cov_13yc4y9no0().f[15]++;
  cov_13yc4y9no0().s[44]++;
  try {
    // 1. Validate
    const validation =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[45]++, _rsvpSchemas.updateRSVPSchema.safeParse(data));
    /* istanbul ignore next */
    cov_13yc4y9no0().s[46]++;
