    // 2. Sanitize user input
    const sanitized =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[48]++, {
      ...validation.data,
      dietary_notes: validation.data.dietary_notes ?
      /* istanbul ignore next */
