    } else
    /* istanbul ignore next */
    {
      cov_13yc4y9no0().b[26][1]++;
    }
    cov_13yc4y9no0().s[78]++;
    if (status) {
      /* istanbul ignore next */
      cov_13yc4y9no0().b[27][0]++;
      cov_13yc4y9no0().s[79]++;
      query = query.eq('status', status);
    } else
    /* istanbul ignore next */
    {
      cov_13yc4y9no0().b[27][1]++;
    }
    // Apply pagination
    const from =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[80]++, (page - 1) * page_size);
    const to =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[81]++, from + page_size - 1);
    /* istanbul ignore next */
    cov_13yc4y9no0().s[82]++;
    query = query.range(from, to);
    // Order by created_at descending
    /* istanbul ignore next */
    cov_13yc4y9no0().s[83]++;
    query = query.order('created_at', {
      ascending: false
    });
    const {
      data,
      error,
      count
    } =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[84]++, await query);
    /* istanbul ignore next */
    cov_13yc4y9no0().s[85]++;
    if (error) {
      /* istanbul ignore next */
      cov_13yc4y9no0().b[28][0]++;
      cov_13yc4y9no0().s[86]++;
      return {
        success: false,
        error: {
          code: _types.ERROR_CODES.DATABASE_ERROR,
          message: error.message,
          details: error
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_13yc4y9no0().b[28][1]++;
    }
    cov_13yc4y9no0().s[87]++;
    return {
      success: true,
      data: {
        rsvps:
        /* istanbul ignore next */
        (cov_13yc4y9no0().b[29][0]++, data) ||
        /* istanbul ignore next */
        (cov_13yc4y9no0().b[29][1]++, []),
        total:
        /* istanbul ignore next */
        (cov_13yc4y9no0().b[30][0]++, count) ||
        /* istanbul ignore next */
        (cov_13yc4y9no0().b[30][1]++, 0)
