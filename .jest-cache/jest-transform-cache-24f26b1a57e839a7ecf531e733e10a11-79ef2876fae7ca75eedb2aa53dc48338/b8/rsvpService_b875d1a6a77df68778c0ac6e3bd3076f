async function calculateActivityCapacity(activityId) {
  /* istanbul ignore next */
  cov_13yc4y9no0().f[21]++;
  cov_13yc4y9no0().s[101]++;
  try {
    // Get activity capacity
    const {
      data: activity,
      error: activityError
    } =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[102]++, await supabase.from('activities').select('capacity, name').eq('id', activityId).single());
    /* istanbul ignore next */
    cov_13yc4y9no0().s[103]++;
    if (activityError) {
      /* istanbul ignore next */
      cov_13yc4y9no0().b[35][0]++;
      cov_13yc4y9no0().s[104]++;
      return {
        success: false,
        error: {
          code: _types.ERROR_CODES.DATABASE_ERROR,
          message: activityError.message,
          details: activityError
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_13yc4y9no0().b[35][1]++;
    }
    // Count attending RSVPs
    const {
      data: rsvps,
      error: rsvpError
    } =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[105]++, await supabase.from('rsvps').select('guest_count').eq('activity_id', activityId).eq('status', 'attending'));
    /* istanbul ignore next */
    cov_13yc4y9no0().s[106]++;
    if (rsvpError) {
      /* istanbul ignore next */
      cov_13yc4y9no0().b[36][0]++;
      cov_13yc4y9no0().s[107]++;
      return {
        success: false,
        error: {
          code: _types.ERROR_CODES.DATABASE_ERROR,
          message: rsvpError.message,
          details: rsvpError
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_13yc4y9no0().b[36][1]++;
    }
    // Calculate total attending count
    const attending_count =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[108]++, rsvps.reduce((sum, rsvp) => {
      /* istanbul ignore next */
      cov_13yc4y9no0().f[22]++;
      cov_13yc4y9no0().s[109]++;
      return sum + (
      /* istanbul ignore next */
      (cov_13yc4y9no0().b[37][0]++, rsvp.guest_count) ||
      /* istanbul ignore next */
      (cov_13yc4y9no0().b[37][1]++, 1));
    }, 0));
    // Calculate available capacity
    const available =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[110]++, activity.capacity !== null ?
    /* istanbul ignore next */
    (cov_13yc4y9no0().b[38][0]++, activity.capacity - attending_count) :
    /* istanbul ignore next */
    (cov_13yc4y9no0().b[38][1]++, null));
    /* istanbul ignore next */
    cov_13yc4y9no0().s[111]++;
    return {
      success: true,
      data: {
        capacity: activity.capacity,
        attending_count,
        available
      }
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_13yc4y9no0().s[112]++;
    return {
      success: false,
      error: {
        code: _types.ERROR_CODES.UNKNOWN_ERROR,
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_13yc4y9no0().b[39][0]++, error.message) :
        /* istanbul ignore next */
        (cov_13yc4y9no0().b[39][1]++, 'Unknown error')
      }
    };
  }
}
async function generateCapacityAlerts(threshold =
/* istanbul ignore next */
(cov_13yc4y9no0().b[40][0]++, 0.9)) {
  /* istanbul ignore next */
  cov_13yc4y9no0().f[23]++;
  cov_13yc4y9no0().s[113]++;
  try {
    // Get all activities with capacity limits
    const {
      data: activities,
      error: activitiesError
    } =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[114]++, await supabase.from('activities').select('id, name, capacity').not('capacity', 'is', null).eq('status', 'published'));
    /* istanbul ignore next */
    cov_13yc4y9no0().s[115]++;
    if (activitiesError) {
      /* istanbul ignore next */
      cov_13yc4y9no0().b[41][0]++;
      cov_13yc4y9no0().s[116]++;
      return {
        success: false,
        error: {
          code: _types.ERROR_CODES.DATABASE_ERROR,
          message: activitiesError.message,
          details: activitiesError
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_13yc4y9no0().b[41][1]++;
    }
    const alerts =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[117]++, []);
    // Check capacity for each activity
    /* istanbul ignore next */
    cov_13yc4y9no0().s[118]++;
    for (const activity of activities) {
      const capacityResult =
      /* istanbul ignore next */
      (cov_13yc4y9no0().s[119]++, await calculateActivityCapacity(activity.id));
      /* istanbul ignore next */
      cov_13yc4y9no0().s[120]++;
      if (!capacityResult.success) {
        /* istanbul ignore next */
        cov_13yc4y9no0().b[42][0]++;
        cov_13yc4y9no0().s[121]++;
        continue; // Skip activities with errors
      } else
      /* istanbul ignore next */
      {
        cov_13yc4y9no0().b[42][1]++;
      }
      const {
        capacity,
        attending_count
      } =
      /* istanbul ignore next */
      (cov_13yc4y9no0().s[122]++, capacityResult.data);
      /* istanbul ignore next */
      cov_13yc4y9no0().s[123]++;
      if (capacity === null) {
        /* istanbul ignore next */
        cov_13yc4y9no0().b[43][0]++;
        cov_13yc4y9no0().s[124]++;
        continue; // Skip activities without capacity limits
      } else
      /* istanbul ignore next */
      {
        cov_13yc4y9no0().b[43][1]++;
      }
      const utilization =
      /* istanbul ignore next */
      (cov_13yc4y9no0().s[125]++, attending_count / capacity);
      // Generate alert if threshold is met or exceeded
      /* istanbul ignore next */
      cov_13yc4y9no0().s[126]++;
      if (utilization >= threshold) {
        /* istanbul ignore next */
        cov_13yc4y9no0().b[44][0]++;
        let alert_level;
        let message;
        /* istanbul ignore next */
        cov_13yc4y9no0().s[127]++;
        if (attending_count >= capacity) {
          /* istanbul ignore next */
          cov_13yc4y9no0().b[45][0]++;
          cov_13yc4y9no0().s[128]++;
          alert_level = 'full';
          /* istanbul ignore next */
          cov_13yc4y9no0().s[129]++;
          message = `Activity "${activity.name}" is at full capacity (${attending_count}/${capacity})`;
        } else {
          /* istanbul ignore next */
          cov_13yc4y9no0().b[45][1]++;
          cov_13yc4y9no0().s[130]++;
          if (utilization >= 0.95) {
            /* istanbul ignore next */
            cov_13yc4y9no0().b[46][0]++;
            cov_13yc4y9no0().s[131]++;
            alert_level = 'critical';
            /* istanbul ignore next */
            cov_13yc4y9no0().s[132]++;
            message = `Activity "${activity.name}" is critically full (${attending_count}/${capacity}, ${Math.round(utilization * 100)}%)`;
          } else {
            /* istanbul ignore next */
            cov_13yc4y9no0().b[46][1]++;
            cov_13yc4y9no0().s[133]++;
            alert_level = 'warning';
            /* istanbul ignore next */
            cov_13yc4y9no0().s[134]++;
            message = `Activity "${activity.name}" is approaching capacity (${attending_count}/${capacity}, ${Math.round(utilization * 100)}%)`;
          }
        }
        /* istanbul ignore next */
        cov_13yc4y9no0().s[135]++;
        alerts.push({
          activity_id: activity.id,
          activity_name: activity.name,
          capacity,
          attending_count,
          utilization_percentage: Math.round(utilization * 100),
          alert_level,
          message
        });
      } else
      /* istanbul ignore next */
      {
        cov_13yc4y9no0().b[44][1]++;
      }
    }
    /* istanbul ignore next */
    cov_13yc4y9no0().s[136]++;
    return {
      success: true,
      data: alerts
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_13yc4y9no0().s[137]++;
    return {
      success: false,
      error: {
        code: _types.ERROR_CODES.UNKNOWN_ERROR,
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_13yc4y9no0().b[47][0]++, error.message) :
        /* istanbul ignore next */
        (cov_13yc4y9no0().b[47][1]++, 'Unknown error')
      }
    };
  }
}
async function checkCapacityAvailable(activityId, additionalGuests =
/* istanbul ignore next */
(cov_13yc4y9no0().b[48][0]++, 1)) {
  /* istanbul ignore next */
  cov_13yc4y9no0().f[24]++;
  cov_13yc4y9no0().s[138]++;
  try {
    const capacityResult =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[139]++, await calculateActivityCapacity(activityId));
    /* istanbul ignore next */
    cov_13yc4y9no0().s[140]++;
    if (!capacityResult.success) {
      /* istanbul ignore next */
      cov_13yc4y9no0().b[49][0]++;
      cov_13yc4y9no0().s[141]++;
      return capacityResult;
    } else
    /* istanbul ignore next */
    {
      cov_13yc4y9no0().b[49][1]++;
    }
    const {
      capacity,
      attending_count,
      available
    } =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[142]++, capacityResult.data);
    // If no capacity limit, always available
    /* istanbul ignore next */
    cov_13yc4y9no0().s[143]++;
    if (capacity === null) {
      /* istanbul ignore next */
      cov_13yc4y9no0().b[50][0]++;
      cov_13yc4y9no0().s[144]++;
      return {
        success: true,
        data: {
          available: true,
          message: 'No capacity limit set for this activity'
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_13yc4y9no0().b[50][1]++;
    }
    // Check if additional guests would exceed capacity
    cov_13yc4y9no0().s[145]++;
    if (
    /* istanbul ignore next */
    (cov_13yc4y9no0().b[52][0]++, available !== null) &&
    /* istanbul ignore next */
    (cov_13yc4y9no0().b[52][1]++, available >= additionalGuests)) {
      /* istanbul ignore next */
      cov_13yc4y9no0().b[51][0]++;
      cov_13yc4y9no0().s[146]++;
      return {
        success: true,
        data: {
          available: true,
          message: `Capacity available: ${available} spots remaining`
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_13yc4y9no0().b[51][1]++;
    }
    cov_13yc4y9no0().s[147]++;
    return {
      success: true,
      data: {
        available: false,
        message: `Capacity exceeded: ${attending_count}/${capacity} attending, cannot add ${additionalGuests} more guest(s)`
      }
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_13yc4y9no0().s[148]++;
    return {
      success: false,
      error: {
        code: _types.ERROR_CODES.UNKNOWN_ERROR,
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_13yc4y9no0().b[53][0]++, error.message) :
        /* istanbul ignore next */
        (cov_13yc4y9no0().b[53][1]++, 'Unknown error')
      }
    };
  }
}
async function enforceCapacityLimit(activityId, guestCount, existingRsvpId) {
  /* istanbul ignore next */
  cov_13yc4y9no0().f[25]++;
  cov_13yc4y9no0().s[149]++;
  try {
    const capacityResult =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[150]++, await calculateActivityCapacity(activityId));
    /* istanbul ignore next */
    cov_13yc4y9no0().s[151]++;
    if (!capacityResult.success) {
      /* istanbul ignore next */
      cov_13yc4y9no0().b[54][0]++;
      cov_13yc4y9no0().s[152]++;
      return capacityResult;
    } else
    /* istanbul ignore next */
    {
      cov_13yc4y9no0().b[54][1]++;
    }
    const {
      capacity,
      attending_count
    } =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[153]++, capacityResult.data);
    // If no capacity limit, allow
    /* istanbul ignore next */
    cov_13yc4y9no0().s[154]++;
    if (capacity === null) {
      /* istanbul ignore next */
      cov_13yc4y9no0().b[55][0]++;
      cov_13yc4y9no0().s[155]++;
      return {
        success: true,
        data: undefined
      };
    } else
    /* istanbul ignore next */
    {
      cov_13yc4y9no0().b[55][1]++;
    }
    // If updating existing RSVP, get current guest count to subtract
    let currentGuestCount =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[156]++, 0);
    /* istanbul ignore next */
    cov_13yc4y9no0().s[157]++;
    if (existingRsvpId) {
      /* istanbul ignore next */
      cov_13yc4y9no0().b[56][0]++;
      const {
        data: existingRsvp,
        error
      } =
      /* istanbul ignore next */
      (cov_13yc4y9no0().s[158]++, await supabase.from('rsvps').select('guest_count, status').eq('id', existingRsvpId).single());
      /* istanbul ignore next */
      cov_13yc4y9no0().s[159]++;
      if (
      /* istanbul ignore next */
      (cov_13yc4y9no0().b[58][0]++, !error) &&
      /* istanbul ignore next */
      (cov_13yc4y9no0().b[58][1]++, existingRsvp) &&
      /* istanbul ignore next */
      (cov_13yc4y9no0().b[58][2]++, existingRsvp.status === 'attending')) {
        /* istanbul ignore next */
        cov_13yc4y9no0().b[57][0]++;
        cov_13yc4y9no0().s[160]++;
        currentGuestCount =
        /* istanbul ignore next */
        (cov_13yc4y9no0().b[59][0]++, existingRsvp.guest_count) ||
        /* istanbul ignore next */
        (cov_13yc4y9no0().b[59][1]++, 1);
      } else
      /* istanbul ignore next */
      {
        cov_13yc4y9no0().b[57][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_13yc4y9no0().b[56][1]++;
    }
    // Calculate new total
    const newTotal =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[161]++, attending_count - currentGuestCount + guestCount);
    /* istanbul ignore next */
    cov_13yc4y9no0().s[162]++;
    if (newTotal > capacity) {
      /* istanbul ignore next */
      cov_13yc4y9no0().b[60][0]++;
      cov_13yc4y9no0().s[163]++;
      return {
        success: false,
        error: {
          code: _types.ERROR_CODES.CAPACITY_EXCEEDED,
          message: `Activity capacity exceeded: ${newTotal}/${capacity}`,
          details: {
            capacity,
            current_attending: attending_count,
            requested_guests: guestCount,
            available: capacity - attending_count + currentGuestCount
          }
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_13yc4y9no0().b[60][1]++;
    }
    cov_13yc4y9no0().s[164]++;
    return {
      success: true,
      data: undefined
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_13yc4y9no0().s[165]++;
    return {
      success: false,
      error: {
        code: _types.ERROR_CODES.UNKNOWN_ERROR,
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_13yc4y9no0().b[61][0]++, error.message) :
        /* istanbul ignore next */
        (cov_13yc4y9no0().b[61][1]++, 'Unknown error')
      }
    };
  }
}
