async function list(filters =
/* istanbul ignore next */
(cov_13yc4y9no0().b[22][0]++, {
  page: 1,
  page_size: 50
})) {
  /* istanbul ignore next */
  cov_13yc4y9no0().f[17]++;
  cov_13yc4y9no0().s[65]++;
  try {
    // Validate filters
    const filtersWithDefaults =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[66]++, {
      page: 1,
      page_size: 50,
      ...filters
    });
    const validation =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[67]++, _rsvpSchemas.listRSVPsSchema.safeParse(filtersWithDefaults));
    /* istanbul ignore next */
    cov_13yc4y9no0().s[68]++;
    if (!validation.success) {
      /* istanbul ignore next */
      cov_13yc4y9no0().b[23][0]++;
      cov_13yc4y9no0().s[69]++;
      return {
        success: false,
        error: {
          code: _types.ERROR_CODES.VALIDATION_ERROR,
          message: 'Invalid filter parameters',
          details: validation.error.issues
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_13yc4y9no0().b[23][1]++;
    }
    const {
      guest_id,
      event_id,
      activity_id,
      status,
      page,
      page_size
    } =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[70]++, validation.data);
    // Build query
    let query =
    /* istanbul ignore next */
    (cov_13yc4y9no0().s[71]++, supabase.from('rsvps').select('*', {
      count: 'exact'
    }));
    /* istanbul ignore next */
    cov_13yc4y9no0().s[72]++;
    if (guest_id) {
      /* istanbul ignore next */
      cov_13yc4y9no0().b[24][0]++;
      cov_13yc4y9no0().s[73]++;
      query = query.eq('guest_id', guest_id);
    } else
    /* istanbul ignore next */
    {
      cov_13yc4y9no0().b[24][1]++;
    }
    cov_13yc4y9no0().s[74]++;
    if (event_id) {
      /* istanbul ignore next */
      cov_13yc4y9no0().b[25][0]++;
      cov_13yc4y9no0().s[75]++;
      query = query.eq('event_id', event_id);
    } else
    /* istanbul ignore next */
    {
      cov_13yc4y9no0().b[25][1]++;
    }
    cov_13yc4y9no0().s[76]++;
    if (activity_id) {
      /* istanbul ignore next */
      cov_13yc4y9no0().b[26][0]++;
      cov_13yc4y9no0().s[77]++;
