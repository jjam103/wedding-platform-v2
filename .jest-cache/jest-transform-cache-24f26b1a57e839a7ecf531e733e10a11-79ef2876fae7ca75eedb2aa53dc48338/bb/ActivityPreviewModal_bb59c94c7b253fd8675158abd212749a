    /* istanbul ignore next */
    (cov_nxfqtcp0y().b[10][0]++, activity.capacityRemaining) ??
    /* istanbul ignore next */
    (cov_nxfqtcp0y().b[10][1]++, activity.capacity));
    const percentFull =
    /* istanbul ignore next */
    (cov_nxfqtcp0y().s[49]++, (activity.capacity - remaining) / activity.capacity * 100);
    let statusColor =
    /* istanbul ignore next */
    (cov_nxfqtcp0y().s[50]++, 'green');
