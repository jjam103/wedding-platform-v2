    /* istanbul ignore next */
    (cov_nxfqtcp0y().s[51]++, 'Available');
    /* istanbul ignore next */
    cov_nxfqtcp0y().s[52]++;
    if (remaining === 0) {
      /* istanbul ignore next */
      cov_nxfqtcp0y().b[11][0]++;
      cov_nxfqtcp0y().s[53]++;
      statusColor = 'red';
      /* istanbul ignore next */
      cov_nxfqtcp0y().s[54]++;
      statusText = 'Full';
    } else {
      /* istanbul ignore next */
      cov_nxfqtcp0y().b[11][1]++;
      cov_nxfqtcp0y().s[55]++;
      if (percentFull >= 90) {
        /* istanbul ignore next */
        cov_nxfqtcp0y().b[12][0]++;
        cov_nxfqtcp0y().s[56]++;
        statusColor = 'yellow';
        /* istanbul ignore next */
        cov_nxfqtcp0y().s[57]++;
        statusText = 'Almost Full';
      } else
      /* istanbul ignore next */
      {
        cov_nxfqtcp0y().b[12][1]++;
      }
