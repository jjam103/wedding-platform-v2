      /* istanbul ignore next */
      (cov_nxfqtcp0y().b[31][2]++, activity) &&
      /* istanbul ignore next */
      (cov_nxfqtcp0y().b[31][3]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
