    const guestCost =
    /* istanbul ignore next */
    (cov_nxfqtcp0y().s[63]++, (
    /* istanbul ignore next */
    (cov_nxfqtcp0y().b[15][0]++, activity.cost) ??
    /* istanbul ignore next */
