          (cov_nxfqtcp0y().b[30][1]++, rsvpStatus.dietaryRestrictions) &&
          /* istanbul ignore next */
          (cov_nxfqtcp0y().b[30][2]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
