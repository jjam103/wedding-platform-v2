        children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_Button.Button, {
          variant: "secondary",
          onClick: onClose,
          children: "Close"
        }), (
        /* istanbul ignore next */
        (cov_nxfqtcp0y().b[32][0]++, !rsvpStatus) ||
        /* istanbul ignore next */
        (cov_nxfqtcp0y().b[32][1]++, rsvpStatus.status === 'pending')) &&
        /* istanbul ignore next */
        (cov_nxfqtcp0y().b[32][2]++, /*#__PURE__*/(0, _jsxruntime.jsx)(_Button.Button, {
          onClick: handleRSVPNow,
