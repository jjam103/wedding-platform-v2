        /* istanbul ignore next */
        (cov_nxfqtcp0y().s[23]++, await fetch(`/api/guest/rsvps?activityId=${activityId}`));
        /* istanbul ignore next */
        cov_nxfqtcp0y().s[24]++;
        if (rsvpResponse.ok) {
          /* istanbul ignore next */
          cov_nxfqtcp0y().b[3][0]++;
          const rsvpResult =
          /* istanbul ignore next */
          (cov_nxfqtcp0y().s[25]++, await rsvpResponse.json());
          /* istanbul ignore next */
          cov_nxfqtcp0y().s[26]++;
          if (
          /* istanbul ignore next */
          (cov_nxfqtcp0y().b[5][0]++, rsvpResult.success) &&
          /* istanbul ignore next */
          (cov_nxfqtcp0y().b[5][1]++, rsvpResult.data.length > 0)) {
            /* istanbul ignore next */
            cov_nxfqtcp0y().b[4][0]++;
            cov_nxfqtcp0y().s[27]++;
            setRsvpStatus(rsvpResult.data[0]);
          } else
          /* istanbul ignore next */
          {
            cov_nxfqtcp0y().b[4][1]++;
          }
        } else
        /* istanbul ignore next */
        {
          cov_nxfqtcp0y().b[3][1]++;
        }
      } catch (err) {
        /* istanbul ignore next */
        cov_nxfqtcp0y().s[28]++;
        setError(err instanceof Error ?
        /* istanbul ignore next */
        (cov_nxfqtcp0y().b[6][0]++, err.message) :
        /* istanbul ignore next */
        (cov_nxfqtcp0y().b[6][1]++, 'Failed to load activity'));
      } finally {
        /* istanbul ignore next */
        cov_nxfqtcp0y().s[29]++;
        setLoading(false);
      }
    }
    /* istanbul ignore next */
    cov_nxfqtcp0y().s[30]++;
    fetchActivityData();
  }, [activityId]);
  /* istanbul ignore next */
  cov_nxfqtcp0y().s[31]++;
  const handleViewFullDetails = () => {
    /* istanbul ignore next */
    cov_nxfqtcp0y().f[4]++;
    cov_nxfqtcp0y().s[32]++;
    if (activity?.slug) {
      /* istanbul ignore next */
      cov_nxfqtcp0y().b[7][0]++;
      cov_nxfqtcp0y().s[33]++;
      router.push(`/activity/${activity.slug}`);
    } else {
      /* istanbul ignore next */
      cov_nxfqtcp0y().b[7][1]++;
      cov_nxfqtcp0y().s[34]++;
      router.push(`/activity/${activityId}`);
    }
    /* istanbul ignore next */
    cov_nxfqtcp0y().s[35]++;
    onClose();
  };
  /* istanbul ignore next */
  cov_nxfqtcp0y().s[36]++;
  const handleRSVPNow = () => {
    /* istanbul ignore next */
    cov_nxfqtcp0y().f[5]++;
