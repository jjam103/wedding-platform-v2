          /* istanbul ignore next */
          (cov_nxfqtcp0y().b[29][0]++, activity.description) &&
          /* istanbul ignore next */
          (cov_nxfqtcp0y().b[29][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
