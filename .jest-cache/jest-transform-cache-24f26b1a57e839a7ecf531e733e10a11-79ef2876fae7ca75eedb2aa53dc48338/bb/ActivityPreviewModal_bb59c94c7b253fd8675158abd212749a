            /* istanbul ignore next */
            (cov_nxfqtcp0y().b[27][0]++, activity.capacity) &&
            /* istanbul ignore next */
            (cov_nxfqtcp0y().b[27][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
