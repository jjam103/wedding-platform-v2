                onClose: mockOnClose
            }));
            await (0, _react.waitFor)(()=>{
                expect(_react.screen.getByText('#1', {
                    selector: 'span'
                })).toBeInTheDocument();
            });
            const sections = _react.screen.getAllByText(/#\d+/, {
                selector: 'span'
