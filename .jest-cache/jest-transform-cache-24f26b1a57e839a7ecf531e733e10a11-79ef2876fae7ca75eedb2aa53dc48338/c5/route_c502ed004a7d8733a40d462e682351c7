  } catch (error) {
    /* istanbul ignore next */
    cov_2qunpldybh().s[25]++;
    console.error('Bulk delete events error:', error);
    /* istanbul ignore next */
    cov_2qunpldybh().s[26]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: 'INTERNAL_ERROR',
        message: error instanceof Error ?
        /* istanbul ignore next */
        (cov_2qunpldybh().b[6][0]++, error.message) :
        /* istanbul ignore next */
        (cov_2qunpldybh().b[6][1]++, 'Unknown error')
      }
    }, {
      status: 500
    });
  }
}
