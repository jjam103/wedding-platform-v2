        it('should update badge when pendingPhotosCount prop changes', ()=>{
            const { rerender } = (0, _react.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_GroupedNavigation.GroupedNavigation, {
                pendingPhotosCount: 5
            }));
            // Initial badge
            expect(_react.screen.getAllByLabelText('5 pending').length).toBeGreaterThan(0);
            // Update count
            rerender(/*#__PURE__*/ (0, _jsxruntime.jsx)(_GroupedNavigation.GroupedNavigation, {
                pendingPhotosCount: 10
            }));
            // Badge should update
            expect(_react.screen.getAllByLabelText('10 pending').length).toBeGreaterThan(0);
