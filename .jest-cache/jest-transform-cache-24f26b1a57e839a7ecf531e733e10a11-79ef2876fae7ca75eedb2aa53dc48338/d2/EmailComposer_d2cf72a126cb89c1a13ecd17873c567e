      cov_15sgiidj9t().b[11][0]++;
      cov_15sgiidj9t().s[55]++;
      addToast({
        type: 'error',
        message: 'Please enter email body'
      });
      /* istanbul ignore next */
      cov_15sgiidj9t().s[56]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_15sgiidj9t().b[11][1]++;
    }
    cov_15sgiidj9t().s[57]++;
    setSubmitting(true);
    /* istanbul ignore next */
    cov_15sgiidj9t().s[58]++;
    try {
      // Get recipient emails
      let recipientEmails =
      /* istanbul ignore next */
      (cov_15sgiidj9t().s[59]++, []);
      /* istanbul ignore next */
      cov_15sgiidj9t().s[60]++;
      if (recipientType === 'guests') {
        /* istanbul ignore next */
        cov_15sgiidj9t().b[12][0]++;
        cov_15sgiidj9t().s[61]++;
        recipientEmails = guests.filter(g => {
          /* istanbul ignore next */
          cov_15sgiidj9t().f[7]++;
          cov_15sgiidj9t().s[62]++;
          return /* istanbul ignore next */(cov_15sgiidj9t().b[13][0]++, selectedRecipients.includes(g.id)) &&
          /* istanbul ignore next */
          (cov_15sgiidj9t().b[13][1]++, g.email);
        }).map(g => {
          /* istanbul ignore next */
          cov_15sgiidj9t().f[8]++;
          cov_15sgiidj9t().s[63]++;
          return g.email;
        });
      } else {
        /* istanbul ignore next */
        cov_15sgiidj9t().b[12][1]++;
        // For groups, fetch all guests in selected groups
        const groupGuests =
        /* istanbul ignore next */
        (cov_15sgiidj9t().s[64]++, guests.filter(g => {
          /* istanbul ignore next */
          cov_15sgiidj9t().f[9]++;
          cov_15sgiidj9t().s[65]++;
          return selectedRecipients.some(groupId => {
            /* istanbul ignore next */
            cov_15sgiidj9t().f[10]++;
            cov_15sgiidj9t().s[66]++;
            // This is simplified - in reality, we'd need to fetch group members
            return true;
          });
        }));
        /* istanbul ignore next */
        cov_15sgiidj9t().s[67]++;
        recipientEmails = groupGuests.filter(g => {
          /* istanbul ignore next */
          cov_15sgiidj9t().f[11]++;
          cov_15sgiidj9t().s[68]++;
          return g.email;
        }).map(g => {
          /* istanbul ignore next */
          cov_15sgiidj9t().f[12]++;
          cov_15sgiidj9t().s[69]++;
          return g.email;
        });
      }
      /* istanbul ignore next */
      cov_15sgiidj9t().s[70]++;
      if (recipientEmails.length === 0) {
        /* istanbul ignore next */
        cov_15sgiidj9t().b[14][0]++;
        cov_15sgiidj9t().s[71]++;
        addToast({
          type: 'error',
          message: 'No valid email addresses found for selected recipients'
        });
        /* istanbul ignore next */
        cov_15sgiidj9t().s[72]++;
        setSubmitting(false);
        /* istanbul ignore next */
        cov_15sgiidj9t().s[73]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_15sgiidj9t().b[14][1]++;
      }
      // Send email
      const response =
      /* istanbul ignore next */
      (cov_15sgiidj9t().s[74]++, await fetch('/api/admin/emails/send', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json'
        },
        body: JSON.stringify({
          recipients: recipientEmails,
          subject,
          html: body,
          text: body.replace(/<[^>]*>/g, ''),
          template_id:
          /* istanbul ignore next */
          (cov_15sgiidj9t().b[15][0]++, selectedTemplate) ||
          /* istanbul ignore next */
          (cov_15sgiidj9t().b[15][1]++, undefined)
        })
      }));
      const result =
      /* istanbul ignore next */
      (cov_15sgiidj9t().s[75]++, await response.json());
      /* istanbul ignore next */
      cov_15sgiidj9t().s[76]++;
      if (result.success) {
        /* istanbul ignore next */
        cov_15sgiidj9t().b[16][0]++;
        cov_15sgiidj9t().s[77]++;
        addToast({
          type: 'success',
          message: `Email sent to ${recipientEmails.length} recipient(s)`
        });
        /* istanbul ignore next */
        cov_15sgiidj9t().s[78]++;
        onSuccess();
        /* istanbul ignore next */
        cov_15sgiidj9t().s[79]++;
        onClose();
      } else {
        /* istanbul ignore next */
        cov_15sgiidj9t().b[16][1]++;
        cov_15sgiidj9t().s[80]++;
        addToast({
          type: 'error',
          message:
          /* istanbul ignore next */
          (cov_15sgiidj9t().b[17][0]++, result.error?.message) ||
          /* istanbul ignore next */
          (cov_15sgiidj9t().b[17][1]++, 'Failed to send email')
        });
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_15sgiidj9t().s[81]++;
      addToast({
        type: 'error',
        message: 'Failed to send email'
      });
    } finally {
      /* istanbul ignore next */
      cov_15sgiidj9t().s[82]++;
      setSubmitting(false);
    }
  };
  /* istanbul ignore next */
  cov_15sgiidj9t().s[83]++;
  if (!isOpen) {
    /* istanbul ignore next */
    cov_15sgiidj9t().b[18][0]++;
    cov_15sgiidj9t().s[84]++;
    return null;
  } else
  /* istanbul ignore next */
  {
    cov_15sgiidj9t().b[18][1]++;
  }
  cov_15sgiidj9t().s[85]++;
  return /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
    className: "fixed inset-0 bg-black bg-opacity-50 z-50 flex items-center justify-center p-4",
    children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
      className: "bg-white rounded-xl shadow-2xl max-w-4xl w-full max-h-[90vh] overflow-y-auto",
      children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "px-6 py-4 border-b border-sage-200 flex items-center justify-between sticky top-0 bg-white",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("h2", {
          className: "text-2xl font-bold text-sage-900",
          children: "Compose Email"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("button", {
          onClick: onClose,
          className: "text-sage-400 hover:text-sage-600 transition-colors",
          "aria-label": "Close",
          children: /*#__PURE__*/(0, _jsxruntime.jsx)("svg", {
            className: "w-6 h-6",
            fill: "none",
            stroke: "currentColor",
            viewBox: "0 0 24 24",
            children: /*#__PURE__*/(0, _jsxruntime.jsx)("path", {
              strokeLinecap: "round",
              strokeLinejoin: "round",
              strokeWidth: 2,
              d: "M6 18L18 6M6 6l12 12"
            })
          })
        })]
      }), /*#__PURE__*/(0, _jsxruntime.jsx)("form", {
        onSubmit: handleSubmit,
        className: "px-6 py-4",
        children: loading ?
        /* istanbul ignore next */
        (/*#__PURE__*/cov_15sgiidj9t().b[19][0]++, (0, _jsxruntime.jsxs)("div", {
          className: "text-center py-8",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
            className: "inline-block animate-spin rounded-full h-8 w-8 border-b-2 border-jungle-500"
          }), /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
            className: "mt-2 text-sage-600",
            children: "Loading..."
          })]
        })) :
        /* istanbul ignore next */
        (/*#__PURE__*/cov_15sgiidj9t().b[19][1]++, (0, _jsxruntime.jsxs)(_jsxruntime.Fragment, {
          children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "mb-4",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)("label", {
              htmlFor: "template",
              className: "block text-sm font-medium text-sage-700 mb-1",
              children: "Email Template (Optional)"
            }), /*#__PURE__*/(0, _jsxruntime.jsxs)("select", {
              id: "template",
              value: selectedTemplate,
              onChange: e => {
                /* istanbul ignore next */
                cov_15sgiidj9t().f[13]++;
                cov_15sgiidj9t().s[86]++;
                return handleTemplateChange(e.target.value);
              },
              className: "w-full px-3 py-2 border border-sage-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-jungle-500",
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)("option", {
                value: "",
                children: "-- No Template --"
              }), templates.map(template => {
                /* istanbul ignore next */
                cov_15sgiidj9t().f[14]++;
                cov_15sgiidj9t().s[87]++;
                return /*#__PURE__*/(0, _jsxruntime.jsx)("option", {
                  value: template.id,
                  children: template.name
                }, template.id);
              })]
            })]
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "mb-4",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)("label", {
              className: "block text-sm font-medium text-sage-700 mb-1",
              children: "Recipient Type"
            }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
              className: "flex gap-4",
              children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("label", {
                className: "flex items-center",
                children: [/*#__PURE__*/(0, _jsxruntime.jsx)("input", {
                  type: "radio",
                  value: "guests",
                  checked: recipientType === 'guests',
                  onChange: e => {
                    /* istanbul ignore next */
                    cov_15sgiidj9t().f[15]++;
                    cov_15sgiidj9t().s[88]++;
                    setRecipientType(e.target.value);
                    /* istanbul ignore next */
