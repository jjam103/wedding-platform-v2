    }
    // 4. Verify page is published
    cov_1sbo2cfry5().s[19]++;
    if (result.data.status !== 'published') {
      /* istanbul ignore next */
      cov_1sbo2cfry5().b[6][0]++;
      cov_1sbo2cfry5().s[20]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'NOT_FOUND',
          message: 'Content page not found'
        }
      }, {
        status: 404
      });
    } else
    /* istanbul ignore next */
    {
      cov_1sbo2cfry5().b[6][1]++;
    }
    // 5. Get sections and columns for the page
    const {
      data: sections,
      error: sectionsError
    } =
    /* istanbul ignore next */
    (cov_1sbo2cfry5().s[21]++, await supabase.from('sections').select(`
        *,
        columns (*)
      `).eq('page_type', 'custom').eq('page_id', result.data.id).order('display_order', {
      ascending: true
    }));
    /* istanbul ignore next */
    cov_1sbo2cfry5().s[22]++;
    if (sectionsError) {
      /* istanbul ignore next */
      cov_1sbo2cfry5().b[7][0]++;
      cov_1sbo2cfry5().s[23]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'DATABASE_ERROR',
          message: sectionsError.message
        }
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_1sbo2cfry5().b[7][1]++;
    }
    // 6. Return response with sections
    cov_1sbo2cfry5().s[24]++;
    return _server.NextResponse.json({
      success: true,
      data: {
        ...result.data,
        sections:
        /* istanbul ignore next */
        (cov_1sbo2cfry5().b[8][0]++, sections) ||
        /* istanbul ignore next */
        (cov_1sbo2cfry5().b[8][1]++, [])
      }
    }, {
      status: 200
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_1sbo2cfry5().s[25]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: 'INTERNAL_ERROR',
        message: 'An unexpected error occurred'
      }
    }, {
      status: 500
    });
  }
}
