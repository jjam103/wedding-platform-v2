function DefaultErrorFallback({ error, onReset }) {
    // Determine if this is a network error
    const isNetworkError = error.message.includes('fetch') || error.message.includes('network') || error.message.includes('Failed to load') || error.message.includes('timeout');
    // Get user-friendly error message
    const getUserFriendlyMessage = (error)=>{
        if (isNetworkError) {
            return 'Unable to connect to the server. Please check your internet connection and try again.';
        }
        if (error.message.includes('permission') || error.message.includes('unauthorized')) {
            return 'You do not have permission to access this resource.';
        }
        if (error.message.includes('not found')) {
            return 'The requested resource could not be found.';
        }
        return 'An unexpected error occurred. Please try again or contact support if the problem persists.';
    };
    return /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
        className: "min-h-[400px] flex items-center justify-center p-6",
        children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
            className: "max-w-md w-full bg-white rounded-lg shadow-lg border border-volcano-200 p-8 text-center",
            children: [
                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                    className: "mb-6",
                    children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_TropicalIcon.TropicalIcon, {
                        name: "volcano",
                        size: "xl",
                        className: "text-volcano-500 mx-auto"
                    })
                }),
                /*#__PURE__*/ (0, _jsxruntime.jsx)("h2", {
                    className: "text-2xl font-bold text-sage-900 mb-3",
                    children: "Oops! Something went wrong"
                }),
                /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                    className: "text-sage-600 mb-6",
                    children: getUserFriendlyMessage(error)
                }),
                /*#__PURE__*/ (0, _jsxruntime.jsxs)("details", {
                    className: "mb-6 text-left",
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("summary", {
                            className: "cursor-pointer text-sm text-sage-500 hover:text-sage-700 mb-2",
                            children: "Technical details"
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                            className: "bg-sage-50 rounded-lg p-4 text-xs font-mono text-sage-700 overflow-auto max-h-32",
                            children: error.message
                        })
                    ]
                }),
                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
