            });
            // Check aria-invalid is set
            const titleInput = _react.screen.getByLabelText(/title/i);
            expect(titleInput).toHaveAttribute('aria-invalid', 'true');
        });
        it('should have proper help text for slug field', ()=>{
            (0, _react.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_ContentPageForm.ContentPageForm, {
                title: "Add Content Page",
                onSubmit: mockOnSubmit,
                isOpen: true,
                onToggle: mockOnToggle
            }));
            expect(_react.screen.getByText('URL-safe identifier (lowercase, hyphens only). Auto-generated from title.')).toBeInTheDocument();
        });
    });
});

