      }),
      /* istanbul ignore next */
      (cov_22iue8ux2g().b[38][0]++, errors.capacity) &&
      /* istanbul ignore next */
      (cov_22iue8ux2g().b[38][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
        className: "mt-2 text-sm text-red-600",
        children: errors.capacity
      }))]
    }),
    /* istanbul ignore next */
    (cov_22iue8ux2g().b[39][0]++, formData.status === 'attending') &&
    /* istanbul ignore next */
