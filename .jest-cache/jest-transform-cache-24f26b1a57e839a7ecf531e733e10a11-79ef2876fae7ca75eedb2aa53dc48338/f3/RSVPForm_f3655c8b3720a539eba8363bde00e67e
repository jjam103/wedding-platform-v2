      /* istanbul ignore next */
      {
        cov_22iue8ux2g().b[14][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_22iue8ux2g().b[10][1]++;
    }
    // Zod validation
    const validation =
    /* istanbul ignore next */
    (cov_22iue8ux2g().s[32]++, rsvpFormSchema.safeParse(formData));
    /* istanbul ignore next */
    cov_22iue8ux2g().s[33]++;
    if (!validation.success) {
      /* istanbul ignore next */
      cov_22iue8ux2g().b[18][0]++;
      cov_22iue8ux2g().s[34]++;
      validation.error.issues.forEach(issue => {
        /* istanbul ignore next */
        cov_22iue8ux2g().f[3]++;
        cov_22iue8ux2g().s[35]++;
        if (issue.path[0]) {
          /* istanbul ignore next */
          cov_22iue8ux2g().b[19][0]++;
          cov_22iue8ux2g().s[36]++;
          newErrors[issue.path[0].toString()] = issue.message;
        } else
        /* istanbul ignore next */
        {
          cov_22iue8ux2g().b[19][1]++;
        }
      });
    } else
    /* istanbul ignore next */
    {
      cov_22iue8ux2g().b[18][1]++;
    }
    cov_22iue8ux2g().s[37]++;
    setErrors(newErrors);
    /* istanbul ignore next */
    cov_22iue8ux2g().s[38]++;
    return Object.keys(newErrors).length === 0;
  };
  // Handle form submission
  /* istanbul ignore next */
  cov_22iue8ux2g().s[39]++;
  const handleSubmit = async e => {
    /* istanbul ignore next */
    cov_22iue8ux2g().f[4]++;
    cov_22iue8ux2g().s[40]++;
    e.preventDefault();
    /* istanbul ignore next */
    cov_22iue8ux2g().s[41]++;
    setSubmitError(null);
    /* istanbul ignore next */
    cov_22iue8ux2g().s[42]++;
    if (!validateForm()) {
      /* istanbul ignore next */
      cov_22iue8ux2g().b[20][0]++;
      cov_22iue8ux2g().s[43]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_22iue8ux2g().b[20][1]++;
    }
    cov_22iue8ux2g().s[44]++;
    setLoading(true);
    /* istanbul ignore next */
    cov_22iue8ux2g().s[45]++;
    try {
      // Sanitize input
      const sanitizedData =
      /* istanbul ignore next */
      (cov_22iue8ux2g().s[46]++, {
        ...formData,
        dietaryRestrictions: formData.dietaryRestrictions ?
        /* istanbul ignore next */
        (cov_22iue8ux2g().b[21][0]++, (0, _sanitization.sanitizeInput)(formData.dietaryRestrictions)) :
        /* istanbul ignore next */
        (cov_22iue8ux2g().b[21][1]++, undefined)
      });
      // Determine API endpoint
      const url =
      /* istanbul ignore next */
      (cov_22iue8ux2g().s[47]++, currentRsvp ?
      /* istanbul ignore next */
      (cov_22iue8ux2g().b[22][0]++, `/api/guest/rsvps/${currentRsvp.id}`) :
      /* istanbul ignore next */
      (cov_22iue8ux2g().b[22][1]++, `/api/guest/rsvps`));
      const method =
      /* istanbul ignore next */
      (cov_22iue8ux2g().s[48]++, currentRsvp ?
      /* istanbul ignore next */
      (cov_22iue8ux2g().b[23][0]++, 'PUT') :
      /* istanbul ignore next */
      (cov_22iue8ux2g().b[23][1]++, 'POST'));
      const body =
      /* istanbul ignore next */
      (cov_22iue8ux2g().s[49]++, currentRsvp ?
      /* istanbul ignore next */
      (cov_22iue8ux2g().b[24][0]++, sanitizedData) :
      /* istanbul ignore next */
      (cov_22iue8ux2g().b[24][1]++, {
        ...sanitizedData,
        activityId
      }));
      const response =
      /* istanbul ignore next */
      (cov_22iue8ux2g().s[50]++, await fetch(url, {
        method,
        headers: {
          'Content-Type': 'application/json'
        },
        body: JSON.stringify(body)
      }));
      const result =
      /* istanbul ignore next */
      (cov_22iue8ux2g().s[51]++, await response.json());
      /* istanbul ignore next */
      cov_22iue8ux2g().s[52]++;
      if (!result.success) {
        /* istanbul ignore next */
        cov_22iue8ux2g().b[25][0]++;
        cov_22iue8ux2g().s[53]++;
        throw new Error(
        /* istanbul ignore next */
        (cov_22iue8ux2g().b[26][0]++, result.error?.message) ||
        /* istanbul ignore next */
        (cov_22iue8ux2g().b[26][1]++, 'Failed to submit RSVP'));
      } else
      /* istanbul ignore next */
      {
        cov_22iue8ux2g().b[25][1]++;
      }
      // Show success message (could use toast here)
      cov_22iue8ux2g().s[54]++;
      onSuccess();
    } catch (error) {
      /* istanbul ignore next */
      cov_22iue8ux2g().s[55]++;
      setSubmitError(error instanceof Error ?
      /* istanbul ignore next */
      (cov_22iue8ux2g().b[27][0]++, error.message) :
      /* istanbul ignore next */
      (cov_22iue8ux2g().b[27][1]++, 'An error occurred while submitting your RSVP'));
    } finally {
      /* istanbul ignore next */
      cov_22iue8ux2g().s[56]++;
      setLoading(false);
    }
  };
  // Handle status change
  /* istanbul ignore next */
  cov_22iue8ux2g().s[57]++;
  const handleStatusChange = status => {
    /* istanbul ignore next */
    cov_22iue8ux2g().f[5]++;
    cov_22iue8ux2g().s[58]++;
    setFormData({
      ...formData,
      status
    });
    /* istanbul ignore next */
    cov_22iue8ux2g().s[59]++;
    setErrors({});
  };
  // Handle guest count change
  /* istanbul ignore next */
  cov_22iue8ux2g().s[60]++;
  const handleGuestCountChange = count => {
    /* istanbul ignore next */
