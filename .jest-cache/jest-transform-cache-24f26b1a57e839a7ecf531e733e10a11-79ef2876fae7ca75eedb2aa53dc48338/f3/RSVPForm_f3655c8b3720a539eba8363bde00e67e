            /* istanbul ignore next */
            cov_22iue8ux2g().f[11]++;
            cov_22iue8ux2g().s[71]++;
            return handleStatusChange('declined');
          },
          disabled: isDeadlinePassed,
          className: `px-4 py-3 rounded-lg border-2 font-medium transition-colors ${formData.status === 'declined' ?
          /* istanbul ignore next */
          (cov_22iue8ux2g().b[36][0]++, 'border-red-500 bg-red-50 text-red-700') :
          /* istanbul ignore next */
          (cov_22iue8ux2g().b[36][1]++, 'border-gray-300 bg-white text-gray-700 hover:border-red-300')} ${isDeadlinePassed ?
