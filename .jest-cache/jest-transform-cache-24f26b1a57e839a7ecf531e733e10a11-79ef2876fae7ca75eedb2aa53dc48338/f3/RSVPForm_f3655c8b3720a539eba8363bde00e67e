      ...formData,
      plusOne: checked
    });
  };
  /* istanbul ignore next */
  cov_22iue8ux2g().s[68]++;
  return /*#__PURE__*/(0, _jsxruntime.jsxs)("form", {
    onSubmit: handleSubmit,
    className: "space-y-6",
    children: [
    /* istanbul ignore next */
    (cov_22iue8ux2g().b[28][0]++, isDeadlinePassed) &&
    /* istanbul ignore next */
    (cov_22iue8ux2g().b[28][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "bg-red-50 border border-red-200 rounded-lg p-4",
      children: /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
        className: "text-sm text-red-800 font-medium",
        children: " The RSVP deadline for this activity has passed. You can no longer submit or update your RSVP."
      })
    })),
    /* istanbul ignore next */
    (cov_22iue8ux2g().b[29][0]++, !isDeadlinePassed) &&
    /* istanbul ignore next */
    (cov_22iue8ux2g().b[29][1]++, isCapacityAlmostFull) &&
    /* istanbul ignore next */
    (cov_22iue8ux2g().b[29][2]++, !isCapacityFull) &&
    /* istanbul ignore next */
    (cov_22iue8ux2g().b[29][3]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "bg-yellow-50 border border-yellow-200 rounded-lg p-4",
      children: /*#__PURE__*/(0, _jsxruntime.jsxs)("p", {
        className: "text-sm text-yellow-800 font-medium",
        children: [" This activity is almost full! Only ", activityDetails?.capacityRemaining, " spot(s) remaining."]
      })
    })), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
      children: [/*#__PURE__*/(0, _jsxruntime.jsx)("label", {
        className: "block text-sm font-medium text-gray-700 mb-2",
        children: "RSVP Status *"
      }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "grid grid-cols-3 gap-3",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("button", {
          type: "button",
          onClick: () => {
            /* istanbul ignore next */
            cov_22iue8ux2g().f[9]++;
            cov_22iue8ux2g().s[69]++;
            return handleStatusChange('attending');
          },
          disabled:
          /* istanbul ignore next */
          (cov_22iue8ux2g().b[30][0]++, isDeadlinePassed) ||
          /* istanbul ignore next */
          (cov_22iue8ux2g().b[30][1]++, isCapacityFull) &&
          /* istanbul ignore next */
          (cov_22iue8ux2g().b[30][2]++, formData.status !== 'attending'),
          className: `px-4 py-3 rounded-lg border-2 font-medium transition-colors ${formData.status === 'attending' ?
          /* istanbul ignore next */
          (cov_22iue8ux2g().b[31][0]++, 'border-green-500 bg-green-50 text-green-700') :
          /* istanbul ignore next */
          (cov_22iue8ux2g().b[31][1]++, 'border-gray-300 bg-white text-gray-700 hover:border-green-300')} ${
          /* istanbul ignore next */
          (cov_22iue8ux2g().b[33][0]++, isDeadlinePassed) ||
