  /* istanbul ignore next */
  (cov_1hqfg21hkf().b[7][1]++, cache.has(obj))) {
    /* istanbul ignore next */
    cov_1hqfg21hkf().b[6][0]++;
    cov_1hqfg21hkf().s[21]++;
    return cache.get(obj);
  } else
  /* istanbul ignore next */
  {
    cov_1hqfg21hkf().b[6][1]++;
  }
  var newObj =
  /* istanbul ignore next */
  (cov_1hqfg21hkf().s[22]++, {
    __proto__: null
  });
  var hasPropertyDescriptor =
  /* istanbul ignore next */
  (cov_1hqfg21hkf().s[23]++,
  /* istanbul ignore next */
  (cov_1hqfg21hkf().b[8][0]++, Object.defineProperty) &&
  /* istanbul ignore next */
  (cov_1hqfg21hkf().b[8][1]++, Object.getOwnPropertyDescriptor));
  /* istanbul ignore next */
  cov_1hqfg21hkf().s[24]++;
  for (var key in obj) {
    /* istanbul ignore next */
    cov_1hqfg21hkf().s[25]++;
    if (
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[10][0]++, key !== "default") &&
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[10][1]++, Object.prototype.hasOwnProperty.call(obj, key))) {
      /* istanbul ignore next */
      cov_1hqfg21hkf().b[9][0]++;
      var desc =
      /* istanbul ignore next */
      (cov_1hqfg21hkf().s[26]++, hasPropertyDescriptor ?
      /* istanbul ignore next */
      (cov_1hqfg21hkf().b[11][0]++, Object.getOwnPropertyDescriptor(obj, key)) :
      /* istanbul ignore next */
      (cov_1hqfg21hkf().b[11][1]++, null));
      /* istanbul ignore next */
      cov_1hqfg21hkf().s[27]++;
      if (
      /* istanbul ignore next */
      (cov_1hqfg21hkf().b[13][0]++, desc) && (
      /* istanbul ignore next */
      (cov_1hqfg21hkf().b[13][1]++, desc.get) ||
      /* istanbul ignore next */
      (cov_1hqfg21hkf().b[13][2]++, desc.set))) {
        /* istanbul ignore next */
        cov_1hqfg21hkf().b[12][0]++;
        cov_1hqfg21hkf().s[28]++;
        Object.defineProperty(newObj, key, desc);
      } else {
        /* istanbul ignore next */
        cov_1hqfg21hkf().b[12][1]++;
        cov_1hqfg21hkf().s[29]++;
        newObj[key] = obj[key];
      }
    } else
    /* istanbul ignore next */
    {
      cov_1hqfg21hkf().b[9][1]++;
    }
  }
  /* istanbul ignore next */
  cov_1hqfg21hkf().s[30]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_1hqfg21hkf().s[31]++;
  if (cache) {
    /* istanbul ignore next */
    cov_1hqfg21hkf().b[14][0]++;
    cov_1hqfg21hkf().s[32]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_1hqfg21hkf().b[14][1]++;
  }
  cov_1hqfg21hkf().s[33]++;
  return newObj;
}
async function GET(request, {
  params
}) {
  /* istanbul ignore next */
  cov_1hqfg21hkf().f[4]++;
  cov_1hqfg21hkf().s[34]++;
  try {
    // Await params for Next.js 15+
    const {
      slug
    } =
    /* istanbul ignore next */
    (cov_1hqfg21hkf().s[35]++, await params);
    // 1. AUTHENTICATION
    const supabase =
    /* istanbul ignore next */
    (cov_1hqfg21hkf().s[36]++, (0, _authhelpersnextjs.createRouteHandlerClient)({
      cookies: _headers.cookies
    }));
    const {
      data: {
        session
      },
      error: authError
    } =
    /* istanbul ignore next */
    (cov_1hqfg21hkf().s[37]++, await supabase.auth.getSession());
    /* istanbul ignore next */
    cov_1hqfg21hkf().s[38]++;
    if (
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[16][0]++, authError) ||
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[16][1]++, !session)) {
      /* istanbul ignore next */
      cov_1hqfg21hkf().b[15][0]++;
      cov_1hqfg21hkf().s[39]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'UNAUTHORIZED',
          message: 'Authentication required'
        }
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_1hqfg21hkf().b[15][1]++;
    }
    // Get guest ID from session
    const {
      data: guest,
      error: guestError
    } =
    /* istanbul ignore next */
    (cov_1hqfg21hkf().s[40]++, await supabase.from('guests').select('id, group_id').eq('email', session.user.email).single());
    /* istanbul ignore next */
    cov_1hqfg21hkf().s[41]++;
    if (
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[18][0]++, guestError) ||
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[18][1]++, !guest)) {
      /* istanbul ignore next */
      cov_1hqfg21hkf().b[17][0]++;
      cov_1hqfg21hkf().s[42]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'NOT_FOUND',
          message: 'Guest not found'
        }
      }, {
        status: 404
      });
    } else
    /* istanbul ignore next */
    {
      cov_1hqfg21hkf().b[17][1]++;
    }
    // 2. VALIDATION
    const slugValidation =
    /* istanbul ignore next */
    (cov_1hqfg21hkf().s[43]++, _zod.z.string().min(1).safeParse(slug));
    /* istanbul ignore next */
    cov_1hqfg21hkf().s[44]++;
    if (!slugValidation.success) {
      /* istanbul ignore next */
      cov_1hqfg21hkf().b[19][0]++;
      cov_1hqfg21hkf().s[45]++;
      return _server.NextResponse.json({
        success: false,
        error: {
          code: 'VALIDATION_ERROR',
          message: 'Invalid slug'
        }
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_1hqfg21hkf().b[19][1]++;
    }
    // 3. SERVICE CALL - Get activity by slug
    const activityResult =
    /* istanbul ignore next */
    (cov_1hqfg21hkf().s[46]++, await _activityService.getBySlug(slugValidation.data));
    /* istanbul ignore next */
    cov_1hqfg21hkf().s[47]++;
    if (!activityResult.success) {
      /* istanbul ignore next */
      cov_1hqfg21hkf().b[20][0]++;
      cov_1hqfg21hkf().s[48]++;
      return _server.NextResponse.json(activityResult, {
        status: 404
      });
    } else
    /* istanbul ignore next */
    {
      cov_1hqfg21hkf().b[20][1]++;
    }
    // Get RSVP status for this activity
    const rsvpResult =
    /* istanbul ignore next */
    (cov_1hqfg21hkf().s[49]++, await _rsvpService.list({
      guest_id: guest.id,
      activity_id: activityResult.data.id,
      page: 1,
      page_size: 1
    }));
    const rsvpStatus =
    /* istanbul ignore next */
    (cov_1hqfg21hkf().s[50]++,
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[22][0]++, rsvpResult.success) &&
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[22][1]++, rsvpResult.data.rsvps.length > 0) ?
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[21][0]++, rsvpResult.data.rsvps[0].status) :
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[21][1]++, 'pending'));
    // Get current attendee count from RSVPs
    const {
      data: attendingRsvps
    } =
    /* istanbul ignore next */
    (cov_1hqfg21hkf().s[51]++, await supabase.from('rsvps').select('guest_count').eq('activity_id', activityResult.data.id).eq('status', 'attending'));
    const currentAttendees =
    /* istanbul ignore next */
    (cov_1hqfg21hkf().s[52]++, attendingRsvps ?
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[23][0]++, attendingRsvps.reduce((sum, rsvp) => {
      /* istanbul ignore next */
      cov_1hqfg21hkf().f[5]++;
      cov_1hqfg21hkf().s[53]++;
      return sum + (
      /* istanbul ignore next */
      (cov_1hqfg21hkf().b[24][0]++, rsvp.guest_count) ||
      /* istanbul ignore next */
      (cov_1hqfg21hkf().b[24][1]++, 1));
    }, 0)) :
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[23][1]++, 0));
    // Calculate capacity remaining
    const capacityRemaining =
    /* istanbul ignore next */
    (cov_1hqfg21hkf().s[54]++, activityResult.data.capacity ?
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[25][0]++, activityResult.data.capacity - currentAttendees) :
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[25][1]++, null));
    // Calculate net cost (cost per person - host subsidy)
    const netCost =
    /* istanbul ignore next */
    (cov_1hqfg21hkf().s[55]++, activityResult.data.costPerPerson ?
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[26][0]++, Math.max(0, activityResult.data.costPerPerson - (
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[27][0]++, activityResult.data.hostSubsidy) ||
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[27][1]++, 0)))) :
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[26][1]++, 0));
    // Calculate capacity percentage
    const capacityPercentage =
    /* istanbul ignore next */
    (cov_1hqfg21hkf().s[56]++, activityResult.data.capacity ?
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[28][0]++, currentAttendees / activityResult.data.capacity * 100) :
    /* istanbul ignore next */
    (cov_1hqfg21hkf().b[28][1]++, 0));
    // 4. RESPONSE
    /* istanbul ignore next */
    cov_1hqfg21hkf().s[57]++;
    return _server.NextResponse.json({
      success: true,
      data: {
        ...activityResult.data,
        rsvpStatus,
        capacityRemaining,
        capacityPercentage,
        netCost,
        isFull:
        /* istanbul ignore next */
        (cov_1hqfg21hkf().b[29][0]++, capacityRemaining !== null) &&
        /* istanbul ignore next */
        (cov_1hqfg21hkf().b[29][1]++, capacityRemaining <= 0),
        isAlmostFull: capacityPercentage >= 90
      }
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_1hqfg21hkf().s[58]++;
    console.error('API Error:', {
      path: request.url,
      error
    });
    /* istanbul ignore next */
    cov_1hqfg21hkf().s[59]++;
    return _server.NextResponse.json({
      success: false,
      error: {
        code: 'INTERNAL_ERROR',
        message: 'An unexpected error occurred'
      }
    }, {
      status: 500
    });
  }
}
